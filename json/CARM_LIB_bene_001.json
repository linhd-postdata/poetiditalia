{
    "author": "carmina libraria",
    "title": "Alexander Benedictus, de observatione in pestilentia",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|bene|001",
    "text": [
        [
            {
                "verse": "Quinti Haemyliani Cimcriaci Poetae Hendecasyllabicon in V LI. Alexandri Paeantii Ad Lectorem"
            }
        ],
        [
            {
                "verse": "Qui non volt stygiis sata sub umbris"
            },
            {
                "verse": "Saeva pestilitate disperire,"
            },
            {
                "verse": "Nec ferrugineam videre puppim"
            },
            {
                "verse": "Nec vitae pretium seni trientem"
            },
            {
                "verse": "Olli porrigere ore subtrementi;"
            },
            {
                "verse": "Sed vivo esse potens sui vigore"
            },
            {
                "verse": "Et servare animam diu valentem,"
            },
            {
                "verse": "Haec hortor legat aure non supina"
            },
            {
                "verse": "Quae paentius exaravit Artis"
            },
            {
                "verse": "Ille assertor appolinatis ille"
            },
            {
                "verse": "Ingens castalidum decus sororum"
            }
        ]
    ]
}