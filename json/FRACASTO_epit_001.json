{
    "author": "Girolamo Fracastoro",
    "title": "epitaphius",
    "url": "http://www.poetiditalia.it/texts/FRACASTO|epit|001",
    "text": [
        [
            {
                "verse": "Quod Molsae fuerat mortale hac conditur urna"
            },
            {
                "verse": "Extruxere suis quam Aonides manibus."
            },
            {
                "verse": "Coetibus at Superum fruitur nitidissimus almis,"
            },
            {
                "verse": "Itque comes magno spiritus Hippolyto,"
            },
            {
                "verse": "Quem Medica de gente satum pulcherrima virtus"
            },
            {
                "verse": "Extulit, et coeli templa tenere dedit;"
            },
            {
                "verse": "Quemque unum ante omnes coluit, dum fata sinebant,"
            },
            {
                "verse": "Atque oculis vates praetulit ipse suis."
            }
        ]
    ]
}