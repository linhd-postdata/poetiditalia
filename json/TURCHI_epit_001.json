{
    "author": "Turchi Tommaso",
    "title": "epitaphium",
    "url": "http://www.poetiditalia.it/texts/TURCHI|epit|001",
    "text": [
        [
            {
                "verse": "Inclita magnanimae fuerat qui gloria gentis"
            },
            {
                "verse": "Estensis iacet hic Nicolaus marchio cuius"
            },
            {
                "verse": "Imperio celebri Ferraria pace quievit."
            },
            {
                "verse": "Iustitia quis erat vel Marte potentior illo?"
            },
            {
                "verse": "Saeva per Italiam potuit compescere bella,"
            },
            {
                "verse": "Armatas frenare acies, frenare furentes"
            },
            {
                "verse": "Consiliis populos et foedera iungere regnis."
            },
            {
                "verse": "Hac victor sibi parte viam affectavit Olimpo."
            }
        ],
        []
    ]
}