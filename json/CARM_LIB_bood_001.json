{
    "author": "carmina libraria",
    "title": "Anselmus Boetius de Boodt, gemmarum et lapidum historia",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|bood|001",
    "text": [
        [
            {
                "verse": "in historiam gemmarum et lapidum Anselmi Boetii..editam nova cura Adriani Toll, medicinae doctoris"
            }
        ],
        [
            {
                "verse": "Erecte Tolli, cuius indoles gestit"
            },
            {
                "verse": "Debere saeclo et otio fatiscentes"
            },
            {
                "verse": "Incusat horas semifeminatorum,"
            },
            {
                "verse": "Per eruditas ambulare doctorum"
            },
            {
                "verse": "Sueta curas, extricare perplexa"
            },
            {
                "verse": "Purgare verum sordibus vetustatis"
            },
            {
                "verse": "\u00ce\u0093\u00ce\u00b1\u00ce\u00bb\u00ce\u00b7\u00ce\u00bd\u00ce\u00b9\u00ce\u00ba\u00cf\u0089\u00cf\u0084\u00ce\u00b5\u00cf\u0081\u00cf\u0089\u00cf\u0084\u00ce\u00ac\u00cf\u0084\u00cf\u0089\u00ce\u00bd decus grande"
            },
            {
                "verse": "Recentiorum lima tersa scriptorum."
            },
            {
                "verse": "Ut nunc Eoos Indicosque splendores"
            },
            {
                "verse": "Inter refulges, et tuos Smaragdorum"
            },
            {
                "verse": "Radiis honores Sardioque diffundis"
            },
            {
                "verse": "Ditemque Hydaspen gemmeumque derives"
            },
            {
                "verse": "In nostra Gangen regna. Non tibi sputa"
            },
            {
                "verse": "Et inquinata fercla ventris affecti"
            },
            {
                "verse": "Molesta sunt nunc, pituita vel putris"
            },
            {
                "verse": "Aut ore manans turpe fetido tabum"
            },
            {
                "verse": "Aut prave olentis vultus ater urinae."
            },
            {
                "verse": "Quin transmarinis  elegantiis totus"
            },
            {
                "verse": "Nites decore et fulgetris lapillorum"
            },
            {
                "verse": "Gemmisque luces undique et coruscantem"
            },
            {
                "verse": "Beryllon ardes succinumque lychnique"
            },
            {
                "verse": "Onychesque nigras ipse quas iacit flammas"
            },
            {
                "verse": "Pyropus in te torquet et virescentes"
            },
            {
                "verse": "Notas Iaspis  laetior tibi pandit."
            },
            {
                "verse": "Carbunculusque et pulcher ora Sapphirus"
            },
            {
                "verse": "Non usitatis ignibus magis flagrant."
            },
            {
                "verse": "Pretiosa scribis, ut Cylindrus, ut Baccae"
            },
            {
                "verse": "Undis latenter obstrepant Erythraeis"
            },
            {
                "verse": "Adamasque duras nil moretur incudes;"
            },
            {
                "verse": "Quam multiformis lividas averruncet"
            },
            {
                "verse": "Pestes Achates, languidamque Torcois"
            },
            {
                "verse": "Venerem lacessat; vulsaque e mari magno"
            },
            {
                "verse": "Coralla largas fluminum vias sistant;"
            },
            {
                "verse": "Et tu feracis munus inclytum Cypri"
            },
            {
                "verse": "Caerulea Cyanos, seu Pharum magis laudas"
            },
            {
                "verse": "Scythicasque rupes Caucasisque praerupta"
            },
            {
                "verse": "Per te fugari disce torva nocturni"
            },
            {
                "verse": "Portenta Morphei  et incubum gravem lectis."
            },
            {
                "verse": "Cum ventr matris abditum latet pondus"
            },
            {
                "verse": "Pullus medetur et sonorus Aetites."
            },
            {
                "verse": "Aquasque Magnes allicit putrescentes"
            },
            {
                "verse": "Electra mentem , margarita languentes"
            },
            {
                "verse": "Vires redonat; quique friget insomni"
            },
            {
                "verse": "Hyacinthus aegro somnolentiam suadet."
            },
            {
                "verse": "Quis splendor artis, quam superba maiestas"
            },
            {
                "verse": "Medici theatri est, Aesculapidum Patres"
            },
            {
                "verse": "Queis apparatus Caelites parant tantos,"
            },
            {
                "verse": "Famulantur astra, format unda quod sanet."
            },
            {
                "verse": "Aer medellam sufficit, vomunt fontes,"
            },
            {
                "verse": "Montes ministrant queis salubre quoducumque est."
            },
            {
                "verse": "Arbusta stillant, saxa robur indulgent;"
            },
            {
                "verse": "Gemmas, lapillos, ferra, vitra, tot petras"
            },
            {
                "verse": "Hic Porphyriten, hic dat Alabastriten,"
            },
            {
                "verse": "Ilic Pyriten, hic bitumen et gypsum"
            },
            {
                "verse": "Aurum, cerussam , nitra sulphur argentum"
            },
            {
                "verse": "Opesque cunctas Mater alma largitur."
            },
            {
                "verse": "Centum iuvatur, laeditur modis centum"
            },
            {
                "verse": "Humana Vita. Totus orbis obiectat"
            },
            {
                "verse": "Et damna et usus, commodusque viventi"
            },
            {
                "verse": "Incommodusque est. Causa mortis atque idem"
            },
            {
                "verse": "Salutis autor, quails hasta Pelidae."
            },
            {
                "verse": "Ante ora morbi fomes et comes iuxta"
            },
            {
                "verse": "Medicina praesto est. Quicquid obvium tangis"
            },
            {
                "verse": "Sanum flagellat, allevat laborantem."
            },
            {
                "verse": "Corpus vacillat saepius remorbescens"
            },
            {
                "verse": "Cadaverosis debitum sepulcretis,"
            },
            {
                "verse": "Rursum resurgit et valemus athletae,"
            },
            {
                "verse": "Vicesque servant Sanitasque Morbusque"
            },
            {
                "verse": "Et nunc amica, nunc iniqua  Natura."
            },
            {
                "verse": "Quae cuncta dum nos impiger doces Tolli"
            },
            {
                "verse": "Aeternitatis praemio supervive."
            },
            {
                "verse": "Quod si nepotes nomen oscitabundi"
            },
            {
                "verse": "Tum silenti nubibus tenebrabunt,"
            },
            {
                "verse": "Lapides loquentur, et Boetii famam"
            },
            {
                "verse": "Tuasque laudes gemmulae recognoscent."
            },
            {
                "verse": "Caspar Barlaeus"
            }
        ],
        []
    ]
}