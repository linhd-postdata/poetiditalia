{
    "author": "Niccol\u00f2\u00b2 Perotti",
    "title": "ad Caesarem",
    "url": "http://www.poetiditalia.it/texts/PEROTTI|caes|001",
    "text": [
        [
            {
                "verse": "Cinxisti viridi, Caesar, mea tempora lauro:"
            },
            {
                "verse": "Ecce meas ornat sacra corona comas;"
            },
            {
                "verse": "Non mea me virtus tali nunc munere dignum,"
            },
            {
                "verse": "Dulce sed exhibuit Principis ingenium."
            },
            {
                "verse": "Dique deaeque precor, pro me, mitissime Caesar,"
            },
            {
                "verse": "Persolvant meritis praemia digna tuis;"
            },
            {
                "verse": "Ast ego quo tali tibi sim pro munere gratus"
            },
            {
                "verse": "Extollam laudes semper in astra tuas."
            }
        ]
    ]
}