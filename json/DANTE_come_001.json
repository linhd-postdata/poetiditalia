    "text": [
        [
            {
                "verse": "Ultima regna canam, fluvido contermina mundo,"
            },
            {
                "verse": "Spiritibus que lata patent, que premia solvunt"
            },
            {
                "verse": "Pro meritis cuicunque suis."
            }
        ]
    ]
