{
    "author": "Francesco Maria Molza",
    "title": "epigramma",
    "url": "http://www.poetiditalia.it/texts/MOLZA_F|epig|001",
    "text": [
        [
            {
                "verse": "In visum ferro Laurens dum percutit hostem,"
            },
            {
                "verse": "Quod premeret patriae libera colla suae;"
            },
            {
                "verse": "Te ne hic nunc, inquit, patiar, qui ferre tyrannos"
            },
            {
                "verse": "Vix olim Romae marmoreos potui?"
            }
        ]
    ]
}