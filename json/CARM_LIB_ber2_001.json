{
    "author": "carmina libraria",
    "title": "Philippus Beroaldus, de felicitate opusculum",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|ber2|001",
    "text": [
        [
            {
                "verse": "Felix cui costant bona corporis et bona mentis"
            },
            {
                "verse": "Nec non fortunae munera parca deae."
            }
        ],
        [],
        [
            {
                "verse": "endecasyllabon ad illustrissimum Iohannem Bentivolium"
            }
        ],
        [
            {
                "verse": "Felix o nimis et beate princeps"
            },
            {
                "verse": "Cuius praesidio est beata felix"
            },
            {
                "verse": "Pollens Felsina. Nos sumus quieti."
            },
            {
                "verse": "Hoc pro te superos libens precabor:"
            },
            {
                "verse": "Ut donent tibi Nestoris senectam"
            },
            {
                "verse": "Augusti decus et vicem Metelli"
            },
            {
                "verse": "Sit natis quoque faustitas parentis."
            },
            {
                "verse": "Pro me hoc debeo te rogare princeps"
            },
            {
                "verse": "Ut molli foveas pium clientem"
            },
            {
                "verse": "Afflatu niveae beatitatis."
            }
        ],
        [],
        [
            {
                "verse": "eiusdem endecasyllabon ad Germaniam"
            }
        ],
        [
            {
                "verse": "O Germania muneris repertrix"
            },
            {
                "verse": "Quo nil utilius dedit vetustas:"
            },
            {
                "verse": "Libros scribere quae doces premendo."
            },
            {
                "verse": "Felix frugibus inclyta et metallis"
            },
            {
                "verse": "Et dives pecoris feraxque gemmae"
            },
            {
                "verse": "Quod glessum vocitant tui parentes,"
            },
            {
                "verse": "Electrum Attica, Succinum latini."
            },
            {
                "verse": "Pollens principibus potens virorum."
            },
            {
                "verse": "Qui glaucis oculis Comaque flava,"
            },
            {
                "verse": "Grandes corpore, spiritu feroces"
            },
            {
                "verse": "Septi robore prodigique vitae"
            },
            {
                "verse": "Hostes aggrediuntur et lacessunt."
            },
            {
                "verse": "O Germania gloriosa salve"
            },
            {
                "verse": "Tu vasa ex oricalcho et apparatum"
            },
            {
                "verse": "Mensarum nitidum aere machinasque"
            },
            {
                "verse": "Ad nos belligeras subinde mittis."
            },
            {
                "verse": "Abs te turba venit scohlasticorum"
            },
            {
                "verse": "Hoc qui gymnasium excolunt opimant"
            },
            {
                "verse": "Iam doctum et latialiter sonantes"
            },
            {
                "verse": "Tris nuper quoque regulos badenses"
            },
            {
                "verse": "Lautos magnificos probos dedisti."
            },
            {
                "verse": "Inter quos Iacobus enitescit"
            },
            {
                "verse": "Lingua, dexteritate, comitate,"
            },
            {
                "verse": "Cultu, mundiciis, nitore, victu."
            },
            {
                "verse": "Clarus, munificus, potens, disertus"
            },
            {
                "verse": "Splendorque et columen scholasticorum."
            },
            {
                "verse": "At Germania praepotens alumnos"
            },
            {
                "verse": "Ad nos consimilem subinde mitte."
            }
        ]
    ]
}