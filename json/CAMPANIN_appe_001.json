{
    "author": "Campanino",
    "title": "appendix",
    "url": "http://www.poetiditalia.it/texts/CAMPANIN|appe|001",
    "text": [
        [
            {
                "verse": "Quid longae mihi lucubrationes,"
            },
            {
                "verse": "Quid prodest studii labor tenacis,"
            },
            {
                "verse": "Si tales chelys exitus datura"
            },
            {
                "verse": "Tormenta et pretium futura plectri?"
            },
            {
                "verse": "A quanto melius ligone terram"
            },
            {
                "verse": "Versare et domitos boves fuisset curvo,"
            },
            {
                "verse": "Platina, subdidisse aratro!"
            }
        ],
        [
            {
                "verse": "E cavea infelix dilectos specto sodales,"
            },
            {
                "verse": "Quos vidi media liber in urbe prius,"
            },
            {
                "verse": "Nec datur e cavea minimam dimittere vocem,"
            },
            {
                "verse": "Quod tibi non ullus, garrula pica, negat."
            },
            {
                "verse": "Cum sit sors eadem nobis caveaque premamur,"
            },
            {
                "verse": "Cur mihi non eadem quae tibi lingua datur?"
            }
        ],
        [
            {
                "verse": "Quod ventus mihi fluctuet solutus"
            },
            {
                "verse": "Effundens strepitu graves querelas,"
            },
            {
                "verse": "Augustine, dari putas salutem"
            },
            {
                "verse": "Campano tenebris situque merso."
            },
            {
                "verse": "Non salus mihi corporis rogatur,"
            },
            {
                "verse": "Sed mens indiget adiuvante dextra:"
            },
            {
                "verse": "Haec nullis Arabum potest medellis"
            },
            {
                "verse": "Sanari aut crepitu tonante ventris,"
            },
            {
                "verse": "Sed solo mihi numinis favore."
            }
        ],
        [
            {
                "verse": "Esses thesauri domus reposti:"
            },
            {
                "verse": "Fecit composita manu architectus,"
            },
            {
                "verse": "Sed nullo tamen innitescis auro."
            },
            {
                "verse": "Ah quam dura lues, scelus nefasque!"
            },
            {
                "verse": "Nunc squales lacrimis, tonas querelis"
            },
            {
                "verse": "Carcer fatiferi situs et horror."
            }
        ],
        [
            {
                "verse": "Dic quis te comitem dedit tenebris,"
            },
            {
                "verse": "Aut quis participem Stygis paludis"
            },
            {
                "verse": "Fecit lumina ..."
            }
        ],
        [
            {
                "verse": "Lucili studium nocet necatque"
            },
            {
                "verse": "Et dat carceribus suos alundos:"
            },
            {
                "verse": "Quam duro melius solum bidente"
            },
            {
                "verse": "Versaremus, ut omnis interiret"
            },
            {
                "verse": "Vanarum ardor amorque litterarum."
            }
        ],
        [
            {
                "verse": "Ioannes, Venetae decus Camenae,"
            },
            {
                "verse": "Quondam, dum licuit, mihi fidelis,"
            },
            {
                "verse": "Sic te candida, sic beata quondam"
            },
            {
                "verse": "Integrum omnibus augeat senectus"
            },
            {
                "verse": "Et sis in Veneto bonus senatu!"
            },
            {
                "verse": "Si nostri superest, ut esse credo,"
            },
            {
                "verse": "Cura et Pieridum novem minister"
            },
            {
                "verse": "Servas quos tibi fudimus liquores,"
            },
            {
                "verse": "Si nobis retines fidem priorem,"
            },
            {
                "verse": "Huc mittas celerem brevemque chartam,"
            },
            {
                "verse": "Quae me tam recreabit una..."
            },
            {
                "verse": "Quam gaudet posito labore festus"
            },
            {
                "verse": "Colonus, glaciem Canemve dirum"
            },
            {
                "verse": "Qui sensit miser acriore caelo."
            },
            {
                "verse": "Nam donec nivibus tegentur Alpes,"
            },
            {
                "verse": "Dum Libys tumidos dabit leones,"
            },
            {
                "verse": "Dum septem iuga Romuli manebunt,"
            },
            {
                "verse": "Dum fulmen Iovis audient Epiri,"
            },
            {
                "verse": "Hoc te pectore continebo semper."
            },
            {
                "verse": "Sed dum altae nivibus tegentur Alpes"
            },
            {
                "verse": "Dum Libys tumidos dabit leones,"
            },
            {
                "verse": "Dum septem iuga Romuli manebunt,"
            },
            {
                "verse": "Dum fulmen Iovis audient Molossi,"
            },
            {
                "verse": "Tu hoc sub pectore vixeris perennis."
            }
        ],
        [
            {
                "verse": "Fortunae, gladiis trucique fato,"
            },
            {
                "verse": "Pomponi, rigida resiste mente,"
            },
            {
                "verse": "Nec te sors mala nubilumque tempus"
            },
            {
                "verse": "Oblitum faciat severitatis."
            },
            {
                "verse": "Virtus difficili labore crescit,"
            },
            {
                "verse": "Ornat plurima militem cicatrix"
            },
            {
                "verse": "Et robur bona Reguli vetustas"
            },
            {
                "verse": "Laudavit; Mario fames sitisque"
            },
            {
                "verse": "Et canna toleratus in palustri"
            },
            {
                "verse": "Somnus perpetuum dedere nomen."
            },
            {
                "verse": "Sic nobis quoque carcer et cruentis"
            },
            {
                "verse": "(Saevum!) funiculis retorta membra"
            },
            {
                "verse": "Et quicquid patimur laboriosum,"
            },
            {
                "verse": "Cum deus nitido favebit ore,"
            },
            {
                "verse": "Fiet maxima protinus voluptas."
            }
        ],
        [
            {
                "verse": "Fulginas, patriae nitor Camenae,"
            },
            {
                "verse": "Quondam, dum licuit, mihi sodalis,"
            },
            {
                "verse": "Si nostri superest, ut esse credo,"
            },
            {
                "verse": "Cura, si veteres ioci, lepores"
            },
            {
                "verse": "Nondum deciderint, amicus atque"
            },
            {
                "verse": "In tantis remanes mihi procellis,"
            },
            {
                "verse": "Ne, quaeso, a solita sodalitate"
            },
            {
                "verse": "Mentem dimoveas, perennis uno"
            },
            {
                "verse": "Sed semper maneas tenore fidus."
            },
            {
                "verse": "Non semper glaciem Canemve dirum"
            },
            {
                "verse": "Colonus patitur, quiescit atque"
            },
            {
                "verse": "Interdum posito labore festus,"
            },
            {
                "verse": "Nec me perpetuis acerba curis"
            },
            {
                "verse": "Fortuna afficiet cadetque livor:"
            },
            {
                "verse": "Qui si perstiterit malusque nolit"
            },
            {
                "verse": "Vivum linquere, mortuum relinquet."
            }
        ],
        [
            {
                "verse": "Quondam, dum licuit, frequentiores"
            },
            {
                "verse": "Mittebam tibi, Contarine, nugas,"
            },
            {
                "verse": "Nugas rusticolae rudisque Musae:"
            },
            {
                "verse": "Me nemo e Venetis petiit lacunis,"
            },
            {
                "verse": "Cui non continuo darem Camenas"
            },
            {
                "verse": "Ad te vel Numidam tuum ferendas."
            },
            {
                "verse": "At nunc carcere sordido sepultus"
            },
            {
                "verse": "Si sum dissimilis mei silendo,"
            },
            {
                "verse": "Da, quaeso, veniam pio timori:"
            },
            {
                "verse": "Nam non deposui sodalitatem"
            },
            {
                "verse": "Intra viscera fida sempiternam,"
            },
            {
                "verse": "Heu, sed me vetat acris ira fati"
            },
            {
                "verse": "Et versutia temporis maligni."
            }
        ],
        [
            {
                "verse": "Dum celsae nivibus tegentur Alpes,"
            },
            {
                "verse": "Dum Libys tumidos dabit leones,"
            },
            {
                "verse": "Aegyptus segetes, rates Hiberus,"
            },
            {
                "verse": "Durabit pietas, fides amorque,"
            },
            {
                "verse": "Contarine, tuus: loquentur omnes"
            },
            {
                "verse": "Gentes quod Numidae tuo dedisti,"
            },
            {
                "verse": "Nec ulla Alvisium tacebit aetas."
            },
            {
                "verse": "Fac perstes modo nobilemque mentem"
            },
            {
                "verse": "Firma perpetui nitore amoris:"
            },
            {
                "verse": "Nam vivit Numida et truces sagittas"
            },
            {
                "verse": "Fregit Tartareae malignitatis."
            },
            {
                "verse": "Vix credes, puto, cum repente vatem"
            },
            {
                "verse": "Laetus in Veneta videbis urbe,"
            },
            {
                "verse": "Ni Byzantiacum sequatur axem."
            }
        ],
        [
            {
                "verse": "Nostri gloria fructus iste caeli,"
            },
            {
                "verse": "Campani agricolae perenne munus,"
            },
            {
                "verse": "Usum temporibus Neronianis."
            },
            {
                "verse": "Tanta non fuerat prius voluptas,"
            },
            {
                "verse": "Nec unquam melepeponas Camillus"
            },
            {
                "verse": "Gustavit, Curius gravisve Cursor,"
            },
            {
                "verse": "Pompeius neque Crassus aut Lucullus."
            },
            {
                "verse": "Ergo quanta mero fuit reperto"
            },
            {
                "verse": "Baccho gloria, tanta sit colonis,"
            },
            {
                "verse": "Qui primum ambrosios dedere succos."
            },
            {
                "verse": "Quod misi calamos, ineptus a te"
            },
            {
                "verse": "Fortassis merito queam vocari,"
            },
            {
                "verse": "Cum longe aerisono solo Curetum"
            },
            {
                "verse": "Distent Hernica iura, sed rubentes"
            },
            {
                "verse": "Quod mitto melepeponas repertos"
            },
            {
                "verse": "Campani ingenio prius coloni,"
            },
            {
                "verse": "Nil tam conveniens dedisse dicar."
            }
        ],
        [],
        [],
        []
    ]
}