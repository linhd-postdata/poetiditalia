{
    "author": "carmina libraria",
    "title": "Leo Baptista Alberti, de re aedificatoria",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|alb2|001",
    "text": [
        [
            {
                "verse": "Antonius Sabinus in laudem authoris"
            }
        ],
        [
            {
                "verse": "Qui longum Leo conditus latebat"
            },
            {
                "verse": "Tandem prosilit, emicat, vagatur"
            },
            {
                "verse": "Et grato patet ore sublevatus."
            },
            {
                "verse": "Non vinclis retinetur aut cathenis"
            },
            {
                "verse": "Diris. Ecce venit, videte cuncti"
            },
            {
                "verse": "Ut non horridus et timendus ulli,"
            },
            {
                "verse": "Sed mitis, placidus, pius benignus,"
            },
            {
                "verse": "Non mordet rapit ungulisve quemque."
            },
            {
                "verse": "Sed tangi cupit hinc et inde laetus"
            },
            {
                "verse": "Gestit, annuit, advocat tenentes."
            },
            {
                "verse": "Qualem coelicolae dedere mundo"
            },
            {
                "verse": "Cuius aurea pellis est notata"
            },
            {
                "verse": "Stellis et radiantibus lapillis."
            },
            {
                "verse": "Talem cuncta Hieronymus per ora"
            },
            {
                "verse": "Produxit, nitida manu polivit."
            },
            {
                "verse": "Cuius nectar ab ore, mella, nardus"
            },
            {
                "verse": "Ceu magni fluit amnis unda verna."
            },
            {
                "verse": "Et si quis maculis minus nitebat"
            },
            {
                "verse": "Longo carcere quo diu latebat"
            },
            {
                "verse": "Abstersit solita vigente lima"
            },
            {
                "verse": "Doctis omnibus ut placere possit."
            },
            {
                "verse": "Et dicant simul hic coli meretur."
            },
            {
                "verse": "Quare si quis erat prius retentus"
            },
            {
                "verse": "Fumoso cane sive equo feroci,"
            },
            {
                "verse": "Isthaec destituat, velit Leonem"
            },
            {
                "verse": "Qui maiore tenebitur lepore"
            },
            {
                "verse": "Qui cum gaudia mille consequetur"
            }
        ]
    ]
}