{
    "author": "carmina libraria",
    "title": "Joachim Camerarius, astrologica",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|came|001",
    "text": [
        [
            {
                "verse": "Heobanus Hessus"
            }
        ],
        [
            {
                "verse": "Si memoratur Atlas caelum sudasse ferendo,"
            },
            {
                "verse": "Si tulit Herculeus sidera fessa labor;"
            },
            {
                "verse": "Si flammas rapuit Iovis alta ex arce Prometheus,"
            },
            {
                "verse": "Si Lunam superis abstulit Endymion;"
            },
            {
                "verse": "Te quoque posteritas credit, Ioachime, fuisse"
            },
            {
                "verse": "Caelestes solitum scandere saepe domos."
            },
            {
                "verse": "Qui tamquam astriferos sis nuper ab aethere lapsus"
            },
            {
                "verse": "Astra, gradus, orbes, lumina, signa doces,"
            },
            {
                "verse": "Cum tam pulchra novae potuisti tradere luci"
            },
            {
                "verse": "Scripta palam nullo tempore visa prius."
            }
        ]
    ]
}