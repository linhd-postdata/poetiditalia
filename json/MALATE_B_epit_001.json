{
    "author": "Boccaccio Malatesta",
    "title": "epitaphium",
    "url": "http://www.poetiditalia.it/texts/MALATE_B|epit|001",
    "text": [
        [
            {
                "verse": "Hic situs Estensis summae pietatis alumnus"
            },
            {
                "verse": "Astreae cultor pacis amator inest."
            },
            {
                "verse": "Marchio sic felix facili dicione regebat,"
            },
            {
                "verse": "Marchio munificus hic Nicolaus erat."
            },
            {
                "verse": "Est hic illustris, sapiens, clarissimus heros"
            },
            {
                "verse": "Ingeniive decus. Oppida cuncta gemunt."
            },
            {
                "verse": "Una salus regni patrii cum fratribus ipsis"
            },
            {
                "verse": "Magnanimis princeps tu, Leonelle, gemis."
            },
            {
                "verse": "Corpus quod robur quondam tollebat in astra"
            },
            {
                "verse": "Hic iacet, ast animam sidera celsa tenent."
            },
            {
                "verse": "Mille quadringentis domini quater octo sub annis"
            },
            {
                "verse": "Atque novem estense sidus in arce nitet."
            }
        ],
        [],
        [],
        []
    ]
}