{
    "author": "Benvenuto Campesani",
    "title": "epigramma",
    "url": "http://www.poetiditalia.it/texts/CAMPESAN|epig|001",
    "text": [
        [
            {
                "verse": "de resurrectione Catulli poete Veronensis"
            }
        ],
        [
            {
                "verse": "Ad patriam venio longis a finibus exsul;"
            },
            {
                "verse": "Causa mei reditus compatriota fuit,"
            },
            {
                "verse": "Scilicet a calamis tribuit cui Francia nomen"
            },
            {
                "verse": "Quique notat turbe praetereuntis iter."
            },
            {
                "verse": "Quo licet ingenio vestrum celebrate Catullum,"
            },
            {
                "verse": "Cuius sub modio clausa papirus erat."
            }
        ]
    ]
}