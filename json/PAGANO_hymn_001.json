{
    "author": "Pagano della Torre",
    "title": "hymnus",
    "url": "http://www.poetiditalia.it/texts/PAGANO|hymn|001",
    "text": [
        [
            {
                "verse": "himpnus domini episcopi Paduani dicolos tetrastrophos,nam primi tres versus sunt asclepiadei et quartus gliconius,sicut ille hympnus \"sanctorum meritis inclita gaudia\""
            }
        ],
        [
            {
                "verse": "Magni pontificis, qui Patavas lavat"
            },
            {
                "verse": "Mentes interius pectora fultiens,"
            },
            {
                "verse": "Tutis consiliis magnificentiam"
            },
            {
                "verse": "Exaltet pater omnium,"
            },
            {
                "verse": "Qui virtute micat, qui sapientia,"
            },
            {
                "verse": "Qui magna procerum progenie satus"
            },
            {
                "verse": "De Turri, Senecam qui redolet sacris"
            },
            {
                "verse": "Vite moribus inclitum."
            },
            {
                "verse": "Ierarchia triplex celica muniat"
            },
            {
                "verse": "Invictumque regat spiritualia"
            },
            {
                "verse": "Certantem Domini prelia; Belzebut"
            },
            {
                "verse": "Devictus fugiat procul."
            },
            {
                "verse": "De vatis pluteo centifidem chelim"
            },
            {
                "verse": "Miscentem sapidis Thespiadum tonis"
            },
            {
                "verse": "Sumpsi dulce melos; prosula sed tamen"
            },
            {
                "verse": "Me poscit replicamina."
            }
        ]
    ]
}