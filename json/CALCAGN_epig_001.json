{
    "author": "Celio Calcagnini",
    "title": "epigramma",
    "url": "http://www.poetiditalia.it/texts/CALCAGN|epig|001",
    "text": [
        [
            {
                "verse": "Exi lucidus undecumque Luci"
            },
            {
                "verse": "Tersus, frugifer, elegans, rotundus"
            },
            {
                "verse": "Nec graues metuas subisse rhonchos"
            },
            {
                "verse": "Adeptus modo pristinum decorem,"
            },
            {
                "verse": "Quem doctus Beroaldus et disertus"
            },
            {
                "verse": "Lima restituit seueriore."
            },
            {
                "verse": "Nec solum tibi reddidit priorem"
            },
            {
                "verse": "Cultum: at si quid erat parum remotum"
            },
            {
                "verse": "Consuetudine, dispulit tenebras"
            },
            {
                "verse": "Et lucem dedit enitentiorem."
            },
            {
                "verse": "Quare sub studium eruditiorum"
            },
            {
                "verse": "Exi lucidus undecumque luci."
            }
        ]
    ]
}