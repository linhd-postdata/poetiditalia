{
    "author": "carmina libraria",
    "title": "Iacobus Magni, sophologium",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|magn|001",
    "text": [
        [
            {
                "verse": "epigramma ad huius operis conspectorem"
            }
        ],
        [
            {
                "verse": "Istuc clarorum contendunt dogmata patrum"
            },
            {
                "verse": "Doctos atque bonos ut faciant homines."
            },
            {
                "verse": "At quom non leviter possit percurrere quisquam"
            },
            {
                "verse": "Auctores cunctos multa neglecta manent"
            },
            {
                "verse": "Omnia doctorum quo ergo documenta legantur,"
            },
            {
                "verse": "Nunc Iacobus magni condidit ecce librum."
            },
            {
                "verse": "Tu quoque si bonus esse velis sapiensque videri"
            },
            {
                "verse": "Quod manibus tracta disce sophologium"
            },
            {
                "verse": "Quicquid enim veterum tetigit praeceptio digna"
            },
            {
                "verse": "Mille voluminibus clauditur hoc opere."
            }
        ]
    ]
}