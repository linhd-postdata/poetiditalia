{
    "author": "carmina libraria",
    "title": "Plutarchus, opuscola",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|plut|001",
    "text": [
        [
            {
                "verse": "Aldus Manutius Romanus Iacobo Antiquario Perisino s.p.d."
            }
        ],
        [
            {
                "verse": "Aldus venit en, Aldus ecce venit,"
            },
            {
                "verse": "Nostrum sinciput occiputque nostrum,"
            },
            {
                "verse": "Mel, sal, lac quoque circulumque solus,"
            },
            {
                "verse": "Graios altera et altera Latinos"
            },
            {
                "verse": "Qui apprendendo manus, reduci omneis"
            },
            {
                "verse": "In verum modo limitem, superbos"
            },
            {
                "verse": "Victores superans Olympiorum."
            },
            {
                "verse": "Nunc, o, nunc iuvenes ubique in urbe"
            },
            {
                "verse": "Flores spargite: vere namque primo"
            },
            {
                "verse": "Aldus venit en, Aldus ecce venit."
            }
        ]
    ]
}