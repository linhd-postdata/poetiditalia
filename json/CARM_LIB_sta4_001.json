{
    "author": "carmina libraria",
    "title": "Statius, silvae, Achilleis, Thebais",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|sta4|001",
    "text": [
        [
            {
                "verse": "Domitii carmen ad Statium"
            }
        ],
        [
            {
                "verse": "Nuper non fueras in urbe notus,"
            },
            {
                "verse": "Incultus, lacer, horridus retonsus"
            },
            {
                "verse": "Nunc vultus reparatus in priores"
            },
            {
                "verse": "Splendens, integer, elegans, politus."
            },
            {
                "verse": "O quot milia fronte basiorum"
            },
            {
                "verse": "Gaudenti excipies, novem sorores"
            },
            {
                "verse": "Tam laetis cupiant sedere sylvis,"
            },
            {
                "verse": "Hic tecum pariter velit Papini"
            },
            {
                "verse": "Phoebus castalium novare carmen;"
            },
            {
                "verse": "I nunc Euboicos celer penates"
            },
            {
                "verse": "Qua surgens tumulo canora Virgo"
            },
            {
                "verse": "Nomen nobile pandit et nepotes."
            },
            {
                "verse": "Illic nam tibi cuncta blandientur"
            },
            {
                "verse": "Et tellus popularibus sonabit"
            },
            {
                "verse": "Certans plausibus; hos dabit iuventus"
            },
            {
                "verse": "Fossos quos bibit ungula liquores"
            },
            {
                "verse": "Proles regia te fovebit illa"
            },
            {
                "verse": "Cui parcae tria lustra computarunt"
            },
            {
                "verse": "Spondentes phyliae situm senectae."
            },
            {
                "verse": "Famae et grandia pensa subministrent"
            },
            {
                "verse": "Quae caeli gemino vehetur axe,"
            },
            {
                "verse": "Huius cum bene noveris Papini"
            },
            {
                "verse": "Mores ingenium decusque formae,"
            },
            {
                "verse": "Tum gratum fuerit perisse quondam"
            },
            {
                "verse": "Huic nunc ut liceat tibi renasci."
            }
        ]
    ]
}