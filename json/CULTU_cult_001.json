{
    "author": "de vero cultu Dei",
    "title": "de vero cultu Dei",
    "url": "http://www.poetiditalia.it/texts/CULTU|cult|001",
    "text": [
        [
            {
                "verse": "Ah genus infoelix, quid mortua templa frequentas?"
            },
            {
                "verse": "Quid gelidis infers oscula imaginibus?"
            },
            {
                "verse": "Ingredere in te ipsum, quoties vis templa videre;"
            },
            {
                "verse": "Nos et imago sumus vivaque templa Dei."
            },
            {
                "verse": "Nos habitat, non saxa Deus, non ligna, nec aera."
            },
            {
                "verse": "Spiritus in nobis qui viget, ampla facit."
            },
            {
                "verse": "Memphiticum Moses potuit domitare tyrannum"
            },
            {
                "verse": "Pressaque servili demere colla iugo."
            },
            {
                "verse": "Phoebeas potuit Iosue retinere quadrigas"
            },
            {
                "verse": "Victor et in regem ingeminare diem."
            },
            {
                "verse": "Quid sequar et reliquos? Homo se cognovit Iesus"
            },
            {
                "verse": "Esse Deum et potuit filius esse Dei."
            },
            {
                "verse": "Exanimes rursus homines hic vivere fecit,"
            },
            {
                "verse": "Imperat et ventis, imperat ille mari,"
            },
            {
                "verse": "Ante oculos posuit miracula plurima rerum,"
            },
            {
                "verse": "A patre testatus tradita cuncta sibi."
            },
            {
                "verse": "Non habitat templis manuum molimine factis"
            },
            {
                "verse": "Omnipotens, aedes aurea verus homo."
            }
        ]
    ]
}