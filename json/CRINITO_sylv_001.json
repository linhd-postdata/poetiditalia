{
    "author": "Pietro Crinito",
    "title": "ad Faustum de Sylva Oricellaria",
    "url": "http://www.poetiditalia.it/texts/CRINITO|sylv|001",
    "text": [
        [
            {
                "verse": "Olim decorum frondibus sacris nemus"
            },
            {
                "verse": "Mones dicandum posteris"
            },
            {
                "verse": "In quo labores Cynthius noster solet"
            },
            {
                "verse": "Doctis levare cantibus."
            },
            {
                "verse": "Non defit ullo tempore salubritas loci,"
            },
            {
                "verse": "Mitisque caeli gratia."
            },
            {
                "verse": "Hinc sat videbis alta Florae moenia"
            },
            {
                "verse": "Et Faesulas bivertices."
            },
            {
                "verse": "O quam reviso nobilem sylvam libens,"
            },
            {
                "verse": "Genio dicatam Palladis!"
            },
            {
                "verse": "Libet vagari Fauste per sacrum nemus,"
            },
            {
                "verse": "Dum vernat alta populus,"
            },
            {
                "verse": "Et dum virenti sibilat pinus coma,"
            },
            {
                "verse": "Amata priscis vatibus."
            },
            {
                "verse": "In astra surgens maximi quaercus Iovis,"
            },
            {
                "verse": "Protendit alta brachia:"
            },
            {
                "verse": "Suberque iuxta pumicoso cortice"
            },
            {
                "verse": "Ramis virescit aemulis"
            },
            {
                "verse": "Illic et alnus blandienti gratia"
            },
            {
                "verse": "Intexit umbris gramina."
            },
            {
                "verse": "Philyraque mollis pervenustis frondibus"
            },
            {
                "verse": "Procrescit in sylvae decus."
            },
            {
                "verse": "Hederae sequaces reptitant per arbores,"
            },
            {
                "verse": "Viridique nexu vestiunt."
            },
            {
                "verse": "Et molliores mutuis amplexibus"
            },
            {
                "verse": "Vites maritantur pyris,"
            },
            {
                "verse": "Ac dum subinde vinciuntur arctius"
            },
            {
                "verse": "Tegunt amores frondibus."
            },
            {
                "verse": "O quam virenti fronde sylvescit nemus"
            },
            {
                "verse": "Cum verna cypris incalet."
            },
            {
                "verse": "Platanus in altum porrigit laetum caput"
            },
            {
                "verse": "Ascita ad umbrae gratiam,"
            },
            {
                "verse": "Mutansque proprium quotannis corticem"
            },
            {
                "verse": "Inter virentes arbores,"
            },
            {
                "verse": "Audax renidet cum iuventam respicit"
            },
            {
                "verse": "Velut resurgens laetior."
            },
            {
                "verse": "Neque haec rigari postulat dulci mero,"
            },
            {
                "verse": "Aut amnium nimphas petit."
            },
            {
                "verse": "Lotos per orbem turbinat comas suas,"
            },
            {
                "verse": "Lotos sacrata Cyntio:"
            },
            {
                "verse": "Ubi grata me Dryas revisit, et comi"
            },
            {
                "verse": "Lepore alumnum detinet."
            },
            {
                "verse": "Sylvam hanc tuetur Curus, et capripes deus:"
            },
            {
                "verse": "Tuentur et Satyri leves."
            },
            {
                "verse": "Iuvat videre laureas Daphnes comas,"
            },
            {
                "verse": "Floresque cum baccis suis."
            },
            {
                "verse": "Illic strepentes conqueruntur alites,"
            },
            {
                "verse": "Varioque certant murmure."
            },
            {
                "verse": "Et interim discursitant damae leves,"
            },
            {
                "verse": "Lepusque subsilit pavens,"
            },
            {
                "verse": "Proludit antris et cuniculus falax,"
            },
            {
                "verse": "Sylvaeque cultor hinnulus"
            },
            {
                "verse": "Et histrices acuminatis spiculis"
            },
            {
                "verse": "Minantur, et tamen placent,"
            },
            {
                "verse": "Seu tela vibrant saeviente praelio,"
            },
            {
                "verse": "Seu mitiores lusitant."
            },
            {
                "verse": "Quantum propinqui floret horti amoenitas ."
            },
            {
                "verse": "Cum consitis abietibus?"
            },
            {
                "verse": "Seu te virentes floreis pratis comae"
            },
            {
                "verse": "Seu cultus herbarum capit,"
            },
            {
                "verse": "Utcumque sit parantur illa commoda,"
            },
            {
                "verse": "Quae molliter curas fovent."
            },
            {
                "verse": "Et o beatum, qui sub antiqua ilice"
            },
            {
                "verse": "Liventis expers ambitus"
            },
            {
                "verse": "Vel sacra vatum curat, aut doctum otium"
            },
            {
                "verse": "Curis solutior fovet."
            },
            {
                "verse": "Sic ille nil miratur aestus principum,"
            },
            {
                "verse": "Nec sceptra regum suscipit."
            },
            {
                "verse": "Sed in virenti detinetur gramine,"
            },
            {
                "verse": "Et se reservat posteris."
            }
        ]
    ]
}