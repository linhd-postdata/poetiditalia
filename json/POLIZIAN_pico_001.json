{
    "author": "Angelo Poliziano",
    "title": "in Ioannem Picum",
    "url": "http://www.poetiditalia.it/texts/POLIZIAN|pico|001",
    "text": [
        [
            {
                "verse": "Dulcis Nobilitas, pudens Iuventus,"
            },
            {
                "verse": "Maiestas hilaris, decora Virtus,"
            },
            {
                "verse": "Mores, Gratia, Forma, Musa, Candor,"
            },
            {
                "verse": "Qui qua sint facie requirit, unum"
            },
            {
                "verse": "Picum si videat, videbit omnes."
            }
        ]
    ]
}