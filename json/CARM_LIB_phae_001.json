{
    "author": "carmina libraria",
    "title": "Phaedrus, fabularum Aesopiarum libri V",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|phae|001",
    "text": [
        [
            {
                "verse": "Phrix ille servus, mente sanus libera"
            },
            {
                "verse": "Graecas iocosus fecit ex re fabulas"
            },
            {
                "verse": "Adfabulatus quae docerent Socratem"
            },
            {
                "verse": "Graecis trimetris vinxit ille Babrius"
            },
            {
                "verse": "Et post Latinis Phaedrus olim Caesaris"
            },
            {
                "verse": "Libertus Augusti stylo atque tempore"
            },
            {
                "verse": "Par proximusve Laberio vel Publio"
            },
            {
                "verse": "Quem nunc tenebris erutum Orcinae specus"
            },
            {
                "verse": "Pithoee superis reddis auris maximam"
            },
            {
                "verse": "Initurus a me gratiam, imo ab omnibus"
            },
            {
                "verse": "Spero eruditis; caeteros nihil moror"
            },
            {
                "verse": "Quis prisca sordet litterarum  puritas."
            },
            {
                "verse": "Tantum o viderem doctiores Principes"
            },
            {
                "verse": "Quam litteratos servulos, quales erant"
            },
            {
                "verse": "Aesopus et libertus iste Principis."
            }
        ]
    ]
}