{
    "author": "carmina libraria",
    "title": "Ambrosius, opera, edidit Iohannes Amerbach",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|ambr|001",
    "text": [
        [
            {
                "verse": "Quid tibi sancta fides pater o memorande rependet"
            },
            {
                "verse": "Quam tua collustrant scripta decora nimis?"
            },
            {
                "verse": "Per te caesaribus vivendi norma beate"
            },
            {
                "verse": "Praescripta est multis christicolisque bonis."
            },
            {
                "verse": "Plurima certe tuis debet veneranda libellis"
            },
            {
                "verse": "Religio infractam quod facis esse fidem."
            },
            {
                "verse": "Haeretici exhorrent merito venerabile nomen."
            },
            {
                "verse": "Ambrosii, quorum malleus ipse fuit."
            },
            {
                "verse": "Nec potuere quidem verbum mutare maligni"
            },
            {
                "verse": "Illius ex scriptis dogmatibusque viri."
            }
        ]
    ]
}