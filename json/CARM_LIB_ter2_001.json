{
    "author": "carmina libraria",
    "title": "Terentius, comoediae",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|ter2|001",
    "text": [
        [
            {
                "verse": "Horridus atque lacer fuit ante Terentius, apta"
            },
            {
                "verse": "Nunc iterum meruit cultius ire toga."
            },
            {
                "verse": "Vix agnoscendus carie mendisque latebat"
            },
            {
                "verse": "Omnia nunc prisco reddita more nitent."
            },
            {
                "verse": "Ergo age festivi pubes germana Terenti,"
            },
            {
                "verse": "Disce iocos. Linguae consulet ille tuae."
            }
        ],
        []
    ]
}