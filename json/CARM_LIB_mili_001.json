{
    "author": "carmina libraria",
    "title": "Iohannes Milis, repertorium",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|mili|001",
    "text": [
        [
            {
                "verse": "Noscere forte voles quis sculpserit hoc opus aere"
            },
            {
                "verse": "Presserit has chartas quisve caracteribus,"
            },
            {
                "verse": "Ne mirere: licet tersum mendisque solutum"
            },
            {
                "verse": "Immune et vitiis hoc opus invenias."
            },
            {
                "verse": "Ille quidem Kesler Nicolaus littore Rheni"
            },
            {
                "verse": "Urbs dedit insigne cui Basilea domum."
            },
            {
                "verse": "Ille inquam impensis qui numquam (crede) pepercit"
            },
            {
                "verse": "Lector amice dabit his liber ille fidem"
            },
            {
                "verse": "Hic etenim secreta obstrusaque Palladis arce"
            },
            {
                "verse": "Fortia iura omnis legulus inveniet."
            },
            {
                "verse": "Comperiet praesens quicquid quaesisse necesse est"
            },
            {
                "verse": "Absenti quamvis nomen ubique gerat."
            }
        ]
    ]
}