{
    "author": "carmina libraria",
    "title": "Henricus Bebellus, facetiae",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|bebe|001",
    "text": [
        [
            {
                "verse": "liber ad lectorem"
            }
        ],
        [
            {
                "verse": "Pone supercilium lector nasute, dicaces"
            },
            {
                "verse": "Affero ego risus atque sales lepidos;"
            },
            {
                "verse": "Curia me spernat, fora me clamosa refutent,"
            },
            {
                "verse": "Solus ego mensis laetitiaeque cano."
            },
            {
                "verse": "Atque etiam sapiens nos inter pocula tractet"
            },
            {
                "verse": "Et tetricas curas mitiget ipse iocis."
            },
            {
                "verse": "Nemo etiam carpet tristem qui fronte Catonem"
            },
            {
                "verse": "Noverit in libris composuisse sales."
            }
        ]
    ]
}