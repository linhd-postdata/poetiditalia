{
    "author": "Panormita",
    "title": "epitaphium pro Kambio de Zambecariis",
    "url": "http://www.poetiditalia.it/texts/PANORM|epit|001",
    "text": [
        [
            {
                "verse": "Kambius hoc tegitur stirps Zambecaria busto,"
            },
            {
                "verse": "Quocum libertas urbis humata iacet."
            },
            {
                "verse": "Ipse eques atque gradu fuerat prognatus equestri,"
            },
            {
                "verse": "Progenies summo vel patre digna Iove."
            },
            {
                "verse": "Providus et preter fraudem fuit alter Ulixes,"
            },
            {
                "verse": "Eneadum similis dextra animoque fuit."
            },
            {
                "verse": "Pauperibus dono gratus, pietate misellis,"
            },
            {
                "verse": "Iustitia populo, relligione deis."
            },
            {
                "verse": "Praeterea interpres legum non proximus ulli"
            },
            {
                "verse": "Nec minus excellens artibus ingenuis."
            },
            {
                "verse": "<>"
            },
            {
                "verse": "Hic potuit..."
            },
            {
                "verse": "Iam potes ex paucis deprendere plura viator."
            },
            {
                "verse": "Invidia scribi marmora plura negant."
            }
        ]
    ]
}