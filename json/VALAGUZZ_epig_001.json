{
    "author": "Giorgio Valaguzza",
    "title": "epigramma",
    "url": "http://www.poetiditalia.it/texts/VALAGUZZ|epig|001",
    "text": [
        [
            {
                "verse": "Que genuit celo divinum Lutia Sidus,"
            },
            {
                "verse": "Quo fulgent populi, conditur hoc tumulo:"
            },
            {
                "verse": "Sfortia Franciscus te diva matre creatus"
            },
            {
                "verse": "Armorum splendor Gloria Luxque toge."
            }
        ]
    ]
}