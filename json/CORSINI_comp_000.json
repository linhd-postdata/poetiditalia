{
    "author": "Amerigo Corsini",
    "title": "compendium in vitam Cosmi Medicis",
    "url": "http://www.poetiditalia.it/texts/CORSINI|comp|000",
    "text": [
        [],
        [
            {
                "verse": "Mittit Amerigus tibi, Laurens inclite, versus,"
            },
            {
                "verse": "Qui pedibus trepidis atria magna petunt."
            },
            {
                "verse": "Nec mirum, quoniam non doctus carmina vates"
            },
            {
                "verse": "Dictitat, immo rudis institor illa tulit."
            },
            {
                "verse": "Sed tibi, quem referunt, commendet carmina Cosmus,"
            },
            {
                "verse": "Cosmus, Tyrrheni gloria magna soli."
            }
        ]
    ]
}