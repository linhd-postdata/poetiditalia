{
    "author": "carmina libraria",
    "title": "Guilemus de Gouda, cronica",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|goud|001",
    "text": [
        [
            {
                "verse": "Querela de Obitu clarissimi et reverendi viri domini magistri Caspari Rodolphi dialectices professoris in celeberrima academia marpugensi qui obiit 23 die Augusti Anno Christi 1561 scripta ad doctissimum virum M. Theophilum Lonicerum Physiscum et praecetore suum candidissimum"
            }
        ],
        [
            {
                "verse": "A Iacobolo Alutario"
            },
            {
                "verse": "Iuniore Herbornensi"
            }
        ],
        [
            {
                "verse": "Musae Leucorides suum Philippum"
            },
            {
                "verse": "Lugent; Marchiacae suum Sabinum,"
            },
            {
                "verse": "Heydelbergiacae Petrum Secundum"
            },
            {
                "verse": "Vates Teutonicos honore dignos."
            },
            {
                "verse": "At nunc vos etiam decent camoenae"
            },
            {
                "verse": "Luctus Hassiacae gravesque planctus."
            },
            {
                "verse": "Casparus siquiem modo Rodolphi"
            },
            {
                "verse": "Vi mortis valida iacet peremtus"
            },
            {
                "verse": "Vestri Gymnasii corona fulgens."
            },
            {
                "verse": "Vir praestans pietate litterisque,"
            },
            {
                "verse": "Magna fsedulitate qui iuventam"
            },
            {
                "verse": "Informavit in arte differendi"
            },
            {
                "verse": "Et custos fuit ipse disciplinae."
            },
            {
                "verse": "Nam mores iuvenum regebat omnes."
            },
            {
                "verse": "Ah, hic quoties videns cohortem"
            },
            {
                "verse": "Doctam commaculare moribus se"
            },
            {
                "verse": "Spurcis, ingenuit ferens moleste."
            },
            {
                "verse": "Certe verus erat pater cohortis."
            },
            {
                "verse": "Ergo conveniens tibi iuventus"
            },
            {
                "verse": "Amisso patre tempus est dolendi."
            },
            {
                "verse": "Nunc praecordia tunde prae dolore"
            },
            {
                "verse": "Palmis et graviter pium Magistrum"
            },
            {
                "verse": "Deplorato tuum senem verendum"
            },
            {
                "verse": "Cuius fama manebit, ac honores,"
            },
            {
                "verse": "Arx dum Martis erit ferocis alta."
            },
            {
                "verse": "Sed tu Gnate Dei futura damna"
            },
            {
                "verse": "Precor discutias tuum paterna,"
            },
            {
                "verse": "Coetum protege pro benignitate,"
            },
            {
                "verse": "Turcae ne cadat in manus minacis"
            },
            {
                "verse": "Tolluntur quoniam boni malum ne"
            },
            {
                "verse": "Venturum videant diesque tristes."
            }
        ]
    ]
}