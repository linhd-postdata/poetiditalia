{
    "author": "Giacomo Bon",
    "title": "de raptu Cerberi",
    "url": "http://www.poetiditalia.it/texts/BON|rapt|000",
    "text": [
        [],
        [
            {
                "verse": "Diuas, quae Paphiam deam colentem"
            },
            {
                "verse": "Siluas Idalii et nemus Cytheri"
            },
            {
                "verse": "Formosam faciunt Apollinemque,"
            },
            {
                "verse": "Musas, Pallada Liberumque patrem,"
            },
            {
                "verse": "Nostris praeposui tribus libellis,"
            },
            {
                "verse": "Qui Christum Herculea canunt figura,"
            },
            {
                "verse": "Cuius tu imperium geris uicemque."
            },
            {
                "verse": "Iccirco hunc tibi, pontifex, dicamus,"
            },
            {
                "verse": "Orbes quem decimum uocant Leonem."
            },
            {
                "verse": "Ne te mouerit alteri quod olim"
            },
            {
                "verse": "Hic a me fuit Hercules dicatus!"
            },
            {
                "verse": "Est Christus modo, quem refers leonem,"
            },
            {
                "verse": "De Iudae catuli tribu leonis."
            },
            {
                "verse": "Hic omnes superat feras et omni"
            },
            {
                "verse": "Prostrato leo parcit et pusillo."
            },
            {
                "verse": "Res haec magnanimum decet leonem."
            }
        ]
    ]
}