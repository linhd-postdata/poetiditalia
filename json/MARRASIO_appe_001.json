    "author": "Giovanni Marrasio",
    "title": "carminum appendix",
    "url": "http://www.poetiditalia.it/texts/MARRASIO|appe|001",
    "text": [
        [
            {
                "verse": "Aurispa Marrasio"
            }
        ],
        [
            {
                "verse": "Non me germani, patriae nec dulcis amores"
            },
            {
                "verse": "Sollicitant; me solus habes, mea pectora versas"
            },
            {
                "verse": "Solus et ante oculos noctuque dieque pererras."
            }
        ]
    ]
}