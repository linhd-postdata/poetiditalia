    "text": [
        [],
        [
            {
                "verse": "ad Antonellum Sanctoseverinumprincipem Salernitanum"
            }
        ],
        [
            {
                "verse": "Si tuus est vates, si cuncta volumina vatis,"
            },
            {
                "verse": "Quaeris praecipue cur tuus iste liber?"
            },
            {
                "verse": "Sol quoque cuncta regit, solis tamen unica sedes:"
            },
            {
                "verse": "Haec eadem regum est, quae superum est ratio."
            }
        ]
    ]
}