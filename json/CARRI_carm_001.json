{
    "author": "Ludovico Carri",
    "title": "ad Cassandram Fidelem",
    "url": "http://www.poetiditalia.it/texts/CARRI|carm|001",
    "text": [
        [
            {
                "verse": "[Ludovicus Carrus ad divam Cassandram Fidelem Venetam]"
            }
        ],
        [
            {
                "verse": "Pancharis aetruscae celeberrima gloria linguae"
            },
            {
                "verse": "Pierisque chori non mediocre decus"
            },
            {
                "verse": "Extulit eximiis tua <...> laudibus, et te"
            },
            {
                "verse": "Absentem miro iunxit amore mihi."
            },
            {
                "verse": "Diceris egregia specie superare Dianam"
            },
            {
                "verse": "Atque pudicitia vincere Penelopem,"
            },
            {
                "verse": "Dexteritate manus doctae tibi cedit Arachne;"
            },
            {
                "verse": "Cedit et ingenio diva Minerva tuo,"
            },
            {
                "verse": "Sed magis admiror quod pleno discutis ore"
            },
            {
                "verse": "Quae Plato, quae magnus tradidit Aristoteles."
            }
        ]
    ]
}