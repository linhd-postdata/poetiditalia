{
    "author": "Pietro Paolo, il Vecchio Vergerio",
    "title": "carmina",
    "url": "http://www.poetiditalia.it/texts/VERG_SEN|carm|001",
    "text": [
        [
            {
                "verse": "Haec carmina feci Vergerius, dum Franciscus Zabarella inscripsit nomini suo Libros, quos ediderat de FelicitatePetrus Paulus Vergerius IustinopolitanusFrancisco ZabarellaeQui postea fuit in Cardinalium Collegium cooptatus"
            },
            {
                "verse": "Omnia iam dudum quum sint communia nobis,"
            },
            {
                "verse": "Ingenii proprii laus tibi sola fuit."
            },
            {
                "verse": "Qua procul Ausonio late cognosceris orbe,"
            },
            {
                "verse": "Et celebrat nomen barbara terra tuum."
            },
            {
                "verse": "Hoc quoque partiri mecum cupis usque benignus,"
            },
            {
                "verse": "Ne quid forte tibi sit sine me proprium."
            },
            {
                "verse": "Nomina namque tuo subscribis nostra Libello,"
            },
            {
                "verse": "Meque operis tanti vis fore participem."
            },
            {
                "verse": "Hoc ego non mereor, nec me pars ulla laboris"
            },
            {
                "verse": "Contigit: omnis erat cura, laborque tuus."
            },
            {
                "verse": "Si tamen immeritum titulis ornare parasti"
            },
            {
                "verse": "Talibus, ecce patens area lata tibi."
            },
            {
                "verse": "Scribis enim, semperque premis sine pace papyros,"
            },
            {
                "verse": "Sensa quibus Legum discutis ambigua."
            },
            {
                "verse": "His nomen praescribe meum, quo praedicer amplis"
            },
            {
                "verse": "Commonitus [\u00e2\u0080\u00a6] iura voluminibus."
            }
        ],
        [
            {
                "verse": "Ad illustrem principemFranciscum Iunioremde CarrariaSuper reditu Natorum eius Francisci, et Iacobiex hostili captivitate congratulantis multitu-dinis Metrum Saphicum, Adonicum,Dicolos, TetrastrophosP. P. Vergerii Iustinopolitani"
            },
            {
                "verse": "Carriger nobis Pater, atque Princeps"
            },
            {
                "verse": "Cuius urbs ductu Patavina dudum"
            },
            {
                "verse": "Floret, et latis procul alma terris"
            },
            {
                "verse": "Fama pererrat."
            },
            {
                "verse": "Exhibe nunc te populo videndum:"
            },
            {
                "verse": "Vince languorem modo, si quis exstat:"
            },
            {
                "verse": "Non satis firmum recreent amoena"
            },
            {
                "verse": "Gaudia corpus."
            },
            {
                "verse": "En senes tardi, pavidaeque matres,"
            },
            {
                "verse": "Virginum coetus, pueri, puellae"
            },
            {
                "verse": "Proruunt tectis, alacrique certant"
            },
            {
                "verse": "Plaudere gestu."
            },
            {
                "verse": "Cerno quos belli similis procella"
            },
            {
                "verse": "Cepit, et virtus similis redemit,"
            },
            {
                "verse": "Laetus ambobus vario receptis"
            },
            {
                "verse": "Tempore natis."
            },
            {
                "verse": "Ille victores prius insolentes"
            },
            {
                "verse": "Lusit, et noctu comites adhortans"
            },
            {
                "verse": "Praeterit muros, superat profundas"
            },
            {
                "verse": "Aggere fossas."
            },
            {
                "verse": "Inde veloci properat anhelus"
            },
            {
                "verse": "In nemus densum, penetrare cursu"
            },
            {
                "verse": "Culta dimittit, solitosque calles,"
            },
            {
                "verse": "Devia pergens."
            },
            {
                "verse": "Ambulat noctu, redeunte luce"
            },
            {
                "verse": "Insidet sylvis; famis, et viarum"
            },
            {
                "verse": "Sentit angores, patriamque tandem"
            },
            {
                "verse": "Appulit Urbem."
            },
            {
                "verse": "Hic pilae ludum studio frequentans"
            },
            {
                "verse": "Est locum nactus, simul atque tempus,"
            },
            {
                "verse": "Unde custodes super imminentes"
            },
            {
                "verse": "Fallere posset."
            },
            {
                "verse": "Quos ioco primum spoliavit armis,"
            },
            {
                "verse": "Inde vagina gladium resumens,"
            },
            {
                "verse": "Exilit porti, mediaque luce"
            },
            {
                "verse": "Moenia transit."
            },
            {
                "verse": "Nave transvectus stagna lata linquit,"
            },
            {
                "verse": "Unde progressus fluvio rapaci"
            },
            {
                "verse": "Innatat: tandem loca tuta sospes"
            },
            {
                "verse": "Nave recepit."
            },
            {
                "verse": "Stipiti semper generosa surgens"
            },
            {
                "verse": "Virga respondet, sequiturque Patrem"
            },
            {
                "verse": "Passibus magnis bene nata proles,"
            },
            {
                "verse": "Cum finit aetas."
            },
            {
                "verse": "Tu tuam quondam prope solus Urbem"
            },
            {
                "verse": "Vi recepisti, redimisque victor."
            },
            {
                "verse": "Patriamque muro triplici recinctam"
            },
            {
                "verse": "Hoste fugato."
            },
            {
                "verse": "Nunc tui Nati bona spes Parentum,"
            },
            {
                "verse": "Quos nequiverunt numero minores"
            },
            {
                "verse": "Vincere armati, potuere inermes"
            },
            {
                "verse": "Vincere sensu."
            },
            {
                "verse": "Igitur felix age tempus omne:"
            },
            {
                "verse": "Clara te Virtus tua, te tuorum"
            },
            {
                "verse": "Clara Natorum probitas beatum"
            },
            {
                "verse": "Reddet in aevum."
            },
            {
                "verse": "Nos diem faustum colimus canentes,"
            },
            {
                "verse": "Quo tuos Natos reduces videmus:"
            },
            {
                "verse": "Longa sit Patri tibi, longa natis"
            },
            {
                "verse": "Vita precamur."
            }
        ]
    ]
}