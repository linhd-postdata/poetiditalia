{
    "author": "Francesco Pescennio Negri",
    "title": "carmina ad Cosmodystychiam pertinentia",
    "url": "http://www.poetiditalia.it/texts/NEGRI_FP|cosm|001",
    "text": [
        [
            {
                "verse": "Pragmata cunctiparae phiseos simul omnia si quis"
            },
            {
                "verse": "Nosse cupit, nostrum perlegat istud opus"
            },
            {
                "verse": "Omnia tam parvo cernes congesta libello,"
            },
            {
                "verse": "Tu modo cuncta legas, nec vereare moras."
            },
            {
                "verse": "Inde genetliacon auctoris rite Celaeni"
            },
            {
                "verse": "Inspicies, mores fataque dira mei."
            },
            {
                "verse": "In cuius tumulum ter iacto pulvere dicas:"
            },
            {
                "verse": "Hoc saltem Nigri molliter ossa cubent."
            }
        ],
        [
            {
                "verse": "Pastor, quem boreas terruit horrifer"
            },
            {
                "verse": "Decertans hyemis tempore frigidae"
            },
            {
                "verse": "Concussos pelagi flectere vortices"
            },
            {
                "verse": "Et saevo nemorum sternere spiritu"
            },
            {
                "verse": "Tractus floridulos, grata favonii"
            },
            {
                "verse": "Expectat cupido flamina pectore,"
            },
            {
                "verse": "Ut laxo Cybeles munere deviae"
            },
            {
                "verse": "Et carpant cythisos rure capellulae,"
            },
            {
                "verse": "Et cantus solitos fistula personet,"
            },
            {
                "verse": "Abiectis gravibus corde serenulo"
            },
            {
                "verse": "Curis, sollicitant quae male pauperes;"
            },
            {
                "verse": "Sic fluctu medio stat Niger aequoris"
            },
            {
                "verse": "Nimbosi, celeres horridus aphricos"
            },
            {
                "verse": "Perpessus rapidis nunc aquilonibus"
            },
            {
                "verse": "Certantes, tumidis nunc propius notis,"
            },
            {
                "verse": "Expectans tepidae sed tamen optima"
            },
            {
                "verse": "Aurai tremulo tempora corculo:"
            },
            {
                "verse": "Hunc imbres feriunt, hunc Iovis improbi"
            },
            {
                "verse": "Certant Olenio frigore tempora"
            },
            {
                "verse": "Consumptum miseris manibus addere."
            },
            {
                "verse": "Euxini dubius versor in aequoris"
            },
            {
                "verse": "Anfractu, teneam quem prius haesitans"
            },
            {
                "verse": "Portum, seu capiam quae modo littora:"
            },
            {
                "verse": "Per cautes veluti signa Caphareas"
            },
            {
                "verse": "Deducens, trepidas inter et Orcadas,"
            },
            {
                "verse": "Horresco scilices laedere Nauplias"
            },
            {
                "verse": "Et quicquid rapidi unda minax freti"
            },
            {
                "verse": "Portendit pavidis puppibus horridi."
            },
            {
                "verse": "At tu, Christicolum maxime pontifex,"
            },
            {
                "verse": "Prolem qui Medicen semper amabilem,"
            },
            {
                "verse": "Illustrem nitidam, splendidiusculo"
            },
            {
                "verse": "Ornas ingenio, consilio, fide,"
            },
            {
                "verse": "Prudenti eloquio, quod sapientia"
            },
            {
                "verse": "Mixtum iusticia dexteritateque"
            },
            {
                "verse": "Divinis penitus fragrat odoribus,"
            },
            {
                "verse": "Ingens Italiae splendor et unicum"
            },
            {
                "verse": "Virtutis columen, pontificum decus,"
            },
            {
                "verse": "Pravis exitium, praesidium bonis;"
            },
            {
                "verse": "Tu mi Tyndaridum stella nitentior,"
            },
            {
                "verse": "Tu mi leuconotus nubila deterens,"
            },
            {
                "verse": "Tu mi candidulus portus et optimi"
            },
            {
                "verse": "Veris nuncia nunc aura favonii."
            },
            {
                "verse": "Emittas radios, quaeso, carinulae"
            },
            {
                "verse": "Erranti, ut meritum littore faustulo"
            },
            {
                "verse": "Firmata exodium iam tibi concinat."
            },
            {
                "verse": "Sic te purpureo flore Fluentia"
            },
            {
                "verse": "Felicem videat, patria nobilis,"
            },
            {
                "verse": "Fuso Nestoreo puniceos dies"
            },
            {
                "verse": "Explere, ut supera splendidus abside"
            },
            {
                "verse": "Demittas radios inferioribus."
            }
        ],
        [
            {
                "verse": "Qui cantu colitis pio Camenas"
            },
            {
                "verse": "Terpandri Ismeniaeve vel Pelasgi"
            },
            {
                "verse": "Saccae legibus abditam per artem;"
            },
            {
                "verse": "Qui Permessidos ora proluistis"
            },
            {
                "verse": "Alto gurgite; Quisquis astra certis"
            },
            {
                "verse": "Scrutatur numeris statumque rerum"
            },
            {
                "verse": "Scit caelestibus applicare causis,"
            },
            {
                "verse": "Et quicumque abacos arat vel aequor"
            },
            {
                "verse": "Docti pulveris imprimit figuris,"
            },
            {
                "verse": "Gerrhas perlegito tui Celaeni"
            },
            {
                "verse": "Naso quo Canius solet Catones."
            }
        ]
    ]
}