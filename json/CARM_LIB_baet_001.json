{
    "author": "carmina libraria",
    "title": "Gaspar Baetiae, opera omnia",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|baet|001",
    "text": [
        [
            {
                "verse": "licentiati Fernandi de Castro carmen aud auctorem"
            }
        ],
        [
            {
                "verse": "Floriferos referunt festiva poemata campos,"
            },
            {
                "verse": "Floriferum reddunt scripta serena solum."
            },
            {
                "verse": "Laudantur variis redolentia floribus arva"
            },
            {
                "verse": "Sed mage qui fructum gignit amoenus ager."
            },
            {
                "verse": "Docte, tuis, Caspar, scriptis innascitur annis"
            },
            {
                "verse": "Copia Pomonae choridos atque deae."
            },
            {
                "verse": "Molle quibus veterum repleta volumina partus"
            },
            {
                "verse": "Hic liber omnigenos ut novus hortus habet."
            },
            {
                "verse": "Extulit in Caelum Alcidem, non immemor aetas"
            },
            {
                "verse": "Thuraque cum precibus vittaque saepe dedit."
            },
            {
                "verse": "Et quantum Alcides nodoso stipite tantum"
            },
            {
                "verse": "Praestitit hic clari viribus ingenii."
            },
            {
                "verse": "Ingenium tibi deficiet, si dicere laudes"
            },
            {
                "verse": "Forte viri immensas longius ausus eris."
            }
        ]
    ]
}