{
    "author": "carmina libraria",
    "title": "Guilelmus Paraldus, sermones dominicales",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|para|001",
    "text": [
        [
            {
                "verse": "ad doctorem Venedelinum Stainbach theologiae specimen et ornamentum pro indice ab eo composito in homilialogum Domini Guiglielmi parhisiensis"
            }
        ],
        [
            {
                "verse": "Disertissime Vendeline patrum"
            },
            {
                "verse": "Divinae moderator et palestrae"
            },
            {
                "verse": "Idem non modicus tuus poeta"
            },
            {
                "verse": "Commendat sine bladulo susurro"
            },
            {
                "verse": "Usum quem publicum tuo labore"
            },
            {
                "verse": "Conatus vigili et innare nocte"
            },
            {
                "verse": "(Quae res in numerum dicare divum"
            },
            {
                "verse": "Convenit veteres scientiores)"
            },
            {
                "verse": "Iam nunc fructiferum indicem et venustum"
            },
            {
                "verse": "In librum popularis addidisti"
            },
            {
                "verse": "Huius sermologi genus ad omne"
            },
            {
                "verse": "Virtutis vehementer incitantis"
            },
            {
                "verse": "Peccatum soliti execrare acerbae."
            },
            {
                "verse": "Hoc propter meruit, cole perenne"
            },
            {
                "verse": "Auctor posteritatibus sacrandus"
            },
            {
                "verse": "Has grates referant tibi minores"
            },
            {
                "verse": "Lectores duce quo tua Minerva"
            },
            {
                "verse": "Invenisse queunt libro repente"
            },
            {
                "verse": "In toto optima quaeque non tacenda"
            },
            {
                "verse": "Omne ad propositum satis decenter."
            },
            {
                "verse": "O sudor veneratione dignus"
            },
            {
                "verse": "Utcumque est stomacho legas benigno"
            },
            {
                "verse": "Muse bebelie rudes ofellas"
            },
            {
                "verse": "Solum mnemosynon tui clientis"
            },
            {
                "verse": "Orans ut tibi sit perenne carus"
            },
            {
                "verse": "Et notus bene pessimus poeta,"
            },
            {
                "verse": "Tanto pessimus omnium poeta"
            },
            {
                "verse": "Quanto tu optimus es theologiae"
            },
            {
                "verse": "Doctor. Nestoreos vale per annos."
            }
        ]
    ]
}