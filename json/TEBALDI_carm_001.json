{
    "author": "Pietro Paolo Tebaldi",
    "title": "carmina",
    "url": "http://www.poetiditalia.it/texts/TEBALDI|carm|001",
    "text": [
        [
            {
                "verse": "Quom vigiles tacuere canes, et gallica turba"
            },
            {
                "verse": "Coeperant auspicio moenia facta deum"
            },
            {
                "verse": "Essent coniectis gallorum perdita flammis"
            },
            {
                "verse": "Si tempestive non strepuisset avis:"
            },
            {
                "verse": "Nullaque de tantis restassent nomina rebus:"
            },
            {
                "verse": "Romulidumque omnis gloria tecta foret."
            },
            {
                "verse": "Sic fuerant latiae peritura poemata linguae"
            },
            {
                "verse": "Ni flueret Galli dulcis ab ore melos."
            }
        ],
        [
            {
                "verse": "Bruma imbres veniunt, veniunt aestate calores:"
            },
            {
                "verse": "Imbribus haec multis, illa calore mala est:"
            },
            {
                "verse": "Pomifer autumnus, vel pressis sordidus uvis"
            },
            {
                "verse": "Ad stygias multos impulit ire rates."
            },
            {
                "verse": "Ver violis, ver flore nitens mortalibus almum est."
            },
            {
                "verse": "Namque imbre, et pomis, sorde, calore caret"
            },
            {
                "verse": "Ver anno ut melius, cunctis sic Vera libellus"
            },
            {
                "verse": "Arte, iocis, culto carmine, materiae."
            }
        ]
    ]
}