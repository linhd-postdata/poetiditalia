{
    "author": "carmina libraria",
    "title": "Robertus Valturius, de re militari",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|valt|001",
    "text": [
        [
            {
                "verse": "Pauli Ramusii epigramma"
            }
        ],
        [
            {
                "verse": "Armorum fulmen iacet hic, Malatesta Robertus"
            },
            {
                "verse": "Cuius praestantes gloria vicit avos."
            },
            {
                "verse": "Nam post mille acies superatas marte secundo"
            },
            {
                "verse": "Dum solvit patrios obsidione lares,"
            },
            {
                "verse": "Agmina prostravit tuscis immissa per agros"
            },
            {
                "verse": "Et calabri fudit fortia castra ducis."
            },
            {
                "verse": "Occubuit posthaec inter fera proelia certe"
            },
            {
                "verse": "Non potuit fato candidiore mori."
            }
        ]
    ]
}