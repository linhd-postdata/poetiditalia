{
    "author": "carmina libraria",
    "title": "Prosperus Calanius, paraphrasis in librum Galeni de inaequali intemperie",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|calp|001",
    "text": [
        [
            {
                "verse": "Ioannes Baptista Gyraldus, medicus Ferrariensis ad libellum Prosperi Calanis"
            }
        ],
        [
            {
                "verse": "Rectam Asclepiadum viam secute"
            },
            {
                "verse": "Quam cous senior prius polivit,"
            },
            {
                "verse": "Et mox excoluit magis Galenus;"
            },
            {
                "verse": "I felix liber, I virum per ora"
            },
            {
                "verse": "Doctorum intrepidus, nihil pavesce,"
            },
            {
                "verse": "Nil tantis ducibus tibi timendum."
            },
            {
                "verse": "Ipso nomine vel beatus ibis."
            }
        ]
    ]
}