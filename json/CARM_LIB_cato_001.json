{
    "author": "carmina libraria",
    "title": "Marcus Porcius Cato, disticha",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|cato|001",
    "text": [
        [
            {
                "verse": "ad Cathonis lectorem. Sebastianus Brandt"
            }
        ],
        [
            {
                "verse": "Aspice divinum praeclari dogma Cathonis"
            },
            {
                "verse": "Quo pueros mores cum gravitate docet."
            },
            {
                "verse": "Instruit infantes materno in lacte decorem"
            },
            {
                "verse": "Haurire et mores indolem et inde bonam."
            },
            {
                "verse": "Hunc lege non propter vim carminis amplaque verba,"
            },
            {
                "verse": "Sed decus unde tibi prodeat atque salus."
            },
            {
                "verse": "Ut postquam instructus sis moribus, inde poetas"
            },
            {
                "verse": "Virgilium atque alios historicosque leges."
            },
            {
                "verse": "Eccce Cathonis vernarum carmina lingua"
            },
            {
                "verse": "Vertimus; hos rythmos edidimusque novos."
            },
            {
                "verse": "Ac verbum verbo curavi reddere quantum"
            },
            {
                "verse": "Id rythmus tulit et praecipitantis opus."
            },
            {
                "verse": "Nam bene dum pueris statui facere atque iuventae"
            },
            {
                "verse": "Apposui levibus versibus eccce manum."
            },
            {
                "verse": "Atque aliquem in tenui contrivi messe laborem"
            },
            {
                "verse": "Qui tibi si placeat dic mihi Teda. Vale."
            }
        ]
    ]
}