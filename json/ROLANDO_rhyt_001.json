{
    "author": "Rolando",
    "title": "rhythmus",
    "url": "http://www.poetiditalia.it/texts/ROLANDO|rhyt|001",
    "text": [
        [
            {
                "verse": "Anni ducentesimi quinti et milleni"
            },
            {
                "verse": "Quadrageni tempore currunt isto pleni"
            },
            {
                "verse": "Quo Rex Regum intulit ictum in ameni"
            },
            {
                "verse": "Cornetani Populi coetum, et sereni"
            },
            {
                "verse": "Nam tuentes patriam fide Nazarei"
            },
            {
                "verse": "Et Iura Ecclesiae defendentes Dei"
            },
            {
                "verse": "Capti sunt insidiis Frederici Rei"
            },
            {
                "verse": "Quatrageni quatuor in luce Diei"
            },
            {
                "verse": "Qui est quintus Decimus in Septembris mense"
            },
            {
                "verse": "In exacto praelio plures cadunt ense"
            },
            {
                "verse": "Cappineto hinc et hinc sunt nimis offense"
            },
            {
                "verse": "Acies et machine trite sunt condense"
            },
            {
                "verse": "Ut ovis ad victimam ducti sunt ligati"
            },
            {
                "verse": "Innocentes nequiter bonis denudati"
            },
            {
                "verse": "Sistunt in cumpedibus ferreis catenati"
            },
            {
                "verse": "Atro nempe carcere steterunt dapnati"
            },
            {
                "verse": "Nequam Capitanens cuncta seriatim"
            },
            {
                "verse": "Retulit nequissimus, ac in scriptis statim"
            },
            {
                "verse": "Qui rescripsit propere mandans nominatim"
            },
            {
                "verse": "Cornetanos perimi furcis catervatim"
            },
            {
                "verse": "Et Vitalis pessimus illud Adversanus"
            },
            {
                "verse": "Nuntiat sententiam duram, et vesanus"
            },
            {
                "verse": "Furcis, ut perimeret gula, non per manus"
            },
            {
                "verse": "Nisi terram redderet cetus Cornetanus"
            },
            {
                "verse": "Ad hoc constantissimi fide respondentes"
            },
            {
                "verse": "Nos subire potius mortem non merentes"
            },
            {
                "verse": "Velle, quam ut populus Cornetanus dentes"
            },
            {
                "verse": "Sensiat rabdissimos, et crudeles gentes"
            },
            {
                "verse": "Pravus ille Nuntius orbis destructoris"
            },
            {
                "verse": "Frederici pessimi summi proditoris"
            },
            {
                "verse": "Scelus nefandissimum contulit doloris"
            },
            {
                "verse": "Dum in Christi filios dat causam meroris"
            },
            {
                "verse": "Posquam per hebdomadas septem nominatos"
            },
            {
                "verse": "Tenuit Cristicolas atque condamnatos"
            },
            {
                "verse": "Cornetanam patriam properans nudatos"
            },
            {
                "verse": "Eos secum praecipit, ducitque ligatos"
            },
            {
                "verse": "Intrante Novembrio die Sabbatorum"
            },
            {
                "verse": "Quarta constituit pena dapnatorum"
            },
            {
                "verse": "Incidunt ligantia filii pravorum"
            },
            {
                "verse": "Quibus furcas erigunt in nece Iustorum"
            },
            {
                "verse": "Secus Martam erigunt supra Maloferam"
            },
            {
                "verse": "Furcas sub Monterano dant vocem severam"
            },
            {
                "verse": "Alt Alt Alt captum Cornet et tunc sancti veram"
            },
            {
                "verse": "Precem dicunt Domino satis adque meram"
            },
            {
                "verse": "Christe Rex mirifice tibi commendamus"
            },
            {
                "verse": "Nos, et nostram patriam ut tecum vivamus"
            },
            {
                "verse": "Nam tuam iustitiam nunquam dubitamus"
            },
            {
                "verse": "Mori, o Rex Gloriae, ad te nunc migramus"
            },
            {
                "verse": "Censerat Vicarius Petrus Malaspina"
            },
            {
                "verse": "Neminem respicere illos in ruina"
            },
            {
                "verse": "Nam quae agunt impii fient repentina"
            },
            {
                "verse": "Et nos consolabitur gratia divina"
            },
            {
                "verse": "Et tunc vox nec strepitus fuit per Cornetum"
            },
            {
                "verse": "Orum patres, filii, fratres ad quietum"
            },
            {
                "verse": "Sunt, et nemo ausus vultu dare fletum"
            },
            {
                "verse": "Fidei constantia deponentes metum"
            },
            {
                "verse": "Tunc Vitalis rabie, ac furore plenus"
            },
            {
                "verse": "Precipit fidelium furcis mori genus"
            },
            {
                "verse": "Dei, qui sunt numero binus et terdenus"
            },
            {
                "verse": "Abeunt in Domino, Coetus sit serenus"
            },
            {
                "verse": "Quorum nota nomina est Octavianus"
            },
            {
                "verse": "Geptius, et Angelus, et Gerardus planus"
            },
            {
                "verse": "Petrus surdus alacer ac dura manus"
            },
            {
                "verse": "Quisque milex extitit probu sensu canus"
            },
            {
                "verse": "Angelus Raynerii, Magister Mactheus"
            },
            {
                "verse": "Magalottus, Angelus, et Bartholomeus"
            },
            {
                "verse": "Bernardinus, Iacobus, Petrus, et Mactheus"
            },
            {
                "verse": "Farulfus Notarius quem erexit Deus"
            },
            {
                "verse": "Et Rollandus Iacobi, Rollandus Certensis"
            },
            {
                "verse": "Iannes olim Stephani, Gilius ut ensis"
            },
            {
                "verse": "Benvenutus validus olim Montaltensis"
            },
            {
                "verse": "Et Petrus Letitie fidus Tuscanensis"
            },
            {
                "verse": "Pellegrinus Scambius et Bonaventura"
            },
            {
                "verse": "Ranutius, Gregorius cuius Dei cura"
            },
            {
                "verse": "Petrus Polli iuvenis cuius vita pura"
            },
            {
                "verse": "Quidam alter obiit vitam, cum secura"
            },
            {
                "verse": "Et Talentus providus, Petrus Faldubonus"
            },
            {
                "verse": "Iohannes Stanchalcerius fortis, recte bonus"
            },
            {
                "verse": "Iohannes Gualdaldrule cuius pius sonus"
            },
            {
                "verse": "Horum quisque pertulit inauditum honus"
            },
            {
                "verse": "Igitur, o populi quis iam non stupescit"
            },
            {
                "verse": "Quod Cornetum facinus tulit, et mutescit"
            },
            {
                "verse": "Quam ecclesiasticam fidem neque nescit"
            },
            {
                "verse": "Et spe regis gloriae ipsum non tabescit"
            },
            {
                "verse": "Post Vitalis reditum pariter suorum"
            },
            {
                "verse": "Quidam locum properant cito mortuorum"
            },
            {
                "verse": "Et caterva remanet sola sospensorum"
            },
            {
                "verse": "Et deponunt corpora subito sanctorum"
            },
            {
                "verse": "Festinanter deferunt corpora Cornetum"
            },
            {
                "verse": "Summa cum tristitia Populus ad fletum"
            },
            {
                "verse": "Motus est aequaliter neque dat quietum"
            },
            {
                "verse": "Sepulturam subeunt, et deponunt metum"
            },
            {
                "verse": "Terreant, o populi, ista quae auditis"
            },
            {
                "verse": "Dum tyranni rabiem per orbem sentitis"
            },
            {
                "verse": "Eidem resistite, qui est actor litis"
            },
            {
                "verse": "Liberi poteritis: esse si velitis"
            },
            {
                "verse": "Acta sunt haec omnia tempore beati"
            },
            {
                "verse": "Pape Innocentii quarti Christo dati"
            },
            {
                "verse": "Per quem gentes subdite dantur libertati"
            },
            {
                "verse": "Et Cornetum sudbitur Dei Magestati"
            }
        ]
    ]
}