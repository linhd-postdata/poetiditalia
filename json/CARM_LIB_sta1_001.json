{
    "author": "carmina libraria",
    "title": "Statius, opera",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|sta1|001",
    "text": [
        [
            {
                "verse": "Domitius ad lectorem"
            }
        ],
        [
            {
                "verse": "Sic mihi perpetuae contingant murmura laudis"
            },
            {
                "verse": "Et bona post funus hora superstes eat"
            },
            {
                "verse": "Ut nostris cupio multis prodesse libellos"
            },
            {
                "verse": "Famaque nonnullo tincta cruore placet"
            },
            {
                "verse": "Me legat invitus nemo, non scripsimus illi;"
            },
            {
                "verse": "Huic scripta est si quem pagina nostra iuvat."
            }
        ]
    ]
}