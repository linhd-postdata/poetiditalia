{
    "author": "carmina libraria",
    "title": "Augustinus, opuscula",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|aug4|001",
    "text": [
        [
            {
                "verse": "Nos iuvat arguta scrutari indagine verum."
            },
            {
                "verse": "Illos Dulichio verba lepore tenent."
            },
            {
                "verse": "Ast aliis gratum est varios versare libellos."
            },
            {
                "verse": "Sunt quoque scripta quibus non nisi sancta placent."
            },
            {
                "verse": "Quisquis es ex istis paucis nummis, eme multos."
            },
            {
                "verse": "Aurelii libros, hancque levato sitim."
            },
            {
                "verse": "Nervosos cernes rationum viribus, atque"
            },
            {
                "verse": "Donatos varios de deitate simul."
            },
            {
                "verse": "Hos menda expertes tulit Argentina primique"
            },
            {
                "verse": "Martinus docili fimus ab arte dedit."
            }
        ]
    ]
}