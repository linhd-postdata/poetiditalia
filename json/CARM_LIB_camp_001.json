{
    "author": "carmina libraria",
    "title": "Iohannes Antonius Campanus, opera",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|camp|001",
    "text": [
        [
            {
                "verse": "carmen"
            }
        ],
        [
            {
                "verse": "Nubila pellit"
            },
            {
                "verse": "Grandine foeta"
            },
            {
                "verse": "Aere sonoro"
            },
            {
                "verse": "Turribus altis"
            },
            {
                "verse": "Edita tandem"
            },
            {
                "verse": "Machina tanto"
            },
            {
                "verse": "Fulsa decore"
            },
            {
                "verse": "Gloria numquam"
            },
            {
                "verse": "Desinet axis"
            },
            {
                "verse": "Dum vehet orbem"
            },
            {
                "verse": "Libera tristi"
            },
            {
                "verse": "Facta timore"
            },
            {
                "verse": "Saecula vivens"
            },
            {
                "verse": "Cuncta loquetur."
            }
        ],
        [
            {
                "verse": "Pulsabat solida et toti notissima mundi"
            },
            {
                "verse": "Per coelum et terras fama vagata mea est."
            },
            {
                "verse": "Turbida tempestas rupit me sola Tyferni"
            },
            {
                "verse": "Nubila dum sonitu rumpere posse puto."
            },
            {
                "verse": "Me miserum fulvi rapiebant frustra metalli"
            },
            {
                "verse": "Et qui curaret reddere nemo fuit."
            },
            {
                "verse": "Sed me iterum Fernus vigili sudore Michael"
            },
            {
                "verse": "Iam tandem fudit prae pietate meus."
            },
            {
                "verse": "Sint gratiae Domino"
            }
        ],
        [],
        [
            {
                "verse": "Petri Sabini in nova Campani editionem decastichon"
            }
        ],
        [
            {
                "verse": "Perlege Campani numerosa volumina lector"
            },
            {
                "verse": "Non otiosas paginas;"
            },
            {
                "verse": "Nam proprii et pressi cultor sermonis ubique est"
            },
            {
                "verse": "Priscis cedere nescius."
            },
            {
                "verse": "Gratia magna tibi referenda est FERNE MICHAEL"
            },
            {
                "verse": "Quod ductu tuo prodeunt."
            },
            {
                "verse": "Sicut Maeonios versus Pisistratus olim"
            },
            {
                "verse": "Haec undecumque colligis."
            },
            {
                "verse": "Ergo ut durat adhuc tanti pia fama Tyranni"
            },
            {
                "verse": "Sic te posteritas colet."
            }
        ],
        [],
        [
            {
                "verse": "ad Sabinum"
            }
        ],
        [
            {
                "verse": "Hei mihi quam vana est spes haec et gloria famae"
            },
            {
                "verse": "Pro fama morimur docte Sabine fame."
            },
            {
                "verse": "Stet sua per totum virtutis gloria mundum"
            },
            {
                "verse": "Recte vive deo cetera fumus erunt."
            }
        ],
        [],
        [
            {
                "verse": "Michael"
            }
        ],
        [
            {
                "verse": "Rumpite si libeat iam nunc mea stamina parcae,"
            },
            {
                "verse": "Imperium vitae non habet ulla meae."
            },
            {
                "verse": "Vivere quod posthac properantia fata negarint"
            },
            {
                "verse": "Id mihi Campani iam monimenta dabunt."
            }
        ]
    ]
}