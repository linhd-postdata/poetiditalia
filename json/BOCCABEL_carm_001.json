{
    "author": "Emilio Boccabella",
    "title": "carmen Ludovico de Gonzaga",
    "url": "http://www.poetiditalia.it/texts/BOCCABEL|carm|001",
    "text": [
        [
            {
                "verse": "Campani salibus nihil est limatius: arctat"
            },
            {
                "verse": "Argutum brevibus sic epigrama iocis,"
            },
            {
                "verse": "Exprimit affectus cunctisque vocabula rebus"
            },
            {
                "verse": "Comodat, aculeos undique sempre agit."
            },
            {
                "verse": "Hos tu, ne tineis possent, Ludovice, perire,"
            },
            {
                "verse": "Membrana et minio nobile reddis opus."
            },
            {
                "verse": "Principibus placuit vivens Pauloque Pioque,"
            },
            {
                "verse": "Nec nihil a Sixto muneris auctor habet,"
            },
            {
                "verse": "Iactat et interdum sese Papiensis amicum;"
            },
            {
                "verse": "Horum etenim vates tempore primus erat,"
            },
            {
                "verse": "Quamvis Parca suos virides abruperit annos,"
            },
            {
                "verse": "Hospitio fratris viveret ille tui."
            },
            {
                "verse": "Viveret o utinam! Victuro carmine famam"
            },
            {
                "verse": "Gonzagae tecum tolleret usque domus."
            },
            {
                "verse": "Sed, quando Elisiis nemo revocatur ab umbris,"
            },
            {
                "verse": "Te dignum est libro consuluisse suo."
            },
            {
                "verse": "Campanus felix igitur virtutis honore"
            },
            {
                "verse": "Clarus erit per te, vivus ut ante fuit."
            }
        ]
    ]
}