{
    "author": "Francesco Filelfo",
    "title": "epigramma",
    "url": "http://www.poetiditalia.it/texts/FILELFO|epig|001",
    "text": [
        [
            {
                "verse": "Philelfus Domitio Veronensi"
            }
        ],
        [
            {
                "verse": "Calderine, tuum si nondum pectore vatem"
            },
            {
                "verse": "Eicis, ut properes se petiisse rogat;"
            },
            {
                "verse": "Non enim turpe est iuveni observare senectam,"
            },
            {
                "verse": "Sit licet ingenio clarus et eloquio."
            },
            {
                "verse": "Huc igitur venias veterem visurus amicum,"
            },
            {
                "verse": "Aut hunc, si malis, opperiare domi."
            }
        ]
    ]
}