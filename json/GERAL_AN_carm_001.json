            {
                "verse": "Coniugem tristi revocavit Orco,"
            },
            {
                "verse": "Saeva placavit numeris sonoris"
            },
            {
                "verse": "Pectora Ditis."
            }
        ],
        [
            {
                "verse": "O fax Insubriae micans amoenae,"
            },
            {
                "verse": "Ingens Castalidum decus sororum,"
            },
            {
                "verse": "Antoni, lege maxime rogamus,"
