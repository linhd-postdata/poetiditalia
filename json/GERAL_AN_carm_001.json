{
    "author": "Antonio Geraldini",
    "title": "carmina",
    "url": "http://www.poetiditalia.it/texts/GERAL_AN|carm|001",
    "text": [
        [
            {
                "verse": "Antonii Geraldini Amerini liber carminum ad magnificumPetrum Medicem Florentinum optimatem de RepublicaFlorentina bene meritum"
            }
        ],
        [
            {
                "verse": "Ipse grandaevum Cytherea proles"
            },
            {
                "verse": "Portat Aeneas humeris parentem"
            },
            {
                "verse": "Inter ardentis Phrygiae ruinas"
            },
            {
                "verse": "Graia per arma."
            },
            {
                "verse": "Lassus hunc templo Cereris vetusto"
            },
            {
                "verse": "Ilium incensum repetens reliquit"
            },
            {
                "verse": "Inter armatas Danaum catervas"
            },
            {
                "verse": "Inter et enses."
            },
            {
                "verse": "Hostibus Teucros rapuit penates,"
            },
            {
                "verse": "Vexit et Troiae piceis carinis"
            },
            {
                "verse": "Sacra per Scyllae rabiem voracis"
            },
            {
                "verse": "Perque Charybdin."
            },
            {
                "verse": "Haec tamen postquam solio receptus"
            },
            {
                "verse": "Victor Aeneas fuit in Latini,"
            },
            {
                "verse": "Coniugis pulchro recubans venustae"
            },
            {
                "verse": "Pectore liquit."
            },
            {
                "verse": "Urbis at lumen Medices Etruscae"
            },
            {
                "verse": "Petre, qui caros acie penates"
            },
            {
                "verse": "Lyncea dulcis patriae tueris"
            },
            {
                "verse": "Hostis ab ense,"
            },
            {
                "verse": "Tu geris Tuscam vigilans parentem"
            },
            {
                "verse": "Mente quam nulla pateris quiete"
            },
            {
                "verse": "Consulens rebus patriae teneri,"
            },
            {
                "verse": "O pia proles."
            },
            {
                "verse": "Hanc foves ulnis placidus benignis,"
            },
            {
                "verse": "Semper hanc spectas oculis apertis,"
            },
            {
                "verse": "Ne quis insultu veniens iniquo"
            },
            {
                "verse": "Vulneret hostis."
            },
            {
                "verse": "Tuta iam Cosmi meritis supremis"
            },
            {
                "verse": "Illa nunc gratis humeris recumbit"
            },
            {
                "verse": "Filii, et multis meritis superba"
            },
            {
                "verse": "Fulget in orbe."
            },
            {
                "verse": "Gesta tu celsi sequeris parentis,"
            },
            {
                "verse": "Petre, dum nullum renuis laborem,"
            },
            {
                "verse": "Ut pares longam patriae salutem"
            },
            {
                "verse": "Atque triumphos."
            },
            {
                "verse": "Suevit ut Cosmus pater ille magnus,"
            },
            {
                "verse": "Indiae notus populis, Iberis"
            },
            {
                "verse": "Accolis cultus Scythiae et madentis"
            },
            {
                "verse": "Gentibus Austri,"
            },
            {
                "verse": "Artium priscos revocas honores"
            },
            {
                "verse": "Et faves divae studiis Minervae,"
            },
            {
                "verse": "Praemiis doctas renovas Athenas"
            },
            {
                "verse": "Litore Etrusco."
            },
            {
                "verse": "Audit orator fluvium sonantem;"
            },
            {
                "verse": "Arnus at sentit celebres poetas,"
            },
            {
                "verse": "Tuscus ut vates Tiberis canentes"
            },
            {
                "verse": "Audiit olim."
            },
            {
                "verse": "Sume quod ludens hilaris iuventa"
            },
            {
                "verse": "Numini fausto Medicis dicavi,"
            },
            {
                "verse": "Thraciae chordas citharae sonoras"
            },
            {
                "verse": "Pollice pulsans."
            },
            {
                "verse": "Pignus hoc nostri minimum laboris"
            },
            {
                "verse": "Accipe, et vultu relegens amico"
            },
            {
                "verse": "Mitis afflatu faveas benigni"
            },
            {
                "verse": "Sideris instar."
            },
            {
                "verse": "Versibus clemens Iovis aura nostris,"
            },
            {
                "verse": "Delii sidus placidum faventis,"
            },
            {
                "verse": "Lene sis astrum Paphiae serenae"
            },
            {
                "verse": "Mercuriique."
            }
        ],
        [
            {
                "verse": "Aeterna o genitrix regis Olympici,"
            },
            {
                "verse": "Quem tu virgineo pectore concipis,"
            },
            {
                "verse": "Tunc cum dixit \"ave\" nuntius angelus,"
            },
            {
                "verse": "Virgo, es praecipuum numen Etruriae,"
            },
            {
                "verse": "Hic cunctis celebris gentibus Italis,"
            },
            {
                "verse": "Hic felix et ades gentibus exteris,"
            },
            {
                "verse": "Concedisque pia voce petentibus"
            },
            {
                "verse": "Optatum auxilium siderea manu."
            },
            {
                "verse": "Tyrrhenum decoras lampade lucida"
            },
            {
                "verse": "Hac virgo populum culta sub aede; nunc"
            },
            {
                "verse": "Hic maior tripodis numine Delphici"
            },
            {
                "verse": "Ara est, cornigeri nobilior dei"
            },
            {
                "verse": "Haec fonte est Libyco, denique quercubus"
            },
            {
                "verse": "Dodonaeque sacris notior omnibus."
            },
            {
                "verse": "Templis ditior haec est Epidauriis"
            },
            {
                "verse": "Delubris Iovis et maior Olympii."
            },
            {
                "verse": "Exustos aliquos febribus aridis"
            },
            {
                "verse": "Tu sanas manibus fausta salubribus,"
            },
            {
                "verse": "Aestus abiiciens artubus igneos"
            },
            {
                "verse": "Aetnaeis nimium persimiles iugis."
            },
            {
                "verse": "Morborumque abigis pestiferam luem,"
            },
            {
                "verse": "Si quando incubuit pectoribus virum"
            },
            {
                "verse": "Vel contagio vel temporis impii"
            },
            {
                "verse": "Execranda sitim vis rabidam ciens,"
            },
            {
                "verse": "Fibras luctificis ignibus ambiens."
            },
            {
                "verse": "Contractos rigidis articulis foves,"
            },
            {
                "verse": "Distendens solitum membra per ordinem,"
            },
            {
                "verse": "Claudis restituis officium pedum,"
            },
            {
                "verse": "Ut rectis hilares passibus ambulent,"
            },
            {
                "verse": "Et caecis tenebras luminibus fugas."
            },
            {
                "verse": "Obtusos aliis diva aperis favens"
            },
            {
                "verse": "Sensus, ut patulis auribus audiant."
            },
            {
                "verse": "Clausis eloquium fundis ab oribus"
            },
            {
                "verse": "Caelesti nimium nectare dulcius,"
            },
            {
                "verse": "Et quod melle magis tangit Hymettio."
            },
            {
                "verse": "Horum lingua potest findere marmora,"
            },
            {
                "verse": "Plumbis visa mihi obtusior omnibus."
            },
            {
                "verse": "Te clamat miseris navita vocibus,"
            },
            {
                "verse": "Si vidit variis fluctibus aequora"
            },
            {
                "verse": "Spumosisque vadis cana tumescere,"
            },
            {
                "verse": "Convolvique nigris aestibus omnia,"
            },
            {
                "verse": "Visis flaminibus naufragium timens;"
            },
            {
                "verse": "Auscultasque virum vota precantium,"
            },
            {
                "verse": "Sive hi vulneribus letiferis iacent"
            },
            {
                "verse": "Perfossi galeis non capita abdita"
            },
            {
                "verse": "Securis, vel habent pectora saucia"
            },
            {
                "verse": "Non sananda manu sive Melampica"
            },
            {
                "verse": "Chironis vel ope, aut arte Machaonis."
            },
            {
                "verse": "Audis diva preces pectore supplici"
            },
            {
                "verse": "Effusas hominum extrema videntium et"
            },
            {
                "verse": "Mortis supplicium, nocte natantibus"
            },
            {
                "verse": "Iam iam luminibus luce sub ultima."
            },
            {
                "verse": "Haec miranda facis, virgo perinclyta,"
            },
            {
                "verse": "Quae Lesboa nequit iungere barbitos"
            },
            {
                "verse": "Nervo nunc tenui versibus arduis."
            },
            {
                "verse": "Terras frugiferas, velivolum mare,"
            },
            {
                "verse": "Caelum nubiferum, claraque sidera"
            },
            {
                "verse": "Clemens innumeris tu meritis reples."
            },
            {
                "verse": "Sincerum validis me, dea, viribus"
            },
            {
                "verse": "Conserva, et solidus sit vigor artuum"
            },
            {
                "verse": "Totius generi stelligerae domus."
            },
            {
                "verse": "Sensus aetherii mentibus haereant"
            },
            {
                "verse": "Et voces placidis moribus insonent,"
            },
            {
                "verse": "Quae cunctis placeant gentibus et deo."
            }
        ],
        [
            {
                "verse": "Angeli mira nitidi salute"
            },
            {
                "verse": "Fertilis mater subito momento"
            },
            {
                "verse": "Mixta non ulli generi vitorum"
            },
            {
                "verse": "Concipis alma."
            },
            {
                "verse": "Spiritus sacris uteri latebris"
            },
            {
                "verse": "Semen infundens hominum salutis"
            },
            {
                "verse": "Pignus aeternum tenero reliquit"
            },
            {
                "verse": "Virginis alvo."
            },
            {
                "verse": "Hinc et haud altis thalamis recumbens,"
            },
            {
                "verse": "Nixa nec spondae tabulis eburnae"
            },
            {
                "verse": "Nec toris pictis Tyriove tinctae"
            },
            {
                "verse": "Murice lanae,"
            },
            {
                "verse": "Sed super faenum paris alma prolem,"
            },
            {
                "verse": "Quae, ne in obscurum rueremus Orcum,"
            },
            {
                "verse": "Caede crudeli, proprio redemit"
            },
            {
                "verse": "Sanguine vitam."
            },
            {
                "verse": "Inde complexum niveis lacertis"
            },
            {
                "verse": "Tu foves natum gremio tepenti,"
            },
            {
                "verse": "Lacte caelesti simul et fluenti"
            },
            {
                "verse": "Nectare pascens."
            },
            {
                "verse": "Hos tuo servus modulos fidelis"
            },
            {
                "verse": "Numini adscribit, genitrix, sacrato;"
            },
            {
                "verse": "Hoc pia munus capias rogamus"
            },
            {
                "verse": "Mente profusum."
            },
            {
                "verse": "Non mihi docti statuae Myronis,"
            },
            {
                "verse": "Mentoris non sunt operosa signa"
            },
            {
                "verse": "Ulla, Lysippi tabulae aut Apellis,"
            },
            {
                "verse": "Nec Polycleti."
            },
            {
                "verse": "Non meis fulgent loculis lapilli,"
            },
            {
                "verse": "Quos maris splendens tulit unda rubri,"
            },
            {
                "verse": "Non lapis nostra Parius sub arca"
            },
            {
                "verse": "Clauditur ullus."
            },
            {
                "verse": "Multa non auri resident talenta,"
            },
            {
                "verse": "Non ebur nobis niveum Syene"
            },
            {
                "verse": "Misit evulsum ex elephantis ore"
            },
            {
                "verse": "Dentibus albis."
            },
            {
                "verse": "Divites fulvos tripodas ferentes"
            },
            {
                "verse": "Dent novos arae calices calenti,"
            },
            {
                "verse": "Lucidis gemmis pateras rigentes"
            },
            {
                "verse": "Pondere multo."
            },
            {
                "verse": "Aureos pelves teretesque lychnos"
            },
            {
                "verse": "Tecta suspendant laqueata circum,"
            },
            {
                "verse": "Vasa dent reges rigidasque fulvo"
            },
            {
                "verse": "Lampadas auro."
            },
            {
                "verse": "Offerant arae varios tapetas,"
            },
            {
                "verse": "Sericas vestes, Tyrii coloris"
            },
            {
                "verse": "Purpuram multam, variata pictis"
            },
            {
                "verse": "Gausapa filis,"
            },
            {
                "verse": "Arma victores foribus reponant,"
            },
            {
                "verse": "Cereis aras onerent figuris,"
            },
            {
                "verse": "Fumidas dites repleant acerras"
            },
            {
                "verse": "Ture Sabaeo."
            }
        ],
        [
            {
                "verse": "Rugas pone tua fronte, severior"
            },
            {
                "verse": "Marsili, atque libros pone Platonicos,"
            },
            {
                "verse": "Quis nunc invigilans sic tibi comparas"
            },
            {
                "verse": "Nomen perpetuum sagax."
            },
            {
                "verse": "Non hic sidereos caelicolum polos,"
            },
            {
                "verse": "Non tractus rutilos aetheris ignei,"
            },
            {
                "verse": "Non hic multivagum perlegis aera,"
            },
            {
                "verse": "Nimborum et volucrum plagam,"
            },
            {
                "verse": "Non aequor tumidis fluctibus imminens"
            },
            {
                "verse": "Terrarum exitio, rursus ut alveo,"
            },
            {
                "verse": "Quae fundit gravidis nubibus, accipit"
            },
            {
                "verse": "Late prodiga flumina."
            },
            {
                "verse": "Non hic omniferas perlegis undique"
            },
            {
                "verse": "Terras assiduo vomere saucias,"
            },
            {
                "verse": "Non mores hominum clara videntium."
            },
            {
                "verse": "Recto sidera vertice,"
            },
            {
                "verse": "Sed quae nunc lyrico pectine ludimus,"
            },
            {
                "verse": "Ne tempus celeri diffugiat rota"
            },
            {
                "verse": "Sic immune operum, mitis Apollinis"
            },
            {
                "verse": "Laurus despiciam ut sacras."
            },
            {
                "verse": "Non hic Pindaricos aspicies lacus,"
            },
            {
                "verse": "Sed rivum tenuem gurgitis aridi."
            },
            {
                "verse": "Non sentis numeros Orphea nectere"
            },
            {
                "verse": "Maternos citharae modis."
            }
        ],
        [
            {
                "verse": "Maxima, Gine, tuo cepi solacia rure"
            },
            {
                "verse": "Urbanis fessus prope rebus."
            },
            {
                "verse": "Sepositis paulum laxavi pectora curis"
            },
            {
                "verse": "Atque quies menti brevis haesit."
            },
            {
                "verse": "Lenia iucundos tribuerunt otia lusus,"
            },
            {
                "verse": "Estque hilaris lepor auribus haustus,"
            },
            {
                "verse": "Atque sales placidi tremulos movere cachinnos,"
            },
            {
                "verse": "Lusit lautae copia mensae;"
            },
            {
                "verse": "Civilesque fuere dapes et agrestia dona"
            },
            {
                "verse": "Pomaque ramis lecta sub altis."
            },
            {
                "verse": "Inculti agricolae versus cecinere protervos,"
            },
            {
                "verse": "Corpora lassavit pila iacta;"
            },
            {
                "verse": "Robustaque gravem discum promittere dextra,"
            },
            {
                "verse": "Plebi cura est grata torosae."
            },
            {
                "verse": "Cura fuit capreos cursu superare fugaces,"
            },
            {
                "verse": "Saltu haud tardos vincere cervos."
            },
            {
                "verse": "Ipse modo in viridi lentus tellure iacebam"
            },
            {
                "verse": "Excipiens patulae arboris umbram,"
            },
            {
                "verse": "Et modo captabam silvas et olentia prata"
            },
            {
                "verse": "Et leve murmur aquae salientis."
            },
            {
                "verse": "Affuit hac Genius Charitesque in luce faventes"
            },
            {
                "verse": "Nympharumque chori dryadesque."
            },
            {
                "verse": "Capripedes Fauni, Panes, Satyrique procaces"
            },
            {
                "verse": "Lydis coetus saltibus addunt;"
            },
            {
                "verse": "Arnigenaeque deae laetas duxere choreas,"
            },
            {
                "verse": "Naias hic coniuncta Napaeae."
            },
            {
                "verse": "Alterna valles nympharum voce resultant"
            },
            {
                "verse": "Et rupes resonant latebrosae."
            },
            {
                "verse": "Haec sine nube dies niveis numeranda lapillis"
            },
            {
                "verse": "Munere, Gine, tuo alba cucurrit."
            },
            {
                "verse": "Cras Florentinam referes te Ginus in urbem,"
            },
            {
                "verse": "Impedietque pedes toga longa."
            },
            {
                "verse": "Hic privata tenet pariter te et publica cura,"
            },
            {
                "verse": "Gentis gloria magna Caponae."
            },
            {
                "verse": "Hic Nerius genitor nomen laudemque paravit"
            },
            {
                "verse": "Pro patria subiens onus omne."
            },
            {
                "verse": "Hanc propter duxit per densa pericula gressus"
            },
            {
                "verse": "Intrepidum gladiis caput obdens."
            },
            {
                "verse": "Ipse toga eminuit patriae inter moenia multis"
            },
            {
                "verse": "Insignis meritis bonus heros."
            },
            {
                "verse": "Saepe suae patriae varios conversus in hostes"
            },
            {
                "verse": "Tuscas duxit in arma cohortes."
            },
            {
                "verse": "Rettulit ipse etiam vicino ex hoste triumphos,"
            },
            {
                "verse": "Civis et imperium auxit Etruscum."
            }
        ],
        [
            {
                "verse": "Luteo stratis roseis amictu"
            },
            {
                "verse": "Arce Tithonus residens Eoa"
            },
            {
                "verse": "Vidit uxorem rutilas agentem"
            },
            {
                "verse": "Voce quadrigas."
            },
            {
                "verse": "Tum senex divam Phrygius nitentem"
            },
            {
                "verse": "Strinxit infirmis mediam lacertis,"
            },
            {
                "verse": "Eius infigens teneris labellis"
            },
            {
                "verse": "Oscula mille."
            },
            {
                "verse": "Lucifer stellis radians fugatis"
            },
            {
                "verse": "Nuntius genti rediit caducae,"
            },
            {
                "verse": "Mane praedicens croceum recenti"
            },
            {
                "verse": "Praevius ortum."
            },
            {
                "verse": "Iam levis summas movet aura frondes"
            },
            {
                "verse": "Arborum, ponto veniens ab alto,"
            },
            {
                "verse": "Et quatit florum tremulum cacumen"
            },
            {
                "verse": "Omnia lustrans."
            },
            {
                "verse": "Ales et rubra niger ipse crista,"
            },
            {
                "verse": "Excitans somnis homines sepultos"
            },
            {
                "verse": "Dulcibus lucis cecinit propinquae"
            },
            {
                "verse": "Versibus ortum."
            },
            {
                "verse": "Vocibus silvae querulis virentes"
            },
            {
                "verse": "Omnium late volucrum resultant,"
            },
            {
                "verse": "Et cavae valles avium canora"
            },
            {
                "verse": "Carmina reddunt."
            },
            {
                "verse": "Corpus e pictis thalamis quietum"
            },
            {
                "verse": "Iam vigil tollas, iubeo, poeta,"
            },
            {
                "verse": "Et domo pellens tenebras inertis"
            },
            {
                "verse": "Lumina funde."
            },
            {
                "verse": "Hic velim sumas Phariam papyrum,"
            },
            {
                "verse": "Sive membranae bicoloris orbem,"
            },
            {
                "verse": "Sepiam nigram calamosque leves"
            },
            {
                "verse": "Posce lyramque."
            },
            {
                "verse": "Mox canas sollers fidibus Latinis,"
            },
            {
                "verse": "Quos heri dulces modulos canentes"
            },
            {
                "verse": "Legimus, quondam quibus usa fertur"
            },
            {
                "verse": "Lesbia Sappho."
            },
            {
                "verse": "Sic tuis gratus venies amicis,"
            },
            {
                "verse": "Gloriam cautus celebrem parabis;"
            },
            {
                "verse": "Sic eris nostro memorandus aevo,"
            },
            {
                "verse": "Clarus ubique."
            },
            {
                "verse": "Sic nigras mortis tenebras opacae,"
            },
            {
                "verse": "Sic ratem cani fugies Charonis,"
            },
            {
                "verse": "Pallidos umbras, Stygiam paludem,"
