{
    "author": "carmina libraria",
    "title": "Nicolaus de Orbellis, expositio",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|orbe|001",
    "text": [
        [
            {
                "verse": "eiusdem Lodovici Honnonii epigramma"
            }
        ],
        [
            {
                "verse": "Cuncta licet nostrae sint consona scripta salutis"
            },
            {
                "verse": "(Namque aliud verae religionis habent)"
            },
            {
                "verse": "Te tamen e multis divinae dogmata legis"
            },
            {
                "verse": "Sollicitent, recta si cupis ire via."
            },
            {
                "verse": "Anseris exta bidens, siliquas quid devorat atras"
            },
            {
                "verse": "Despicit archadico stutior ille viro"
            },
            {
                "verse": "Ingeniosa petas subtilis dogmata Scoti"
            },
            {
                "verse": "Si vis ethereae noscere rena domus."
            },
            {
                "verse": "Nemo quidem tetigit propiore indagine caelum"
            },
            {
                "verse": "Credo quod humanum vicerit ingenium."
            },
            {
                "verse": "Nullus honorati dubiosa aenigmata patris"
            },
            {
                "verse": "Audaci valuit concelebrare sono."
            },
            {
                "verse": "Consuluit tandem nostris orbellicus annis"
            },
            {
                "verse": "Dedipo melius singula verba notans"
            },
            {
                "verse": "Dignus Scote tui cui tantum luminis esset,"
            },
            {
                "verse": "Cui par ingenium religioque fuit."
            },
            {
                "verse": "Invia quae fuerant multisque incognita lustris"
            },
            {
                "verse": "Aestivo fiunt candiora Die."
            },
            {
                "verse": "Hoc duce carpat iter quisquis sitientia morum"
            },
            {
                "verse": "E mediis cupiat labia rigare vadis."
            }
        ]
    ]
}