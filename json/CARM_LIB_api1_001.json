{
    "author": "carmina libraria",
    "title": "Apicius, in re coquinaria",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|api1|001",
    "text": [
        [
            {
                "verse": "Ad lectorem Antonii Mottae carmen"
            }
        ],
        [
            {
                "verse": "Si vis scire cibos regum condita ducumque"
            },
            {
                "verse": "Fercula, vel quanti sit facienda gula;"
            },
            {
                "verse": "Quid pleno pariter ventri, quid prosit inani"
            },
            {
                "verse": "Aut indignato quid facit ad stomachum."
            },
            {
                "verse": "Hunc nocturna manus veteresque diurna libellum"
            },
            {
                "verse": "Nec studio pigeat invigilare novo."
            },
            {
                "verse": "Prodigus his quamvis AYXH vel Apicius omnem"
            },
            {
                "verse": "Rem tamen invenies rodere quid valeas."
            }
        ],
        [],
        [
            {
                "verse": "Bernardini Molli tetrastichon ad genitorem"
            }
        ],
        [
            {
                "verse": "Prodiga Martigenae fuit olim gloria mensae"
            },
            {
                "verse": "Quod meus inscribit nunc tibi doctor opus;"
            },
            {
                "verse": "Accipe sed laeto quae dantur munera vultu;"
            },
            {
                "verse": "Huic fuerit virtus littera quanta docet."
            }
        ]
    ]
}