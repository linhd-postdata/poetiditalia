{
    "author": "Lelio Bonzi",
    "title": "carmen ad Varchium",
    "url": "http://www.poetiditalia.it/texts/BONZI|carm|001",
    "text": [
        [
            {
                "verse": "ad Benedictum Varchium responsio"
            }
        ],
        [
            {
                "verse": "Accepi, legique libens, iterumque relegi,"
            },
            {
                "verse": "Ac rursus, et rursus legam"
            },
            {
                "verse": "Quae mihi misisti pulcherrima carmina; queis me,"
            },
            {
                "verse": "Prudenter ac vere mones"
            },
            {
                "verse": "Ad studia ut legum redeam, musasque relinquam"
            },
            {
                "verse": "Nostris diebus futiles,"
            },
            {
                "verse": "Quod facere est animus quamprimum corpora languor"
            },
            {
                "verse": "Et pallor ora liquerit,"
            },
            {
                "verse": "Undenis qui me iam mensibus occupat atque"
            },
            {
                "verse": "Vacare seriis vetat."
            },
            {
                "verse": "Interea quid agam potius, doctissime Varchi,"
            },
            {
                "verse": "Minusque culpae obnoxium,"
            },
            {
                "verse": "Non ipse interdum historiam causasque latenteis"
            },
            {
                "verse": "Mutare versibus soles?"
            },
            {
                "verse": "Scis quam multa mihi, quam magna pericla minentur,"
            },
            {
                "verse": "Ni cuncta prudens cavero,"
            },
            {
                "verse": "Qui me migrantis toties de faucibus Orci"
            },
            {
                "verse": "Traxere vitae ad lumina,"
            },
            {
                "verse": "Tuque adeo in primis grata quem mente paterno"
            },
            {
                "verse": "Honore iam diu colo,"
            },
            {
                "verse": "Nec tantum efficio quantum vel tute mireris"
            },
            {
                "verse": "Vel ipse certe debeo."
            },
            {
                "verse": "Sed ne forte puter rebus quod vix datur ipsis,"
            },
            {
                "verse": "Quod velle versibus solvere"
            },
            {
                "verse": "Nil addam ulterius, nisi quod tua dicta tenebo"
            },
            {
                "verse": "Infixa semper pectore."
            }
        ]
    ]
}