{
    "author": "Tranquillo Gravina",
    "title": "phalaecium",
    "url": "http://www.poetiditalia.it/texts/GRAVINA_T|phal|001",
    "text": [
        [
            {
                "verse": "Felix, o nimium Licinna felix,"
            },
            {
                "verse": "Quae tam dulciloquo places poetae,"
            },
            {
                "verse": "Felix ora uirum usque peruolabis."
            },
            {
                "verse": "Te legent iuuenes puellulaeque,"
            },
            {
                "verse": "Siqua est Romuleae perita linguae,"
            },
            {
                "verse": "Spartanaeque tibi inuident sorores."
            },
            {
                "verse": "Aequo non animo ferunt relictae,"
            },
            {
                "verse": "Suo quod lepido places alumno,"
            },
            {
                "verse": "Solam quod lepidus canat poeta."
            },
            {
                "verse": "Tuque, o Callimacho tuo propinquo"
            },
            {
                "verse": "Aequande ingenio et tuo Philetae"
            },
            {
                "verse": "(Nam tu sanguine natus es Pelasgo)"
            },
            {
                "verse": "Manili, Latiae decus Camenae,"
            },
            {
                "verse": "Manili, nimio lepore uates,"
            },
            {
                "verse": "Ne cessa teneros tuos amores"
            },
            {
                "verse": "Ad caelum facili uocare cantu."
            }
        ]
    ]
}