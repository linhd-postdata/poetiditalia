{
    "author": "Coluccio Salutati",
    "title": "de fato et fortuna",
    "url": "http://www.poetiditalia.it/texts/SALUTATI|fato|001",
    "text": [
        [],
        [
            {
                "verse": "Si sic esset -ut divinissimus Dantes compatriota noster ait-"
            },
            {
                "verse": "Foret omnis libera nobis"
            },
            {
                "verse": "Destructa arbitrii, celo cogente, facultas,"
            },
            {
                "verse": "Nec vera iustum posset ratione vocari"
            },
            {
                "verse": "Gaudia pro meritis et habere ex crimine luctum."
            }
        ]
    ]
}