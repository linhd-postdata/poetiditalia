{
    "author": "carmina libraria",
    "title": "Iliadum opus per Pindarum Thebanum e graeco in latinum traductum",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|ilia|001",
    "text": [
        [
            {
                "verse": "Quisquis in nostro faciles amores"
            },
            {
                "verse": "Quos Venus regina Cnidi coercet"
            },
            {
                "verse": "Sive ludorum varias figuras"
            },
            {
                "verse": "Carmine poscis,"
            },
            {
                "verse": "Falleris: luctus celebrare Clio"
            },
            {
                "verse": "Tentat, et tristes animas virorum"
            },
            {
                "verse": "Fortium, qui res hominum ac deorum"
            },
            {
                "verse": "Bella subibant."
            },
            {
                "verse": "Sed tibi virtus referet superbos"
            },
            {
                "verse": "Quos iuvat mortem Priamo innovare"
            },
            {
                "verse": "Fortia et tristi superare Troiae"
            },
            {
                "verse": "Corpora morte."
            },
            {
                "verse": "Quin et Europam et Asiam refuso"
            },
            {
                "verse": "Commovent Graeci Oceano feroces"
            },
            {
                "verse": "Atque iactatam religarunt udo"
            },
            {
                "verse": "Littore classes."
            },
            {
                "verse": "Haec canam et magnos referam triumphos"
            },
            {
                "verse": "Quosque virtutis meritum sub alta"
            },
            {
                "verse": "Sidera extollat, varios domando"
            },
            {
                "verse": "Corpore vires."
            }
        ]
    ]
}