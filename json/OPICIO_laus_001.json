{
    "author": "Giovanni Opicio",
    "title": "pro secessu regis",
    "url": "http://www.poetiditalia.it/texts/OPICIO|laus|001",
    "text": [
        [
            {
                "verse": "Christe, terrarum fabricator ingens"
            },
            {
                "verse": "Orbis, humani et generis, timent quem"
            },
            {
                "verse": "Cuncta, qui necnon homines trisulco"
            },
            {
                "verse": "Fulmine terres;"
            },
            {
                "verse": "Quas tibi laudes meritis canemus,"
            },
            {
                "verse": "Quae modo prestas, Pater alme, nobis,"
            },
            {
                "verse": "Possumus quales precibusque nostris"
            },
            {
                "verse": "Reddere grates."
            },
            {
                "verse": "Iam tuo Henricus dominus Britannum"
            },
            {
                "verse": "Septimus nutu superans catervas"
            },
            {
                "verse": "Hostium evasit, decus orbis ingens,"
            },
            {
                "verse": "Undique victor."
            },
            {
                "verse": "O deus, serves decus hoc, precamur,"
            },
            {
                "verse": "Huicque consortem; foveasque matrem"
            },
            {
                "verse": "Filios una, colit orbis en quos"
            },
            {
                "verse": "Axis uterque."
            }
        ]
    ]
}