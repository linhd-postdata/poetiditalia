{
    "author": "Giovan Battista Ramusio",
    "title": "epigramma",
    "url": "http://www.poetiditalia.it/texts/RAMUSIO|epig|001",
    "text": [
        [
            {
                "verse": "Siquis sensus inest, siqua est mens ulla sepultis,"
            },
            {
                "verse": "Pomponi pietas haec tua grata satis."
            },
            {
                "verse": "Qui sparsos cineres proprio componis acervo,"
            },
            {
                "verse": "Qui titulum proprio restituis tumulo."
            },
            {
                "verse": "Ipse tuo tandem Gallus nunc munere gaudet"
            },
            {
                "verse": "Se penitus dextra non cecidisse sua."
            }
        ]
    ]
}