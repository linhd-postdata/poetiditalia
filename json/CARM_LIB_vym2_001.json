{
    "author": "carmina libraria",
    "title": "Iacobus Vympfelingus et Robertus Gaguinus, disceptatio (epistolae et carmina)",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|vym2|001",
    "text": [
        [
            {
                "verse": "carmen sapphicum Iacobi Wimphelingi Sletstarensis ad Robertum Gauginum Francorum regis oratorem"
            }
        ],
        [
            {
                "verse": "Regis ex raptu tetrico procacis"
            },
            {
                "verse": "Qui dolo sedat thalamos pudicos"
            },
            {
                "verse": "Quae prius gratos dederant odores"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Filiam nuper aquilae potentis"
            },
            {
                "verse": "Gallus elegit, rapiens amavit"
            },
            {
                "verse": "Compotem regni cupiens futuram"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Corde nutanti posuit pudorem"
            },
            {
                "verse": "Immemor iusti, fugiens honestum"
            },
            {
                "verse": "Negligit famae titulos vetustae,"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Integro lustro placuit puella"
            },
            {
                "verse": "Et simul vita fruitur suavi"
            },
            {
                "verse": "Sperat et tedas sibi praeferendas,"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Ut decet sponsam fovet, osculatur"
            },
            {
                "verse": "Assidet stringit niveis lacertis;"
            },
            {
                "verse": "Dat manus, ridet recreatque moestum;"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Coniugis totum studet exhibere"
            },
            {
                "verse": "Munus, at frustra silicem quis optet"
            },
            {
                "verse": "Ut nivem aut ceram fluidam resolvi;"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Mox amor Galli bonus et pudicus"
            },
            {
                "verse": "Carneo victa vitioque virtus"
            },
            {
                "verse": "Victa languescunt, cecidere lugent,"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Sponsa quae cordi fuit ac placebat"
            },
            {
                "verse": "Discplicet coniunx, soceri placet nunc."
            },
            {
                "verse": "Magne cur princeps adeo vacillas?"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Sic mihi causam subitae repulsae"
            },
            {
                "verse": "Cur tuam spernis aliena prefers?"
            },
            {
                "verse": "Sic volo dices satis est potesse"
            },
            {
                "verse": "Lilia marcent."
            },
            {
                "verse": "Sic ubi regis decor aut honestas?"
            },
            {
                "verse": "Sic ubi verum pudor et cor altum?"
            },
            {
                "verse": "Sic ubi virtus animusque costans?"
            },
            {
                "verse": "Lilia marcent."
            }
        ]
    ]
}