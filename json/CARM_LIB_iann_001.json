{
    "author": "carmina libraria",
    "title": "Manettus Iannotius, de dignitate et excellentia hominis libri IV",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|iann|001",
    "text": [
        [
            {
                "verse": "hendecasyllabi, liber de se"
            }
        ],
        [
            {
                "verse": "Parvus sum liber et satis tenellus"
            },
            {
                "verse": "Sed cunctis numeris pereruditus."
            },
            {
                "verse": "Ergo praesidio viri elegantis"
            },
            {
                "verse": "Certe et praesidio viri potentis"
            },
            {
                "verse": "In lucem est opus exeam, secundo"
            },
            {
                "verse": "Phoebi nomine dexterisque Musis."
            },
            {
                "verse": "At me Nicoleos meus patronus,"
            },
            {
                "verse": "Immo Nicoleos pater benignus"
            },
            {
                "verse": "Defendet, leget omnibus peritis"
            },
            {
                "verse": "Scriptorem semel adprobabit illum."
            },
            {
                "verse": "Quod si livor edax simulque Momus"
            },
            {
                "verse": "Nigro forte sale velit nocere,"
            },
            {
                "verse": "Ille est Nicoleos vocatus ille"
            },
            {
                "verse": "Fatali arbitrio puto et salute"
            },
            {
                "verse": "Qui vulgus facile invidentiorum"
            },
            {
                "verse": "Possit fundere et omnibus malignos"
            },
            {
                "verse": "Castris exuere: brevi paratur"
            },
            {
                "verse": "Si causa valeas bonus triumphus."
            },
            {
                "verse": "Ergo fronte abeo semel serena"
            },
            {
                "verse": "Parvus sim licet, at pereruditus."
            }
        ]
    ]
}