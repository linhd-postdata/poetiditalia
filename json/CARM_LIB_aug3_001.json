{
    "author": "carmina libraria",
    "title": "Augustinus, opuscula",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|aug3|001",
    "text": [
        [
            {
                "verse": "Quid prodest homini coeli lustrasse meatus?"
            },
            {
                "verse": "Arctoine poli sydera nosse iuvat?"
            },
            {
                "verse": "Ut Graiis helice Tyrius cynosura petatur"
            },
            {
                "verse": "Dux maris Aegyptus? suda Canopon habet."
            },
            {
                "verse": "Dicere vel causas cur tanto rupta fragore"
            },
            {
                "verse": "Incutiant nobis fulmina saepe metum."
            },
            {
                "verse": "Vel motus terrae, ventorum nomina, cur nix"
            },
            {
                "verse": "Mollior hiberno tempore grando cadit,"
            },
            {
                "verse": "Durior aestivo. Si animae sperare salutem"
            },
            {
                "verse": "Perpetuam infelix his nequeat studiis."
            },
            {
                "verse": "Hanc Augustini lepidis inquire libellis"
            },
            {
                "verse": "Quos emendatos lector habere potes."
            }
        ]
    ]
}