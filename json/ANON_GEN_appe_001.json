{
    "author": "Anonimo genovese",
    "title": "carminum appendix",
    "url": "http://www.poetiditalia.it/texts/ANON_GEN|appe|001",
    "text": [
        [
            {
                "verse": "Margaretam preciosam,"
            },
            {
                "verse": "Sponsam Xristi generosam"
            },
            {
                "verse": "Veneremur hodie."
            },
            {
                "verse": "A gentili patre nata,"
            },
            {
                "verse": "Fide Xristo desponsata,"
            },
            {
                "verse": "Ipsi regi glorie"
            },
            {
                "verse": "Rectam fidem dum sectatur,"
            },
            {
                "verse": "Domo patris propulsatur:"
            },
            {
                "verse": "Nutrici se contulit."
            },
            {
                "verse": "Generosa oves pavit"
            },
            {
                "verse": "Quam prefectus adamavit,"
            },
            {
                "verse": "Sed hec Xristum protulit."
            },
            {
                "verse": "Ad tribunal presentatur"
            },
            {
                "verse": "Et exuta flagellatur:"
            },
            {
                "verse": "Fluunt rivi sanguinis."
            },
            {
                "verse": "Ridens gaudet passione"
            },
            {
                "verse": "Confortata in agone"
            },
            {
                "verse": "Nutu Sancti Spiritus."
            },
            {
                "verse": "Ex contemptu provocatur"
            },
            {
                "verse": "Iudex; aqua preparatur:"
            },
            {
                "verse": "In vas virgo mergitur."
            },
            {
                "verse": "Tyrannus videns illesam"
            },
            {
                "verse": "Clausit antro virgis cesam:"
            },
            {
                "verse": "Demon terrens cernitur."
            },
            {
                "verse": "Ad mucronem cervix tensa"
            },
            {
                "verse": "Orat voce cum immensa"
            },
            {
                "verse": "Pro se venerantibus."
            },
            {
                "verse": "\"Mox spopondit regnum celi"
            },
            {
                "verse": "Promisso pacto fideli"
            },
            {
                "verse": "Cunctis te laudantibus\"."
            }
        ],
        [
            {
                "verse": "Sponsa Dei speciosa,"
            },
            {
                "verse": "Margaretha vernans rosa,"
            },
            {
                "verse": "De hac valle lacrimosa,"
            },
            {
                "verse": "Tua prece graciosa,"
            },
            {
                "verse": "Nos perduc ad regna gaudiosa."
            }
        ],
        [
            {
                "verse": "Virgo Dei Margaretha,"
            },
            {
                "verse": "Hac in die celi leta"
            },
            {
                "verse": "Scandit ad palacia;"
            },
            {
                "verse": "Ex hoc mundo quam vocavit"
            },
            {
                "verse": "Et in celo collocavit"
            },
            {
                "verse": "Pia Xristi gracia."
            },
            {
                "verse": "Hec virtute Dei plena"
            },
            {
                "verse": "Fuit ex Anthiocena"
            },
            {
                "verse": "Civitate genita."
            },
            {
                "verse": "Carnis per nobilitatem"
            },
            {
                "verse": "Fuit extra civitatem"
            },
            {
                "verse": "Ad alendum posita."
            },
            {
                "verse": "Sed nutrici commendata"
            },
            {
                "verse": "Virgo non est dedignata"
            },
            {
                "verse": "Eius oves pascere."
            },
            {
                "verse": "Hanc ut transiens prefectus"
            },
            {
                "verse": "Vidit, amat et illectus"
            },
            {
                "verse": "Cupit sponsam ducere."
            },
            {
                "verse": "Eius iussu comprehensa,"
            },
            {
                "verse": "Nec per minas nec immensa"
            },
            {
                "verse": "Per promissa flectitur."
            },
            {
                "verse": "Se fatetur xristianam;"
            },
            {
                "verse": "Deos spernit, fidem sanam"
            },
            {
                "verse": "Xristi dum aplectitur."
            },
            {
                "verse": "Hanc iratus plus quam credi"
            },
            {
                "verse": "Possit, preses iubet cedi"
            },
            {
                "verse": "Sed invanum ceditur."
            },
            {
                "verse": "Nam licet incarceratur"
            },
            {
                "verse": "Ungulisque laceratur,"
            },
            {
                "verse": "Nec tamen devincitur."
            },
            {
                "verse": "Ignis ridet ustionem;"
            },
            {
                "verse": "Horridum calcat draconem"
            },
            {
                "verse": "Signo crucis previo."
            },
            {
                "verse": "Aqua magnum vas impletur"
            },
            {
                "verse": "In quo mergi dum iubetur"
            },
            {
                "verse": "Exit sumpto bravio."
            },
            {
                "verse": "Martir dum stat in agone,"
            },
            {
                "verse": "Credit Xristum spe corone,"
            },
            {
                "verse": "Nec terrentur passione"
            },
            {
                "verse": "Gentis quinque milia."
            },
            {
                "verse": "Que tyrannus decollari"
            },
            {
                "verse": "Iubet quibus sociari"
            },
            {
                "verse": "Imperatur pena pari"
            },
            {
                "verse": "Tandem lucis filia."
            },
            {
                "verse": "Huius prece agoniste"
            },
            {
                "verse": "Nos a nece salva, Xriste,"
            },
            {
                "verse": "Mundi post naufragia."
            },
            {
                "verse": "Ut mundemur a peccatis"
            },
            {
                "verse": "Et salvemur cum salvatis"
            },
            {
                "verse": "Eius per suffragia."
            }
        ],
        [
            {
                "verse": "Luce mentis et decore"
            },
            {
                "verse": "Virgo noctis in clamore"
            },
            {
                "verse": "Surgit apta lampade."
            },
            {
                "verse": "Patet aula; rex in porta"
            },
            {
                "verse": "Bysso videns hanc retorta"
            },
            {
                "verse": "Ornatamque ciclade,"
            },
            {
                "verse": "Eius formam concupivit;"
            },
            {
                "verse": "Hec est enim que nescivit"
            },
            {
                "verse": "Thorum per lasciviam."
            },
            {
                "verse": "Tenet illa quem dilexit;"
            },
            {
                "verse": "Hanc dilectam rex invexit"
            },
            {
                "verse": "In cellam vinariam."
            },
            {
                "verse": "Empta pridem Margarita"
            },
            {
                "verse": "Sic probata, sic polita,"
            },
            {
                "verse": "Regis auro redimita"
            },
            {
                "verse": "In gemmarum ordine,"
            },
            {
                "verse": "Nube carens empta chare"
            },
            {
                "verse": "Fit emptori luminare"
            },
            {
                "verse": "Fundamentum angulare:"
            },
            {
                "verse": "Tali gaudet lumine."
            },
            {
                "verse": "Vis de forti muliere"
            },
            {
                "verse": "Scire vere? Hec est vere"
            },
            {
                "verse": "Digna nomen hoc habere"
            },
            {
                "verse": "De quot scripta predixere"
            },
            {
                "verse": "Libri Sapiencie."
            },
            {
                "verse": "Fusum rotat Margaretha;"
            },
            {
                "verse": "Vitem colit vino freta;"
            },
            {
                "verse": "Deo servit carne spreta;"
            },
            {
                "verse": "Spernit iudicis decreta,"
            },
            {
                "verse": "Cultrix pudicicie."
            },
            {
                "verse": "Cesa, flammis usta piceis,"
            },
            {
                "verse": "Pendens uncis, fracta ferreis,"
            },
            {
                "verse": "Imbre fuso sanguinis,"
            },
            {
                "verse": "Fide cincta, cruce previa,"
            },
            {
                "verse": "Bella superat hostilia"
            },
            {
                "verse": "Demonys et hominis."
            },
            {
                "verse": "Hec est dextra deitatis"
            },
            {
                "verse": "Dans infirmis dona gratis;"
            },
            {
                "verse": "Monstrat opus novitatis"
            },
            {
                "verse": "Virtutum magnalia."
            },
            {
                "verse": "Sexus lenis, mollis etas,"
            },
            {
                "verse": "Lege subyt ante metas"
            },
            {
                "verse": "Nam doctores et prophetas"
            },
            {
                "verse": "Tenet in memoria."
            },
            {
                "verse": "Ergo carnis obitu"
            },
            {
                "verse": "Sacra valens spiritu"
            },
            {
                "verse": "Orat pro Ecclesia,"
            },
            {
                "verse": "Pro his habundancius"
            },
            {
                "verse": "Sua specialius"
            },
            {
                "verse": "Qui petunt suffragia."
            },
            {
                "verse": "Spondet precem sponsa Dei;"
            },
            {
                "verse": "Rex alludens speciei"
            },
            {
                "verse": "Tanquam sponsus favet ei"
            },
            {
                "verse": "Pro adulescentulis."
            },
            {
                "verse": "Hanc precemur spe salutis"
            },
            {
                "verse": "Ut in hora senectutis"
            },
            {
                "verse": "Nos defendat a versutis"
            },
            {
                "verse": "Sponsum placans osculis."
            },
            {
                "verse": "Tanta sit victoria"
            },
            {
                "verse": "Summi regis gloria"
            },
            {
                "verse": "Et sponso virginum sonet alleluia."
            }
        ],
        [
            {
                "verse": "Gaude, virgo gloriosa,"
            },
            {
                "verse": "Margaretha preciosa"
            },
            {
                "verse": "Rubricata sanguine."
            },
            {
                "verse": "Gaude, que prefecti spreto"
            },
            {
                "verse": "Voto mali corde leto,"
            },
            {
                "verse": "Gaudens Xristi nomine,"
            },
            {
                "verse": "Gaude, que incarcerata"
            },
            {
                "verse": "Demonem es contemplata"
            },
            {
                "verse": "In draconis specie."
            },
            {
                "verse": "Gaude, quod eum fugasti"
            },
            {
                "verse": "Pede sanctoque calcasti"
            },
            {
                "verse": "Feritatem malicie."
            },
            {
                "verse": "Gaude demum, tormentata"
            },
            {
                "verse": "Igne, aqua, decollata"
            },
            {
                "verse": "Cuin palma victorie."
            },
            {
                "verse": "Fac nos fieri, virgo pia,"
            },
            {
                "verse": "Qui te colunt in hac via"
            },
            {
                "verse": "Cives regni glorie."
            }
        ],
        [
            {
                "verse": "Regi divino uni et trino laus perhennis, decus sit et gloria,"
            },
            {
                "verse": "Miro, tenellam gratam puellam Margaretham, qui ditavit gracia."
            },
            {
                "verse": "Stirpe hec clara nata ........."
            },
            {
                "verse": "<>"
            },
            {
                "verse": ".........fernale stimulos annis primeve etatis"
            },
            {
                "verse": "Cepit carnei luxus fidei concepit amore et castitatis."
            },
            {
                "verse": "In quo persistens et non desistens odia primis ut nulla expavit."
            },
            {
                "verse": "Post genitricis mortem, nutricis simplex ut agnus oviculas pavit."
            },
            {
                "verse": "Quam Olibrius videns impius mox in ipsius exarsit amore."
            },
            {
                "verse": "Nam et roseo ac eburneo virgo predulcis micabat candore."
            },
            {
                "verse": "Que dum presidis et tyrannidis promissis et minis non seducitur,"
            },
            {
                "verse": "Penis, vaziis et suppliciis in carcere torquenda recluditur."
            },
            {
                "verse": "Non sinit flecti sed se a recti tramite non oberrat dum vincitur."
            },
            {
                "verse": "Est virgis cesa igne illesa paciens in cruciatu cernitur."
            },
            {
                "verse": "Icta gladii passo gaudii consors uranice effecta est leta."
            },
            {
                "verse": "Xristi filia, nos associa celice eterne, o Margaretha."
            }
        ]
    ]
}