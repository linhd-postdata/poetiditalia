{
    "author": "carmina libraria",
    "title": "Albrecht von Eym, margarita poetica",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|vone|001",
    "text": [
        [
            {
                "verse": "Conradi Leontorii epigramma"
            }
        ],
        [
            {
                "verse": "Quisquis tersiloquas scribere litteras"
            },
            {
                "verse": "Argutasque velis fingere epistolas"
            },
            {
                "verse": "Seu dulci alloquio semper amabiles"
            },
            {
                "verse": "Adiunxisse animos tibi:"
            },
            {
                "verse": "Huc dulcis iuvenis depropera cito"
            },
            {
                "verse": "Qui crassam docili barbariem tuo"
            },
            {
                "verse": "Excussisse studes ingenio et cupis"
            },
            {
                "verse": "Consectare eloquentiam."
            },
            {
                "verse": "His sunt floridulae litterulis rosae"
            },
            {
                "verse": "Quae Musae pariunt fontibus Aonis."
            },
            {
                "verse": "Hinc Germana bibit iam soboles  bona"
            },
            {
                "verse": "Potam Pieriis aquam."
            },
            {
                "verse": "Hic nempe ore sacro Tullius intonat"
            },
            {
                "verse": "Hispanumque nihil Quintilius sonat"
            },
            {
                "verse": "Et quicquid Latium cultius extulit"
            },
            {
                "verse": "Hoc uno exuberat libro."
            },
            {
                "verse": "Non sic Hesperidum mala micantia"
            },
            {
                "verse": "Quae fuerat vigilans ceruleus draco"
            },
            {
                "verse": "Occluditque aditus perpetuo sui"
            },
            {
                "verse": "Hortu delitias premens."
            },
            {
                "verse": "Hunc servat placidum ianitor hortulum"
            },
            {
                "verse": "Invitaque pios ad sua limina."
            },
            {
                "verse": "Hic regina sui Calliopes chori."
            },
            {
                "verse": "Doctis dividit oscula."
            }
        ]
    ]
}