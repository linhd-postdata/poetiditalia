{
    "author": "carmina libraria",
    "title": "Hippocrates, de insania Democriti",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|hipp|001",
    "text": [
        [
            {
                "verse": "praesumptae philosophorum ignorantiae"
            }
        ],
        [
            {
                "verse": "Philosophus medicus vates in plebe vocatus"
            },
            {
                "verse": "Accipiat laeto carmina pauca tono."
            },
            {
                "verse": "Aethereis quicumque cupit tranare vehiclis"
            },
            {
                "verse": "Elysium discat fidus amare deum."
            },
            {
                "verse": "Imparet ingenio genium vicisse facessat."
            },
            {
                "verse": "Compos et affectus perdomuisse suos."
            },
            {
                "verse": "Non itidem patrium satagat distendere regnum"
            },
            {
                "verse": "In baratrum metuens ne sibi terra cadat."
            },
            {
                "verse": "Libertas animosa fugit dum vera sophia"
            },
            {
                "verse": "Mundanis premunt lemmata stulta sacro."
            },
            {
                "verse": "Non petat Harpias quo Mammon quisque superbit."
            },
            {
                "verse": "Pollicem spernat pacis amore fruens."
            },
            {
                "verse": "Credulus et simplex sit cum pietate benignus"
            },
            {
                "verse": "Irradiata cherub theophania regat."
            },
            {
                "verse": "Militet haud aquilis non naviger antropofagia,"
            },
            {
                "verse": "Belial cautem det Sathanas scopulum."
            },
            {
                "verse": "O stolide curis animae perdiscite laetum,"
            },
            {
                "verse": "Discite longanimis schommata Democriti."
            },
            {
                "verse": "Inde palingenesis modulos ne saeva Megera"
            },
            {
                "verse": "Crinibus inflexis perfida fila neat."
            },
            {
                "verse": "Sollicitet mentem custodis sancta voluntas"
            },
            {
                "verse": "Lumine clara beans pectora Trinacrio"
            }
        ]
    ]
}