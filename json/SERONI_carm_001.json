{
    "author": "Giovanni Antonio Seroni",
    "title": "carmen ad Varchium",
    "url": "http://www.poetiditalia.it/texts/SERONI|carm|001",
    "text": [
        [
            {
                "verse": "Benedicto Varchio"
            },
            {
                "verse": "Urimur ut frustra siren cratere, marino,"
            },
            {
                "verse": "Sebetusve sacro diluat amne pater;"
            },
            {
                "verse": "Fluminis et Nimphae tamen et dulcissima siren"
            },
            {
                "verse": "Me revocant, miris alliciuntque modis;"
            },
            {
                "verse": "An non, particula divinae possumus aurae"
            },
            {
                "verse": "Plura simul longe pervolitare loca?"
            },
            {
                "verse": "Possumus, at Thuscae non urimur igne puellae,"
            },
            {
                "verse": "Tu mihi, tu, Varchi maxime, Thuscus amor."
            }
        ]
    ]
}