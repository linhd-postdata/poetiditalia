{
    "author": "Angelo Poliziano",
    "title": "odae",
    "url": "http://www.poetiditalia.it/texts/POLIZIAN|odae|001",
    "text": [
        [],
        [
            {
                "verse": "in laudem cardinalis Mantuani,Baccii Ugolini nomine"
            }
        ],
        [
            {
                "verse": "O meos longum modulata lusus"
            },
            {
                "verse": "Quos amor primam docuit iuventam,"
            },
            {
                "verse": "Flecte nunc mecum numeros, novumque"
            },
            {
                "verse": "Dic, lyra, carmen:"
            },
            {
                "verse": "Non quod hirsutos agat huc leones;"
            },
            {
                "verse": "Sed quod et frontem domini serenet,"
            },
            {
                "verse": "Et levet curas, penitusque doctas"
            },
            {
                "verse": "Mulceat aures."
            },
            {
                "verse": "Vindicat nostros sibi iure cantus"
            },
            {
                "verse": "Qui colit vates citharamque princeps;"
            },
            {
                "verse": "Ille cui sacro rutilus refulget"
            },
            {
                "verse": "Crine galerus;"
            },
            {
                "verse": "Ille cui flagrans triplici corona"
            },
            {
                "verse": "Cinget auratam diadema frontem."
            },
            {
                "verse": "Fallor? An vati bonus haec canenti"
            },
            {
                "verse": "Dictat Apollo?"
            },
            {
                "verse": "Phoebe, quae dictas, rada fac, precamur:"
            },
            {
                "verse": "Dignus est nostrae dominus Thaliae,"
            },
            {
                "verse": "Cui celer versa fluat Hermus uni"
            },
            {
                "verse": "Aureus urna;"
            },
            {
                "verse": "Cui tuas mittat, Cytherea, conchas"
            },
            {
                "verse": "Conscius primi Phaetontis Indus;"
            },
            {
                "verse": "Ipsa cui dives properet beatum"
            },
            {
                "verse": "Copia cornu."
            },
            {
                "verse": "Quippe non gazam pavidus repostam"
            },
            {
                "verse": "Servat, aeoeo similis draconi;"
            },
            {
                "verse": "Sed vigil famam secat, ac perenni"
            },
            {
                "verse": "Imminet aevo."
            },
            {
                "verse": "Ipsa phoebeae vacat aula turbae,"
            },
            {
                "verse": "Dulcior blandis Heliconis umbris;"
            },
            {
                "verse": "Et vocans doctos patet ampla toto"
            },
            {
                "verse": "Ianua poste."
            },
            {
                "verse": "Sic refert magnae titulis superbum"
            },
            {
                "verse": "Stemma Gonzagae recidia virtus,"
            },
            {
                "verse": "Gaudet et fastos superare avitos"
            },
            {
                "verse": "Aemulus haeres."
            },
            {
                "verse": "Scilicet stirpem generosa succo"
            },
            {
                "verse": "Poma commendant, timidumque numquam"
            },
            {
                "verse": "Vulturem foeto Iovis acer ales"
            },
            {
                "verse": "Extudit ovo."
            },
            {
                "verse": "Curre iam toto violentus amne,"
            },
            {
                "verse": "O sacris Minci celebrate Musis!"
            },
            {
                "verse": "Ecce Mecoenas tibi nunc Maroque"
            },
            {
                "verse": "Contingit uni."
            },
            {
                "verse": "Iamque vicinas tibi subdat undas"
            },
            {
                "verse": "Vel Padus multo resonans olore,"
            },
            {
                "verse": "Quamlibet flentes animosus alnos"
            },
            {
                "verse": "Astraque iactet."
            },
            {
                "verse": "Candidas ergo volucres notarat"
            },
            {
                "verse": "Mantuam condens tiberinus Ocnus,"
            },
            {
                "verse": "Nempe quem parcae docuit benignae"
            },
            {
                "verse": "Conscia mater."
            }
        ]
    ]
}