{
    "author": "carmina libraria",
    "title": "Adam Freitag, architectura militaris nova et aucta",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|frei|001",
    "text": [
        [
            {
                "verse": "ad eximium virum Adamum Adamum Freitagium Borussum operis auctorem"
            }
        ],
        [
            {
                "verse": "Arces cingere machinis et urbes"
            },
            {
                "verse": "Et quocumque lubet locum tueri"
            },
            {
                "verse": "Muro pulveris erutaque terra"
            },
            {
                "verse": "Cum lacessitur aut lacessit hostis"
            },
            {
                "verse": "Ex quo fulminis impetum minamur,"
            },
            {
                "verse": "Et Quiritibus aut reperta nostris"
            },
            {
                "verse": "Uno diruta concidunt sub ictu"
            },
            {
                "verse": "Freitagi labor est. Favete chartae"
            },
            {
                "verse": "Quos victoria, quos decent triumphi."
            },
            {
                "verse": "Vobis pagina laureata surgit,"
            },
            {
                "verse": "Illi gloria regiumque nomen."
            },
            {
                "verse": "Res est Regia Regibus probari"
            },
            {
                "verse": "Plus, quae Regia sunt, docere Reges."
            }
        ],
        [
            {
                "verse": "Ex tempore scribebat Daniel Heinsius"
            }
        ]
    ]
}