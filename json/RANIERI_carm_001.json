{
    "author": "Anton Francesco Ranieri",
    "title": "carmen ad Varchium",
    "url": "http://www.poetiditalia.it/texts/RANIERI|carm|001",
    "text": [
        [
            {
                "verse": "ad Benedictum Varchium"
            },
            {
                "verse": "Dulcis deseruisse quod videtur,"
            },
            {
                "verse": "Gaudere Aoniam Camoena rupem"
            },
            {
                "verse": "Labentem placide et redisse ad Arnum."
            },
            {
                "verse": "Tuis auspiciis, beate Varchi,"
            },
            {
                "verse": "Grates ipsi agimus bono poetae,"
            },
            {
                "verse": "Ingentes tibi, dum tuis capillis"
            },
            {
                "verse": "Nectunt Pierides novem coronam,"
            },
            {
                "verse": "Quae tondent violas nova recentes"
            },
            {
                "verse": "Ripas, qua virides coercet amnis,"
            },
            {
                "verse": "Fovens nunc gremio novem sorores."
            }
        ],
        [
            {
                "verse": "responsio"
            },
            {
                "verse": "Quod me tam lepidis, venuste, tanquam"
            },
            {
                "verse": "Doctis hendecasillabis, et ipsis"
            },
            {
                "verse": "Plenis undique gratiis celebres,"
            },
            {
                "verse": "Id sane magis est benignitatis"
            },
            {
                "verse": "Tuae, quam meriti mei. Quid? Immo"
            },
            {
                "verse": "Nil prorsus mihi debeo, sed omne"
            },
            {
                "verse": "Acceptum tibi sedulus referre"
            },
            {
                "verse": "Utcumque est, utinam rudi novellae"
            },
            {
                "verse": "Arni tam mihi debeant Camoenae,"
            },
            {
                "verse": "Quam culto tibi Tybridis vetustae!"
            }
        ]
    ]
}