{
    "author": "Lilio Tifernate",
    "title": "laus Galli",
    "url": "http://www.poetiditalia.it/texts/LIL_TIFE|laus|001",
    "text": [
        [
            {
                "verse": "Qui cunctos dulci superabas carmine vates"
            },
            {
                "verse": "Incolis Elysios tu modo, Galle, locos;"
            },
            {
                "verse": "Sed te deflentes nimio maerore Camenae"
            },
            {
                "verse": "Defunctum ulterius vivere velle negant:"
            },
            {
                "verse": "Namque suum (en ingens dolor) amisere parentem"
            },
            {
                "verse": "Et Cytherae pollens eloquiisque decus."
            }
        ]
    ]
}