{
    "author": "P. Delio Volsco",
    "title": "carmen",
    "url": "http://www.poetiditalia.it/texts/VOLSCO|carm|001",
    "text": [
        [
            {
                "verse": "magnifico d. Ioanni Fusmagen sacrae Caesareae Maiestatis consiliario dignissimo P. Delius Volscus"
            }
        ],
        [
            {
                "verse": "Quod decet ingenium, nobis, Fusmage, dedisti"
            },
            {
                "verse": "Munus, et externis quod solet esse quies."
            },
            {
                "verse": "Nullus in ignotum modus est tibi nempe relictus,"
            },
            {
                "verse": "Quem noster Phoebus quemque Minerua colat."
            },
            {
                "verse": "Officio linguae, mentis bonitate subisti"
            },
            {
                "verse": "Perpetuum uerae munus amicitiae."
            },
            {
                "verse": "His igitur, qui summa tenet fastigia coeli,"
            },
            {
                "verse": "Det tibi felices Nestoreosque dies;"
            },
            {
                "verse": "Det tibi Caesareum sapienti numen amicum,"
            },
            {
                "verse": "Semper cui fidis consulis auxiliis."
            }
        ]
    ]
}