{
    "author": "carmina libraria",
    "title": "Melchior Canus, opera",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|canu|001",
    "text": [
        [
            {
                "verse": "Ioannis Petri Toletani in Melchiorem Canum episcopum Canatiensem ode"
            }
        ],
        [
            {
                "verse": "Si digna tentet Melpomene tuis"
            },
            {
                "verse": "Pensare vates carmina laudibus"
            },
            {
                "verse": "Quae sacra dilectum Minerva"
            },
            {
                "verse": "Ipsa sinu fovet alma castro;"
            },
            {
                "verse": "Dircaea poscat plectra,  sibi Lyram"
            },
            {
                "verse": "Aut si quid insit grandius insonat"
            },
            {
                "verse": "Qualis comanti quae pependit"
            },
            {
                "verse": "Ex humero religata Phoebi."
            },
            {
                "verse": "Ille et beatas eloquii canet"
            },
            {
                "verse": "Caelestis unda, non tibi rivulo"
            },
            {
                "verse": "Sed fronte largo perfluentes"
            },
            {
                "verse": "Fundis ubi placido ore nectar."
            },
            {
                "verse": "Illa et sacrorum oracula fontium"
            },
            {
                "verse": "Vulgatae turbis de perennibus"
            },
            {
                "verse": "Haustum scatebris, quo rigantur"
            },
            {
                "verse": "Ora virum sitibunda flumen;"
            },
            {
                "verse": "Quippe ut vetustas accumulatius"
            },
            {
                "verse": "Proferre largas divitias manu"
            },
            {
                "verse": "Consuevit et fulvi metalli"
            },
            {
                "verse": "Dives opum genus omne promit."
            },
            {
                "verse": "Sic tu vetusto de sapientiae"
            },
            {
                "verse": "Penu benigne deque recentibus"
            },
            {
                "verse": "Deprompta Thesauris talenta"
            },
            {
                "verse": "Dividis uberiore fetu."
            },
            {
                "verse": "Euge o fidelis prome et sterilium"
            },
            {
                "verse": "Custos bonorum, qui tibi creditum"
            },
            {
                "verse": "Non abdidisti parcus aurum"
            },
            {
                "verse": "Foenore sed gemino reponis."
            },
            {
                "verse": "Intrabis ergo quo Dominus iubet"
            },
            {
                "verse": "Dignos ministros in requiem ingredi"
            },
            {
                "verse": "Quo nullus intrat sordidatus"
            },
            {
                "verse": "Sed trabea redimitus alba."
            },
            {
                "verse": "Illic nitebis caelicolum greges"
            },
            {
                "verse": "Inter loratus, clara micantibus"
            },
            {
                "verse": "Ut stella flammis. Quippe doctus"
            },
            {
                "verse": "Talis honos manet inter omnes."
            },
            {
                "verse": "Illic videbis purpurea stola"
            },
            {
                "verse": "Cinctum Magistrum pocula nectaris"
            },
            {
                "verse": "Lectae ministrantem coronae"
            },
            {
                "verse": "Perpetuis recreare mensis."
            },
            {
                "verse": "Intaminati tu quoque corporis"
            },
            {
                "verse": "Artus levatos flamine concines"
            },
            {
                "verse": "Ieiuniorum post labores"
            },
            {
                "verse": "Et domitus repetetur hostis."
            },
            {
                "verse": "Hinc iuxta vinum lympha rubescere et"
            },
            {
                "verse": "Bis sex virorum concio principum"
            },
            {
                "verse": "Dicetur et cymba ad cacumen"
            },
            {
                "verse": "Et solium revocata regni."
            },
            {
                "verse": "Quod cum stupendis prodigiis canes"
            },
            {
                "verse": "Pulsosque morbos et furialibus"
            },
            {
                "verse": "Diris levator et Sepulchris"
            },
            {
                "verse": "Et latebris rediviva membra."
            },
            {
                "verse": "Infida post haec oscula et impiam"
            },
            {
                "verse": "Dices Phalangem et sacrilegum nefas"
            },
            {
                "verse": "Raptumque vinclis tribunal"
            },
            {
                "verse": "Per scelerum omne genus regem."
            },
            {
                "verse": "Infame lignum, sed Domini in nece"
            },
            {
                "verse": "Mox consecratum quod diademate"
            },
            {
                "verse": "Regum nitescat, quod nefandae"
            },
            {
                "verse": "Supplicium fuit ante turbae."
            },
            {
                "verse": "At cum reduces Tartareo  specu"
            },
            {
                "verse": "Clarum trophaeis et spoliis ducem"
            },
            {
                "verse": "Quem perstrepenti concinebam"
            },
            {
                "verse": "Ipse lyra et rudiore plectro."
            },
            {
                "verse": "Omnes iubebo degeneres procul"
            },
            {
                "verse": "Hinc ire curas deque catenulis"
            },
            {
                "verse": "Suspensus auratis beatam"
            },
            {
                "verse": "Aure bibam sitienti venam."
            }
        ],
        []
    ]
}