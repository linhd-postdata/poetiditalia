{
    "author": "carmina libraria",
    "title": "Horatius, censura poetica ad Pisones",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|hor2|001",
    "text": [
        [
            {
                "verse": "ad lectorem Horatianum"
            }
        ],
        [
            {
                "verse": "Quem fratrem potius Calliope soror"
            },
            {
                "verse": "Sumat deliciis quam fleat Orphea."
            },
            {
                "verse": "Qui mulsit tigrides et rabidas feras"
            },
            {
                "verse": "Ne maior sit Horatio."
            },
            {
                "verse": "Vates Pindaricos Virgilius modos"
            },
            {
                "verse": "Quamvis et superet, cesserat huic tamen."
            },
            {
                "verse": "Fratris carminibus aemula ne sua"
            },
            {
                "verse": "Starent condiderit Maro."
            },
            {
                "verse": "Flacci discipulus Naso poeticus"
            },
            {
                "verse": "Sanguis Pompilius Pisooque maximus"
            },
            {
                "verse": "Hausere ammoniti dogma salubriter"
            },
            {
                "verse": "Docti de calabro viro."
            },
            {
                "verse": "Nec solum patriae profuit aut sibi,"
            },
            {
                "verse": "Romanae pariter cede propagini."
            },
            {
                "verse": "Verum barbaricis posteritatibus"
            },
            {
                "verse": "Stillat mella potica."
            },
            {
                "verse": "Tandem quid deceat quid neque dedecet"
            },
            {
                "verse": "Vatem carminibus ac opibus sacris"
            },
            {
                "verse": "Conflandis quid alit  fert et acutior"
            },
            {
                "verse": "Munus lector ama. Et vale"
            }
        ]
    ]
}