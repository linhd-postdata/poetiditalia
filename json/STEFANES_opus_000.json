{
    "author": "Iacopo Stefaneschi",
    "title": "opus metricum",
    "url": "http://www.poetiditalia.it/texts/STEFANES|opus|000",
    "text": [
        [],
        [
            {
                "verse": "Iacobi sancti Georgii ad velum aureum diaconi cardinalis succincta metro de semetipso descriptio"
            }
        ],
        [
            {
                "verse": "Urbs mihi principium generis, Iacobus mihi nomen"
            },
            {
                "verse": "Gaietanus erat, fluvii transtibridis amnem"
            },
            {
                "verse": "Stephanidum de stirpe satus producor ab Ursa."
            },
            {
                "verse": "Murronem cecini repetentem claustra monarcham"
            },
            {
                "verse": "Insertumque polo, Bonifatius utque triumphet"
            },
            {
                "verse": "Urbe sacra diadema ferens, quo cardine fultus"
            },
            {
                "verse": "Hunc panxi celoque patrem metroque refudi"
            },
            {
                "verse": "Centenum fudique prosa; Deus hinc tibi laus est."
            }
        ]
    ]
}