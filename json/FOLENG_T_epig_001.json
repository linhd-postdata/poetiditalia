{
    "author": "Teofilo Folengo",
    "title": "epigrammata",
    "url": "http://www.poetiditalia.it/texts/FOLENG_T|epig|001",
    "text": [
        [
            {
                "verse": "Multicoloritam recipit iam terra camoram"
            },
            {
                "verse": "Bellaque florettos dat pradaria novos."
            },
            {
                "verse": "Montagnae rident, boscamina virda fiuntur,"
            },
            {
                "verse": "Qua eque sibi charum cercat osella virum."
            },
            {
                "verse": "Frigida per caldas rampat luserta muraias"
            },
            {
                "verse": "Et bona florigeros pecchia sachezzat agros."
            },
            {
                "verse": "Exit graniferas formica sacenta masones"
            },
            {
                "verse": "Ranaque domandat quo peregrinus eat."
            },
            {
                "verse": "Pastorella suum cantat damatina morosum,"
            },
            {
                "verse": "Cui texit variis serta galanta rosis."
            }
        ],
        [
            {
                "verse": "Caldus afogatum iam schiappat Apollo terenum"
            },
            {
                "verse": "Cunctaque boiento brostolat arva foco;"
            },
            {
                "verse": "Vult eat addagium pigris carretta cavallis"
            },
            {
                "verse": "Ipsaque straccatur dextra tirando briam."
            },
            {
                "verse": "Omnia maturis ita flavent rura biavis,"
            },
            {
                "verse": "Ut iam polledris fraina negatur equis."
            },
            {
                "verse": "Cantat supra palum crepatque canendo cicala,"
            },
            {
                "verse": "Stigat mastinos mosca tavana canes."
            },
            {
                "verse": "Arsus comportat villanus apena camisam,"
            },
            {
                "verse": "Caneva Todeschis semper aperta manet."
            }
        ],
        [
            {
                "verse": "Ut cibet Invernum mater Natura famatum,"
            },
            {
                "verse": "Multa magazzenis stipat edenda suis."
            },
            {
                "verse": "Formichetta trahit segetum ad granaria somas"
            },
            {
                "verse": "Mellaque ceratis condit apetta casis."
            },
            {
                "verse": "Stramina pro bobus mangianda bovarus adunat"
            },
            {
                "verse": "Idque facis pegoris, bel pegorare, tuis."
            },
            {
                "verse": "Rumor ab obscuris cantinis maximus exit,"
            },
            {
                "verse": "Dum cerchiant vino vasa paranda novo."
            },
            {
                "verse": "Mustolenta replet graspis fameia tinazzos"
            },
            {
                "verse": "Todeschique canunt: \"Ehu ohe, trincher io\"."
            }
        ],
        [
            {
                "verse": "Tornat Hyperboreis iam Borra gaioffus ab Alpis,"
            },
            {
                "verse": "Manticibus sfoliat qui nemus omne suis."
            },
            {
                "verse": "Flumina deventant vitrum campique biacca,"
            },
            {
                "verse": "Brumaque candelas spargit ubique suas."
            },
            {
                "verse": "Cheta stat in gusso foribus limaca seratis,"
            },
            {
                "verse": "Frigore iam moritur mosca, cigala fame."
            },
            {
                "verse": "Vecchiarella parat coctae convivia rapae"
            },
            {
                "verse": "Nec pransat, nisi sit voda conocchia prius."
            },
            {
                "verse": "Pallidat insomnes oliosa lucerna pedantos,"
            },
            {
                "verse": "Tuque malenconica nocte, studente, godis."
            }
        ],
        [
            {
                "verse": "Quos mihi misisti versus de quattuor anni"
            },
            {
                "verse": "Temporibus, medio legimus in Studio."
            },
            {
                "verse": "Diversas de te diversa in gente catavi"
            },
            {
                "verse": "Sentenzas, quarum displicet una mihi,"
            },
            {
                "verse": "Scilicet ignorans quod sis parlare Latinum,"
            },
            {
                "verse": "Unde macaronica dicier arte cupis."
            },
            {
                "verse": "Fac, age, quaeso, provam, si quattuor illa Camenis"
            },
            {
                "verse": "Tempora legitimis dicere forte queas."
            }
        ],
        [
            {
                "verse": "Nil mihi diversae stimmatur opinio turbae:"
            },
            {
                "verse": "Sum macaronus ego, sic macaronus ero."
            },
            {
                "verse": "Sed tibi, Balde, tamen parebo: dat ecce Comina"
            },
            {
                "verse": "Hos utcumque potest facta Latina modos."
            }
        ],
        [
            {
                "verse": "Ver novum domitis adest pruinis,"
            },
            {
                "verse": "Cui Mars nunc aperit tenerque Aprilis,"
            },
            {
                "verse": "Occurrunt Veneres vagique Amores,"
            },
            {
                "verse": "Dum tectum vetuli subintrat Anni."
            },
            {
                "verse": "Sol freno ignivomos equos retentat,"
            },
            {
                "verse": "Lentis solstitium notans quadrigis;"
            },
            {
                "verse": "Aestas nuda sitit, gravesque spicas"
            },
            {
                "verse": "Duro torret, cupidos parat colonos."
            },
            {
                "verse": "Autumnus moderante sole flammas"
            },
            {
                "verse": "Prodit tardigradus, satis quod ille"
            },
            {
                "verse": "Succulentus hebet sua eque vites"
            },
            {
                "verse": "Illi palmitibus gemunt onustis."
            },
            {
                "verse": "Adportat gelidos Hyems novembres,"
            },
            {
                "verse": "Canasque hirta comas trahit podagram,"
            },
            {
                "verse": "Tussit pelliceis pedes ad imos"
            },
            {
                "verse": "Se volvens tunicis fovensve prunas."
            }
        ],
        [
            {
                "verse": "Ver nitens, ubi nix modo imperabat,"
            },
            {
                "verse": "Summit regna suasque ponit herbas:"
            },
            {
                "verse": "His arridet Amor parensque Amoris,"
            },
            {
                "verse": "His blanda Zephyrus iocatur aura."
            },
            {
                "verse": "Aestas delitiis laboriosa"
            },
            {
                "verse": "Praefert seria decutitque flores:"
            },
            {
                "verse": "His fruges subicit Ceres opimas,"
            },
            {
                "verse": "His flavae subeunt deae manipli."
            },
            {
                "verse": "Autumnus vacuos cados vetusti"
            },
            {
                "verse": "Ad novum reparat Thyoniani:"
            },
            {
                "verse": "His plaudit Bromius, Bromique pupi"
            },
            {
                "verse": "His circumsiliunt chorosque ducunt."
            },
            {
                "verse": "Horret bruma gelu casa eque rimas"
            },
            {
                "verse": "Stipat sedula pervias rigori:"
            },
            {
                "verse": "His mugit Boreas suasque victor"
            },
            {
                "verse": "His vibrat Scythico nives ab Arcto."
            }
        ],
        [
            {
                "verse": "Iam novos variosque picta flores"
            },
            {
                "verse": "Passim reddit humus decusque veris;"
            },
            {
                "verse": "Lascivit Cytherea consuitque"
            },
            {
                "verse": "Ludenti violas rosasque Amori."
            },
            {
                "verse": "Dat messes ager hispidus salubres,"
            },
            {
                "verse": "Nec tunc utilis impeditur ardor;"
            },
            {
                "verse": "Texit flava Ceres graves aristas,"
            },
            {
                "verse": "Cingens tempora spiceis corollis."
            },
            {
                "verse": "Pingues pampineos gravant lacertos"
            },
            {
                "verse": "Uvae, pampineos gravant lacerti"
            },
            {
                "verse": "Ulmos, ulmi asinum gravant flagellis,"
            },
            {
                "verse": "Dum, Silene, agis hunc gravem racemis."
            },
            {
                "verse": "Nix Alpes aquilone sibilante"
            },
            {
                "verse": "Summas candidat, implicantur amnes;"
            },
            {
                "verse": "Hinc Nymphae vitreo stupent ab alveo"
            },
            {
                "verse": "Et comunt glacie comas sub ipsa."
            }
        ],
        [
            {
                "verse": "Fragrant multicolora prata gemmis,"
            },
            {
                "verse": "Quas leni populant apes susurro;"
            },
            {
                "verse": "Laetae qua nemorum trabes comantum"
            },
            {
                "verse": "Frondescunt, avibus sonant canoris."
            },
            {
                "verse": "Flagrant pulverulenta rura flammis,"
            },
            {
                "verse": "Quas brevi extenuant oves sub umbra;"
            },
            {
                "verse": "Lentae qua coryli nigraeque olivae"
            },
            {
                "verse": "Pallescunt, querulis strident cicadis."
            },
            {
                "verse": "Turgent mellificata musta botris,"
            },
            {
                "verse": "Quos Bacchae patulis ferunt canistris,"
            },
            {
                "verse": "Et qua vitibus explicantur umbrae:"
            },
            {
                "verse": "\"Io, Bacche - canunt -, io, Lyaee!\""
            },
            {
                "verse": "Torpent obstupefacta stagna crustis,"
            },
            {
                "verse": "Quas nec plaustra notant rota eque signant;"
            },
            {
                "verse": "Et qua cymba modo premebat undas,"
            },
            {
                "verse": "Colludunt pueri labantque plantis."
            }
        ],
        [
            {
                "verse": "Florescunt sata vineaeque gemmant,"
            },
            {
                "verse": "Purae per teretes cient lapillos"
            },
            {
                "verse": "Raucum murmur aquae, udulique rivi"
            },
            {
                "verse": "Albent liliolis, rubent rosetis."
            },
            {
                "verse": "Durescunt segetes agris caduntque"
            },
            {
                "verse": "Iam curvis sata falcibus, nec aestu"
            },
            {
                "verse": "Messores avidum gemunt laborem,"
            },
            {
                "verse": "Sed crebris cyathis levant calorem."
            },
            {
                "verse": "Dulcescunt nimioque botriones"
            },
            {
                "verse": "Sunt ulmis oneri tumente succo;"
            },
            {
                "verse": "Fit vindemia vinitorque laetus"
            },
            {
                "verse": "Suo destituit decore vites."
            },
            {
                "verse": "Canescunt nive tecta stiriaeque"
            },
            {
                "verse": "His pendent vitreae stupentque guttae;"
            },
            {
                "verse": "Pastor straminibus simulque arator"
            },
            {
                "verse": "Defendunt, hic oves, at ille tauros."
            }
        ],
        [
            {
                "verse": "Vector Sydoniae calet puellae:"
            },
            {
                "verse": "Sagittas acuit tener Cupido,"
            },
            {
                "verse": "Torquati geminantur hinc palumbes,"
            },
            {
                "verse": "Auriti lepores boaeque turpes."
            },
            {
                "verse": "Sydus Herculei flagrat Leonis:"
            },
            {
                "verse": "Frondes nec tenui moventur aura,"
            },
            {
                "verse": "Dumos nec gelidi exeunt lacerti,"
            },
            {
                "verse": "Portu nauta latet, viator umbra."
            },
            {
                "verse": "Pugnaci haeret Apollo Scorpioni:"
            },
            {
                "verse": "Spumant labra mero lacusque praelo"
            },
            {
                "verse": "Vi pressante capit novum Phalernum,"
            },
            {
                "verse": "Complenturque cadi amphoraeque grandes."
            },
            {
                "verse": "Horret sydere bruma Capricorni:"
            },
            {
                "verse": "Auget ligna focis veruque torret"
            },
            {
                "verse": "Longo terga suum, superque crates"
            },
            {
                "verse": "Tuceta et croceae strident tomaclae."
            }
        ],
        [
            {
                "verse": "Quare si quemquam chiamamus, dicimus: \"Ola!\","
            },
            {
                "verse": "Et qui respondet consonat ore: \"Che la?\","
            },
            {
                "verse": "Si quis forte huius causam vult scire cotali,"
            },
            {
                "verse": "Dicam, sed dicat hoc prius ille mihi."
            }
        ],
        [
            {
                "verse": "Est poltronus homo, sguataro poltronior omni:"
            },
            {
                "verse": "Bos plus vergognae plusque rasonis habet."
            },
            {
                "verse": "Trat sine respetto turba praesente corezzas"
            },
            {
                "verse": "Nec pingit guanzas ulla rubedo suas;"
            },
            {
                "verse": "Semper enim fortum paret mangiasse fenocchium,"
            },
            {
                "verse": "Semper et aiorum quinque vel octo capos."
            },
            {
                "verse": "Saepe cativeria tamen hac usatur et arte:"
            },
            {
                "verse": "Cum tussit retro, tussit et ante simul,"
            },
            {
                "verse": "Ne quisquam valeat crepitum sentire scapatum"
            },
            {
                "verse": "Ac odor alterius pareat esse viae."
            },
            {
                "verse": "At tussis vacem bombardae vellet habere,"
            },
            {
                "verse": "Tanto croccatur stricta corezza sono."
            }
        ],
        [
            {
                "verse": "Quam bene disposuit cunctis natura facendis,"
            },
            {
                "verse": "Quam bene procedunt ordine cuncta suo!"
            },
            {
                "verse": "Est lagus Italiae, qui nunc de Garda vocatur"
            },
            {
                "verse": "Quique procellosis ut mare balzat aquis."
            },
            {
                "verse": "Non nisi bon pisces mangiantur semper ab illo:"
            },
            {
                "verse": "Sardenae, anguillae, carpio, tenca, trotae."
            },
            {
                "verse": "Sed nil Palladio piscis valet absque liquore:"
            },
            {
                "verse": "Nonne oleo pisces nigra padella coquit?"
            },
            {
                "verse": "Ergo per intornum ripae carigantur olyvis"
            },
            {
                "verse": "Datque vasos ferri Bressa propinqua sui."
            },
            {
                "verse": "Nascitur hic oleum, piscis, piscator et ipsa"
            },
            {
                "verse": "Piscibus assandis apta padella simul."
            }
        ],
        [
            {
                "verse": "Venerat ad mortem terzana febre Tonellus,"
            },
            {
                "verse": "Cui stabat praesens mater et una soror ."
            },
            {
                "verse": "Affuit hic candela sefi constata quatrino,"
            },
            {
                "verse": "Quam tenet in propria povera vecchia manu;"
            },
            {
                "verse": "Dum brusat et culum iamiam focus ardet ad imum,"
            },
            {
                "verse": "Mater ait: \"Fili, iam moriare, precor;"
            },
            {
                "verse": "Nam neque tu moreris, nec ego meschinula filo,"
            },
            {
                "verse": "Et iam candelae culus adustus abit\"."
            }
        ],
        [
            {
                "verse": "Baldraccus numquam nisi de mangiamine pensat:"
            },
            {
                "verse": "Cum mangiat, satiam nescit habere gulam."
            },
            {
                "verse": "Scit dare praeceptum galantiter omne coquinae,"
            },
            {
                "verse": "Namque lecatoria semper in arte studet."
            },
            {
                "verse": "Sic ait: \"In speto rostirier oca tenetur"
            },
            {
                "verse": "Plenaque sint spetiis interiora bonis,"
            },
            {
                "verse": "Quae dum arrostitur, quae dum gyratur atornum,"
            },
            {
                "verse": "Non cesset lardi serva butare brodum."
            },
            {
                "verse": "Haec est materies atque ars et forma coquendi,"
            },
            {
                "verse": "Haec venit a nostris regula docta scholis\"."
            }
        ],
        [
            {
                "verse": "Quam sunt et lepidi, quam et elegantes"
            },
            {
                "verse": "Sunt tui endecasyllabi, Petrille,"
            },
            {
                "verse": "Summaque arte laboriosiores,"
            },
            {
                "verse": "Doctos qui referas in bis Catullos,"
            },
            {
                "verse": "Tam sunt illecebrae tui exoleti"
            },
            {
                "verse": "Scortilli putridae iocique turpes,"
            },
            {
                "verse": "Ut non putridius cadaver, ut non"
            },
            {
                "verse": "Sit, cur turpe oleat magis cloaca."
            },
            {
                "verse": "Quare odoriferas rosas, Petrille,"
            },
            {
                "verse": "Si quando olfacimus tui leporis"
            },
            {
                "verse": "Optamusque ibi mille habere nasos,"
            },
            {
                "verse": "En, hui, quam cito nostra nescio quo"
            },
            {
                "verse": "Ventris profluvio cadit voluptas,"
            },
            {
                "verse": "Atque intercipiunt rosas oleta"
            },
            {
                "verse": "Optamusque ibi nullum habere nasum."
            },
            {
                "verse": "Si nescis, tua bella, mi Petrille,"
            },
            {
                "verse": "Vernum Cynthia concacat rosetum."
            }
        ],
        [
            {
                "verse": "Quisquis Olympiacas poterit numerare fasellas,"
            },
            {
                "verse": "Vel maris in fundo sabbia quanta latet,"
            },
            {
                "verse": "Aut quisquis foias quantas fert sylva Bacani,"
            },
            {
                "verse": "Aut quantas muscas Puia boienta parit,"
            },
            {
                "verse": "Nempe tuas poterit numerare, Briosse, bosias:"
            },
            {
                "verse": "Mentiris subito, cum tua labra moves."
            },
            {
                "verse": "Miraclum reputo quando parlare quiescis,"
            },
            {
                "verse": "Qui neque de tacita nocte tacere potes."
            },
            {
                "verse": "Tyrhenum potius biberet formica profundum"
            },
            {
                "verse": "Et ferret Zenovae piccola musca ratem,"
            },
            {
                "verse": "Quam tua veridicam ferat umquam lingua parolam:"
            },
            {
                "verse": "Nil nisi, cum parlas, bocca bosia tua est."
            },
            {
                "verse": "Dens tibi si caderet quoties mendacia profers,"
            },
            {
                "verse": "Iam tua non posset pane ganassa frui."
            },
            {
                "verse": "In solo flatu dicis sex mille bosias,"
            },
            {
                "verse": "Post quas sex alias mille spudare paras."
            }
        ],
        [
            {
                "verse": "Legiadram mea stalla tenet, Falchette, cavallam,"
            },
            {
                "verse": "Quam quicumque videt percupit esse suam."
            },
            {
                "verse": "Stare parangono Gonellae nempe cavalli"
            },
            {
                "verse": "Posset, qui tantum pellis et ossa fuit."
            },
            {
                "verse": "Longior ipsius sex est magra schena cavezzis,"
            },
            {
                "verse": "Crustas de marzo sanguine semper habet,"
            },
            {
                "verse": "Quas polmoncellos vocat ars merscaltica crustas,"
            },
            {
                "verse": "Quas de pellibovo sella vodata facit."
            },
            {
                "verse": "An sit mula quidem dubitas, an sitve cavalla:"
            },
            {
                "verse": "Orecchias longas tres habet illa pedes."
            },
            {
                "verse": "Zardarum dicunt duplex genus esse, quod omne"
            },
            {
                "verse": "Possidet: in pedibus prima venire solet,"
            },
            {
                "verse": "Altera, quando cavat sibi frenum, meque pedestrum"
            },
            {
                "verse": "Linquens calcagnis pagat eundo viam;"
            },
            {
                "verse": "Cursito post illam, clamo \"Sta, bestia! Pru, sta!\","
            },
            {
                "verse": "Fert mea bastonem dextra, sinistra briam,"
            },
            {
                "verse": "Sed potius muro tunc possem dicere \"Sta! Sta!\":"
            },
            {
                "verse": "Non audit, quamvis grandis orecchia patet;"
            },
            {
                "verse": "Dum curro, per mille cavas, per mille zapellos"
            },
            {
                "verse": "Casco, nam laqueat spronus uterque pedes."
            },
            {
                "verse": "Quando cavalco illam, mihi paret habere morenas"
            },
            {
                "verse": "Et bogas pedibus rozza ribalda gerit;"
            },
            {
                "verse": "Cum me scavalcat, morsu fugit inde cavato,"
            },
            {
                "verse": "Turchescos etenim vincere posset equos."
            },
            {
                "verse": "Et quamvis oculo nihil uno cernat, et altro"
            },
            {
                "verse": "Pochinum, numquam decidit illa tamen;"
            },
            {
                "verse": "Decidit illa nihil quando sine pondere scappat,"
            },
            {
                "verse": "Sed portans aliquem tota stravaccat humi."
            },
            {
                "verse": "Incastellata est, non mancant rogna, spinellae,"
            },
            {
                "verse": "Denique quicquid habet digna cavalla boni,"
            },
            {
                "verse": "Digna cavalla boni quod habet pro pascere, dico,"
            },
            {
                "verse": "Ergo comprandi si cui foret ullula voia,"
            },
            {
                "verse": "Hanc emat et faciat, sicut usanza, provam."
            },
            {
                "verse": "Est unum, quod sit poledrina, signale galantum:"
            },
            {
                "verse": "Vermibus haec alios semper amorbat equos;"
            },
            {
                "verse": "Non patitur vermes senior, sed parvulus infans,"
            },
            {
                "verse": "Quapropter venas ungueo saepe suas."
            },
            {
                "verse": "Utilitas erit haec compranti: sola valebit"
            },
            {
                "verse": "Ledammo campos imboazare suos."
            }
        ],
        [
            {
                "verse": "Resultent \"io io\" coci, resultent"
            },
            {
                "verse": "Iamdudum queruli simul lebetes"
            },
            {
                "verse": "(Nuper nescio quid mali gemebant);"
            },
            {
                "verse": "Quin ipsae insiliant nigrae nigellis"
            },
            {
                "verse": "Ollae cum patinis, et huc et illuc"
            },
            {
                "verse": "Permixti urceolis catini et urnae"
            },
            {
                "verse": "Et quantum est olidissimae popinae,"
            },
            {
                "verse": "\"Io\" ter geminent chorosque ducant:"
            },
            {
                "verse": "Curis namque animi levantur aegris,"
            },
            {
                "verse": "Hostem namque alacres suum triumphant."
            },
