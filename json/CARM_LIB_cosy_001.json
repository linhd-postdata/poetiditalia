{
    "author": "carmina libraria",
    "title": "Constitutiones synodales ecclesiae Constantiniensis",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|cosy|001",
    "text": [
        [
            {
                "verse": "Ad reverendissimum Dominum Hugonem de Landenberg ecclesiae Constantiniensis episcopumelogium Sebastiani Brandt"
            }
        ],
        [
            {
                "verse": "Numine divino praesul celeberrime, sacrae"
            },
            {
                "verse": "Praefectus sedi. Stas populum ante tuum."
            },
            {
                "verse": "Praesul et antistes , speculator, episcopus, unde"
            },
            {
                "verse": "Diceris ut pastor sis populi atque pater."
            },
            {
                "verse": "Ad quod in officii monstras Hugo inclyte praesul"
            },
            {
                "verse": "Principio vigili cuncta labore parans."
            },
            {
                "verse": "Nam synodum celebras vitia atque errata reformas:"
            },
            {
                "verse": "Virtutumque iubes semitam adire tuos."
            },
            {
                "verse": "Haec nova et in primis scrutari rite statuta"
            },
            {
                "verse": "Praecipis, ad more condita proficuos."
            },
            {
                "verse": "Det tibi cunctipotens vitae mitissime praesul"
            },
            {
                "verse": "Tempora: tam sanctis consona principiis."
            }
        ]
    ]
}