{
    "author": "Francesco Pescennio Negri",
    "title": "ad Ioannem Fuxmanium",
    "url": "http://www.poetiditalia.it/texts/NEGRI_FP|fuch|001",
    "text": [
        [
            {
                "verse": "Merces Cimmerio nauita Bosphoro"
            },
            {
                "verse": "Deuectans Syrias, siue hyacinthinas"
            },
            {
                "verse": "Laenas, seu tyrianthinum"
            },
            {
                "verse": "Coccum, Santonicumue"
            },
            {
                "verse": "Cypro nauigio bardocucullulum:"
            },
            {
                "verse": "Aegaei rapidos uorticis impetus"
            },
            {
                "verse": "Tritoneque timet malos"
            },
            {
                "verse": "Saeuam Cymodocenque;"
            },
            {
                "verse": "Constans sed pelagi sustinet impii"
            },
            {
                "verse": "Anfractus uarios et rabiem Noti"
            },
            {
                "verse": "Et quicquid mare feruidum"
            },
            {
                "verse": "Monstri protulit atri,"
            },
            {
                "verse": "Suspectans placidi sidera phosphori,"
            },
            {
                "verse": "Quo cursus penitus dirigat inuios,"
            },
            {
                "verse": "Diuos seu Lacedaemonas"
            },
            {
                "verse": "Clara luce secundos,"
            },
            {
                "verse": "Ut portum capiens quassa carinula"
            },
            {
                "verse": "Optatum, teneris imbuat hostiis"
            },
            {
                "verse": "Dis sacrum bene cespitem"
            },
            {
                "verse": "Tutelaribus, aurae"
            },
            {
                "verse": "Felici placido munere laetior:"
            },
            {
                "verse": "Franciscus rapidos sic patitur Notos,"
            },
            {
                "verse": "Sic fluctu medio maris"
            },
            {
                "verse": "Iras imperiosi,"
            },
            {
                "verse": "Hinc Syrtes timet, hinc Acroceraunia"
            },
            {
                "verse": "Ambraciosque sinus et ferum Apollinem;"
            },
            {
                "verse": "Terrent Sithoniae niues"
            },
            {
                "verse": "Nutantem aequore puppim,"
            },
            {
                "verse": "Terrent et Scythici flamina Circii"
            },
            {
                "verse": "Et quicquid pelagi unda minax tulit;"
            },
            {
                "verse": "Et ni Lucifer optimus"
            },
            {
                "verse": "Errantem regat oram,"
            },
            {
                "verse": "Vel signa arctophylax tarda remiserit,"
            },
            {
                "verse": "Vel se Tyndaridae splendidiusculo"
            },
            {
                "verse": "Monstrarint mihi sidere,"
            },
            {
                "verse": "Fiet naufraga pinus."
            },
            {
                "verse": "Tu ergo solii gloria regii,"
            },
            {
                "verse": "Fuxmani, patriae stella intentior"
            },
            {
                "verse": "Virtutisque parens, tibi"
            },
            {
                "verse": "Sacratae auspice Phoebo,"
            },
            {
                "verse": "Cirrhaei penitus numinis optime"
            },
            {
                "verse": "Hospes, Pieridum praesidium, tuo"
            },
            {
                "verse": "Maecenas solidus Nigro,"
            },
            {
                "verse": "Quem fortuna fatigat:"
            },
            {
                "verse": "Eia nunc radiis splendidioribus"
            },
            {
                "verse": "Lucescas, rapidi ne mala marmoris"
            },
            {
                "verse": "Horrescens peream citus"
            },
            {
                "verse": "Magno non sine damno."
            },
            {
                "verse": "Tu mi Tyndareas porrigito faces,"
            },
            {
                "verse": "Tu saeuam remoue pauperiem Nigri,"
            },
            {
                "verse": "Quem si candidiusculo"
            },
            {
                "verse": "Tandem cernis ocello,"
            },
            {
                "verse": "Continget misero calculus albicans"
            },
            {
                "verse": "Et quandoque ratem portus amoenior"
            },
            {
                "verse": "Errantem capiet, bono"
            },
            {
                "verse": "Tandem sidere rectam;"
            },
            {
                "verse": "Nec dulce exodium defuerit tibi,"
            },
            {
                "verse": "Sed gratis fidibus nomen olympicum,"
            },
            {
                "verse": "Fuxmani, recinent bona"
            },
            {
                "verse": "Serui plectra Celaeni."
            }
        ],
        [
            {
                "verse": "Sancte regentum metuende coetus,"
            },
            {
                "verse": "Austriae splendor nitidumque sidus,"
            },
            {
                "verse": "Delphicae rupis specimen, probator"
            },
            {
                "verse": "Semper honesti;"
            },
            {
                "verse": "Iuris excellens utriusque templum,"
            },
            {
                "verse": "Candidus morum scelerumque censor,"
            },
            {
                "verse": "Alme uirtutis pater et amator"
            },
            {
                "verse": "Relligionis:"
            },
            {
                "verse": "Cui decus semper pietasque cordi est"
            },
            {
                "verse": "Et fides albo decorata panno"
            },
            {
                "verse": "Quaeque dilectum crepitans salutat"
            },
            {
                "verse": "Verbere nidum,"
            },
            {
                "verse": "Cuique facundo modulatur ore"
            },
            {
                "verse": "Lingua, tinnitu resonans amoeno,"
            },
            {
                "verse": "Cui sedent diuae placido Camoenae"
            },
            {
                "verse": "Pectore semper:"
            },
            {
                "verse": "Euge, quo plectro celebrare laudes"
            },
            {
                "verse": "Audeam tantas, uenerande coetus,"
            },
            {
                "verse": "Quo tuas digno memorem cothurno"
            },
            {
                "verse": "Conditiones?"
            },
            {
                "verse": "Pectus in tantos, modulos anhelum est"
            },
            {
                "verse": "Et cupit gratae penitus canoros"
            },
            {
                "verse": "Mentis ardescens amor et cupido"
            },
            {
                "verse": "Scribere uersus."
            },
            {
                "verse": "Magnus occurris tamen undecumque,"
            },
            {
                "verse": "Summus, excelsus, celeber potensque,"
            },
            {
                "verse": "Quo fit, ut rerum tenues fatiget"
            },
            {
                "verse": "Copia uires."
            },
            {
                "verse": "Tu decus uatum, pariterque Nysae"
            },
            {
                "verse": "Splendor ornatae placidusque tutor"
            },
            {
                "verse": "Delphicae sedis Clariaeque dignus"
            },
            {
                "verse": "Incola Cirrhae,"
            },
            {
                "verse": "Tu sapis quidquid sapuere prisci,"
            },
            {
                "verse": "Quicquid et docti penitus nouelli,"
            },
            {
                "verse": "Tu potes coetus ueterum sophorum"
            },
            {
                "verse": "Aequiparare,"
            },
            {
                "verse": "Te colunt nostri merito poetae,"
            },
            {
                "verse": "Te uel intonsi potuere patres,"
            },
            {
                "verse": "Sancte regentum, cecinisse, coetus,"
            },
            {
                "verse": "Carmine digno."
            },
            {
                "verse": "Sed quod est plectro grauiore dignum:"
            },
            {
                "verse": "Tanta facundo comitas in ore est,"
            },
            {
                "verse": "Tanta diuino probitas refulget"
            },
            {
                "verse": "Corde fidesque,"
            },
            {
                "verse": "Ut tibi prorsus cupiamus omnes"
            },
            {
                "verse": "Et dies laetos facilisque soles,"
            },
            {
                "verse": "Tempus et felix niueo lapillo"
            },
            {
                "verse": "Condecorandum."
            },
            {
                "verse": "Sed Niger fidens humilisque seruus"
            },
            {
                "verse": "Semper ardenti calamo sequetur"
            },
            {
                "verse": "Et tuas laudes nitidumque nomen"
            },
            {
                "verse": "Pectore firmo:"
            },
            {
                "verse": "Siue ad extremos penetrabit Indos,"
            },
            {
                "verse": "Unde nimbosus rapidusque Ganges"
            },
            {
                "verse": "In fretum praeceps ruit et minatur"
            },
            {
                "verse": "Unda procellam,"
            },
            {
                "verse": "Siue apud Seras Scythicosqe pigros,"
            },
            {
                "verse": "Sarmatas acris, gelidos Gelonos,"
            },
            {
                "verse": "Thraces, Armenos, celeresque tento"
            },
            {
                "verse": "Iazyges arcu,"
            },
            {
                "verse": "Siue ad angustum maris aduolabit"
            },
            {
                "verse": "Bosphorum uel quae loca protulere"
            },
            {
                "verse": "Caucasum tristem populosque saeuos"
            },
            {
                "verse": "Corpore mixto,"
            },
            {
                "verse": "Siue Germanos domitosque Cimbros"
            },
            {
                "verse": "Tandem et audaces Morinos, fidelem"
            },
            {
                "verse": "Regibus Gallum, docili Sicambros"
            },
            {
                "verse": "Crine decoros,"
            },
            {
                "verse": "Siue ad Hispanos triplices, Britannos"
            },
            {
                "verse": "Orbe diuisos, uigilem draconem"
            },
            {
                "verse": "Hortuli ditis Libycosque fines"
            },
            {
                "verse": "Seu Garamantes,"
            },
            {
                "verse": ".............."
            },
            {
                "verse": "Siue apud Manes Niger hic manebit,"
            },
            {
                "verse": "Semper aeterno memorique corde,"
            },
            {
                "verse": "Sancte Germani populi senatus,"
            },
            {
                "verse": "Te celebrabit;"
            },
            {
                "verse": "Semper et nostrae citharae lyraeque"
            },
            {
                "verse": "Barbitos, plectrum, calami, Camoenae"
            },
            {
                "verse": "Facta cantabunt ubicunque uestra,"
            },
            {
                "verse": "Credite Patres;"
            },
            {
                "verse": "Semper e nostris ouibus honesta"
            },
            {
                "verse": "Vota soluemus: meritis canentes"
            },
            {
                "verse": "Vocibus dicemus \"io triumphe,"
            },
            {
                "verse": "Euge triumphe!\""
            }
        ],
        [
            {
                "verse": "Haec iuga difficiles aditus tribuere, uiator,"
            },
            {
                "verse": "Et nunquam plaustro se uoluere premi."
            },
            {
                "verse": "Ast ego, qui docui montes parere superbos,"
            },
            {
                "verse": "Sigismundus, iter curribus usque dedi."
            },
            {
                "verse": "Austria me coluit coluitque Carinthia semper"
            },
            {
                "verse": "Archiducemque timet Stiria magna suum;"
            },
            {
                "verse": "Carniolaeque ducem celebrant comitemque Tirolis:"
            },
            {
                "verse": "Et pariter montes, flumina, plana timent."
            },
            {
                "verse": "Impulit haec fieri Graecorum fama, Caballus"
            },
            {
                "verse": "Antonius, noster qui modo quaestor erat;"
            },
            {
                "verse": "Praepositusque sacris Wolfgangus Brixina nostris"
            },
            {
                "verse": "Neundlinger tanto praefuit usque bono."
            }
        ]
    ]
}