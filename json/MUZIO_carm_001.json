{
    "author": "Girolamo Muzio",
    "title": "isagogicon",
    "url": "http://www.poetiditalia.it/texts/MUZIO|carm|001",
    "text": [
        [
            {
                "verse": "Quisquis Pierios colit recessus"
            },
            {
                "verse": "Et sacros latices amat beatus"
            },
            {
                "verse": "Praecingens hedera caput sequaci"
            },
            {
                "verse": "Adsit, dum numeros canunt poetae;"
            },
            {
                "verse": "Adsit, dum numeris fauent Camenae,"
            },
            {
                "verse": "Dum sacri Hoelcelium canunt poetae."
            },
            {
                "verse": "Doctus Pegasidum chorus sororum"
            },
            {
                "verse": "Terram ter pedibus quatit quaterque"
            },
            {
                "verse": "Saltans ad modulos, uicesque in orbe"
            },
            {
                "verse": "Alternant placidae modis iocosis."
            },
            {
                "verse": "At Phoebus cithara sonat canora"
            },
            {
                "verse": "Gestans temporibus comas amicae,"
            },
            {
                "verse": "Laurum, perpetuas comas puellae."
            },
            {
                "verse": "Plaudunt turba decens trium sororum"
            },
            {
                "verse": "Texuntque Hoelcelio suo coronam,"
            },
            {
                "verse": "Texunt ut decorent patrem piorum,"
            },
            {
                "verse": "Patremque et decus omnium bonorum."
            },
            {
                "verse": "Qui prudens, facilis, bonus, benignus,"
            },
            {
                "verse": "Auctor propositi tenax honesti,"
            },
            {
                "verse": "Ultor propositi mali seuerus,"
            },
            {
                "verse": "Spernit diuitias colitque Musas,"
            },
            {
                "verse": "Uates uersiculosque amat fouetque."
            },
            {
                "verse": "Hunc ergo, hunc merito canunt poetae,"
            },
            {
                "verse": "Adsint Aonides choris iocisque,"
            },
            {
                "verse": "Phoebus dulcisonum melos repulset,"
            },
            {
                "verse": "Innectant Charites rosasque serto"
            },
            {
                "verse": "Et serto Hoelcelio caput coronent."
            },
            {
                "verse": "Iam non Hoelcelius peribit unquam,"
            },
            {
                "verse": "Non is Tartareas adibit umbras"
            },
            {
                "verse": "Aut sedes Stygias lacusue Auerni."
            },
            {
                "verse": "Sed leti impatiens in omne tempus"
            },
            {
                "verse": "Uiuet carminibus legendus orbi:"
            },
            {
                "verse": "Qua caelo irradians diem recludit"
            },
            {
                "verse": "Sol, quaque Oceani lauatur unda;"
            },
            {
                "verse": "Et uiset Libyae, calentis arua"
            },
            {
                "verse": "Et duram glaciem poli rigentis."
            },
            {
                "verse": "Tandem caelitibus deis uocatus"
            },
            {
                "verse": "Inuiset superam beatus aulam,"
            },
            {
                "verse": "Felix ter quater et quater beatus."
            },
            {
                "verse": "Absit nenia flebilis sepulcro:"
            },
            {
                "verse": "Mordax inuidus hinc procul facessat;"
            },
            {
                "verse": "Adsint qui numeros canant poetae"
            },
            {
                "verse": "Et Musae Clariusque Gratiaeque."
            }
        ]
    ]
}