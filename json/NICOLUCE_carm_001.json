{
    "author": "Lorenzo Nicolucesi",
    "title": "carmen ad Varchium",
    "url": "http://www.poetiditalia.it/texts/NICOLUCE|carm|001",
    "text": [
        [
            {
                "verse": "ad Benedictum Varchium"
            },
            {
                "verse": "Facundissime mi, nepos Atlantis,"
            },
            {
                "verse": "Varchi candide, candidus peritum"
            },
            {
                "verse": "Quem chorus colit et virum potentum"
            },
            {
                "verse": "Magnitudo satis superque pendit,"
            },
            {
                "verse": "Ex quo tempore vidimus legendo"
            },
            {
                "verse": "Scripta tot tibi digna sempiterno"
            },
            {
                "verse": "A doctis celebrari honore, laude, et"
            },
            {
                "verse": "Tum semper cupidus tui videndi"
            },
            {
                "verse": "Et nunc sum meritam manum osculandi,"
            },
            {
                "verse": "Manum quam merito colunt diserti,"
            },
            {
                "verse": "Magni vel faciunt viri potentes."
            },
            {
                "verse": "O manus Latio suum nitorem,"
            },
            {
                "verse": "Quae reddis veterem atque dignitatem"
            },
            {
                "verse": "Pulsa barbarie tuum decorem!"
            },
            {
                "verse": "Quis nam pro meritis queat probare?"
            },
            {
                "verse": "O quam carmine quam vales soluta"
            },
            {
                "verse": "Dicendi ratione, sive Tusca"
            },
            {
                "verse": "Scribas, sive Latina, utraque sive"
            },
            {
                "verse": "Praesens, sic tibi saeculum obligatur."
            },
            {
                "verse": "Debent praeteritum ut suo Maroni"
            },
            {
                "verse": "Aphro, Tullio, Horatio et Catullo,"
            },
            {
                "verse": "Debent omnia saecula ut Petrarchae"
            },
            {
                "verse": "Praedocto, lepido, optimo poetae."
            },
            {
                "verse": "Ergo pro pietate singulari,"
            },
            {
                "verse": "Pro tua eximia benignitate,"
            },
            {
                "verse": "Pro bonis studiis scientiarum,"
            },
            {
                "verse": "Oro me ut numero velis patrone"
            },
            {
                "verse": "Servum ascribere candido tuorum."
            },
            {
                "verse": "Quod si conficies, ero beatus,"
            },
            {
                "verse": "Ethruscae decus o perenne linguae!"
            }
        ],
        [
            {
                "verse": "ad Laurentium Nicolocesium responsio"
            },
            {
                "verse": "Quis me stultior, obsecro, quis esset"
            },
            {
                "verse": "Immodestior impudentiorque?"
            },
            {
                "verse": "Quis risu mage dignus et cachinno?"
            },
            {
                "verse": "Si quae tu nimia benignitate"
            },
            {
                "verse": "Mihi, seu potius volens iocari,"
            },
            {
                "verse": "Das ipse, accipiam, velimque magnas"
            },
            {
                "verse": "Serio tibi gratias referre!"
            },
            {
                "verse": "Quare habe tibi quicquid est, quod in me"
            },
            {
                "verse": "Liberaliter, hercule, et diserto"
            },
            {
                "verse": "Sed falso tamen ore contulisti,"
            },
            {
                "verse": "Et me desine, quaeso, singularis"
            },
            {
                "verse": "Amice ac mihi carior medullis,"
            },
            {
                "verse": "Non amare quidem, sed immerentem"
            },
            {
                "verse": "Tot laudum cumulo gravare mendax."
            }
        ]
    ]
}