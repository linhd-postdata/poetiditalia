{
    "author": "carmina libraria",
    "title": "Iohannes Ionstonius, thaumatographia naturalis",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|ions|001",
    "text": [
        [
            {
                "verse": "ad Nobilem et Clarissimum virum Ioannem Ionstonium de suis philosophicis scriptis"
            }
        ],
        [
            {
                "verse": "Fontes venustos et sapores musteos,"
            },
            {
                "verse": "Aquas perennes limpidas, pellucidas"
            },
            {
                "verse": "Aevo salubres, saeculisque commodas"
            },
            {
                "verse": "Scaturiebat Virgo priscis patribus"
            },
            {
                "verse": "Sophiam venusto quam vocarunt nomine."
            },
            {
                "verse": "Haec illa Virgo proma-conda temporum"
            },
            {
                "verse": "Pudica, pulchra, casta, dives artium"
            },
            {
                "verse": "Magistra Veritatis, ordinis Parens,"
            },
            {
                "verse": "Humanitatis una formatrix rudis"
            },
            {
                "verse": "Et Architecta litterati seminis"
            },
            {
                "verse": "Analphabeto invita semper agmini."
            },
            {
                "verse": "Regina, regni compos esset dum sui"
            },
            {
                "verse": "Audivit inquinata nullis sordibus"
            },
            {
                "verse": "Libidinosis nota necdum adulteris."
            },
            {
                "verse": "Ut inter ignes aetheris Sol splenduit"
            },
            {
                "verse": "Suisque lucem contulit cultoribus."
            },
            {
                "verse": "Haec claruere Magna quondam Numina."
            },
            {
                "verse": "Divinus orbis ille doctor totius"
            },
            {
                "verse": "Insigne gentis Atticae miraculum,"
            },
            {
                "verse": "Et qui Magistrum paene praecessit suum"
            },
            {
                "verse": "Ortus Stagira  maximus Princeps sophum"
            },
            {
                "verse": "Cui comparari nemo tum mortalium"
            },
            {
                "verse": "Potis eruditione. Vel si quos adhuc"
            },
            {
                "verse": "Aetas beati temporis vetus tulit"
            },
            {
                "verse": "Successit aevum deneger prioribus"
            },
            {
                "verse": "Qua Virgo tandem prostituta adulteris"
            },
            {
                "verse": "Distorta, deformata, vulsis artubus"
            },
            {
                "verse": "Deruncinata; limpidis pro fontibus"
            },
            {
                "verse": "Ulvis lutosis cespitisque uligine"
            },
            {
                "verse": "Coeno, lacunis, putidis paludibus"
            },
            {
                "verse": "Et falsitatis ac amurcae fecibus"
            },
            {
                "verse": "Scatet. Scatebris hauriunt istis meras"
            },
            {
                "verse": "Apinas, inertes inficetiae strophas,"
            },
            {
                "verse": "Insanientis insipientiae logos"
            },
            {
                "verse": "Tricasque fucis, falsitatis fabricas."
            },
            {
                "verse": "Hinc tota mendax lena turpis et Lupa"
            },
            {
                "verse": "Infamis audit, audit Ossa sordium."
            },
            {
                "verse": "Eius misertus Ionstone, o aevi iubas"
            },
            {
                "verse": "Ocelle dexter eruditi saeculi"
            },
            {
                "verse": "Nunc expolis industrius ceu artifex"
            },
            {
                "verse": "Incude ab ipso, litterariae rei"
            },
            {
                "verse": "Facemque praefers Virgini suum decus"
            },
            {
                "verse": "Sophiae reponis, eieratis sordibus."
            },
            {
                "verse": "Patrocinaris veritatis sic viae"
            },
            {
                "verse": "Et sic salubrem sedulus sapientiam"
            },
            {
                "verse": "Stillas in usus, ne quid hic puertiam"
            },
            {
                "verse": "Moretur ultra vi fugace praecaves."
            },
            {
                "verse": "Nunc haurientur veritatis schemata,"
            },
            {
                "verse": "Exempla rerum, plana virtutum sacra,"
            },
            {
                "verse": "Lepidi lepores, gratiae gratae, chorus"
            },
            {
                "verse": "Lubentiarum, Amorum amoena factio"
            },
            {
                "verse": "Faventiaeque et elegantiae typus"
            },
            {
                "verse": "Ductore te. Quin eloquar quid sentiam?"
            },
            {
                "verse": "Me iudice uno et eruditis omnibus"
            },
            {
                "verse": "Mala fatente idem vel invidentia"
            },
            {
                "verse": "Fers omne punctum laudis et palmam meres."
            },
            {
                "verse": "Sic tute Philosophantium longe gregem"
            },
            {
                "verse": "Post terga linquis, Bos quibus linguam obsidet"
            },
            {
                "verse": "Quos scripturire scabra detinet sitis."
            },
            {
                "verse": "O macte tantis, macte o ingens bonis!"
            },
            {
                "verse": "Et dum licet prodesse, perge sedulo,"
            },
            {
                "verse": "Faciles ad arcem propera Sophiae vias."
            },
            {
                "verse": "Satura iuventae vota quo maturitas"
            },
            {
                "verse": "Sequatur uber mox adulto corpore."
            },
            {
                "verse": "Sic sic perennem promerebis gratiam,"
            },
            {
                "verse": "Sic te sequetur gloria et nomen tenax."
            }
        ],
        [
            {
                "verse": "Venceslaus Clemens a Libeo Monte S."
            }
        ]
    ]
}