{
    "author": "Orazio Prospero",
    "title": "carmen ad Varchium",
    "url": "http://www.poetiditalia.it/texts/PROSPERO|carm|001",
    "text": [
        [
            {
                "verse": "ad Benedictum Varchium"
            }
        ],
        [
            {
                "verse": "Varchi, Pieridum decus, voluptas,"
            },
            {
                "verse": "Qui es carissimus omnibus disertis,"
            },
            {
                "verse": "Sciens et Latiae, Atticaeque linguae,"
            },
            {
                "verse": "Paulus, ut decuit, volens, libensque"
            },
            {
                "verse": "Paratum sibi pharmacum voravit,"
            },
            {
                "verse": "Et post sic placide iacens quievit,"
            },
            {
                "verse": "Curis omnibus, ut solet, solutus."
            },
            {
                "verse": "Hinc spes est mihi, quod iam ut optat, omnem"
            },
            {
                "verse": "Liquorem expulerit, brevi et valebit,"
            },
            {
                "verse": "Omnes ut cupimus valere amici."
            },
            {
                "verse": "Te vero colit ut iure debet,"
            },
            {
                "verse": "Plurimi facit, et diserta verba"
            },
            {
                "verse": "Miratur tua et Atticos lepores,"
            },
            {
                "verse": "Primum et, quod placeat, vocat te amicum."
            },
            {
                "verse": "Ad me carmina, amice, quod dedisti,"
            },
            {
                "verse": "Quas agam tibi gratias merenti,"
            },
            {
                "verse": "Quaero, nec tamen invenire possum."
            },
            {
                "verse": "Hoc tantum loquar, accipe, et libenter"
            },
            {
                "verse": "Tuum me fore semper atque ubique."
            }
        ]
    ]
}