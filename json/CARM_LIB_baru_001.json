{
    "author": "carmina libraria",
    "title": "Bartholomaeus de Usingen, compendium naturalis philosophiae",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|baru|001",
    "text": [
        [
            {
                "verse": "ad lectorem"
            }
        ],
        [
            {
                "verse": "Vis varias rerum lector cognoscere causas,"
            },
            {
                "verse": "Vis breve naturae fertilioris opus."
            },
            {
                "verse": "Huc ades, invenies quod te iuvet haec tibi lector"
            },
            {
                "verse": "Pandimus a veteri scripta redempta cabo."
            }
        ],
        [
            {
                "verse": "tetrasticon B T"
            }
        ],
        [
            {
                "verse": "Syrmata longa patres, glossemata spurca legebant"
            },
            {
                "verse": "Centauri multi multa chymera fuit."
            },
            {
                "verse": "Clausum naturae specimen monumenta perennis"
            },
            {
                "verse": "Clara regesta sonant Bartholomee tua."
            }
        ],
        [
            {
                "verse": "tetrasticon E D"
            }
        ],
        [
            {
                "verse": "Sicut odoriferi circum vineta Timoli"
            },
            {
                "verse": "Fervet inexpleti plena laboris apis."
            },
            {
                "verse": "Sic naturalis sophiae penetralia lustrans"
            },
            {
                "verse": "Pandit obumbratum Bartolomaeus iter."
            },
            {
                "verse": "Sparsa per innumeros quondam natura libellos"
            },
            {
                "verse": "Omnis in hoc lector codice clausa latet"
            }
        ]
    ]
}