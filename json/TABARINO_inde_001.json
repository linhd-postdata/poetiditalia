{
    "author": "Giovanni Mattia Tabarino",
    "title": "carmen ad Inderbacchium",
    "url": "http://www.poetiditalia.it/texts/TABARINO|inde|001",
    "text": [
        [
            {
                "verse": "divo Ioanni Inderbacchio antistiti Tridentino"
            }
        ],
        [
            {
                "verse": "Carmine perpetuum Moecenas nomen adeptus,"
            },
            {
                "verse": "Virgilii numeris Pollio semper erit,"
            },
            {
                "verse": "Aeternumque tibi decus, Inderbacche, dicavit"
            },
            {
                "verse": "Carmine, qui titulos sculpsit in aere tuos."
            },
            {
                "verse": "Hoc qui amagnanimi generosum pectus et ingens"
            },
            {
                "verse": "Cor Moecenatis, splendide, solus habes,"
            },
            {
                "verse": "Principe nil potuit tribui tibi dignius alto,"
            },
            {
                "verse": "Cum sint aethereae carmina mentis opus."
            },
            {
                "verse": "Convenit ergo sacro divini Musa poetae"
            },
            {
                "verse": "Haec tibi: tu doctus, gloria et orbis honos,"
            },
            {
                "verse": "Imperiumque tibi speramus maius habendum,"
            },
            {
                "verse": "Si iusta superi pondera lance premunt."
            }
        ]
    ]
}