{
    "author": "Giorgio Sisgoreo",
    "title": "carmina Ambrosiana",
    "url": "http://www.poetiditalia.it/texts/SISGOREO|ambr|001",
    "text": [
        [
            {
                "verse": "Carpo ego ficedulas, lepores, conchilia, rombos,"
            },
            {
                "verse": "Non edo, quod carpo: sum miser, alter edit."
            }
        ],
        [
            {
                "verse": "Perlege, qui transis, carmen breve, docte viator."
            },
            {
                "verse": "Sarcophagus tenet hic Gorgida pontificem."
            }
        ],
        [
            {
                "verse": "Uva haec Pyeriis calcatur saepe puellis,"
            },
            {
                "verse": "Haec spuit inclusum nectaris uva merum."
            },
            {
                "verse": "Dignus es et Musis et sacro nectare dignus,"
            },
            {
                "verse": "Nunc dignus gustes; si sapit, Azzo, refer."
            }
        ],
        [
            {
                "verse": "Perdulces uvae, perdulcia carmina, Gorgi,"
            },
            {
                "verse": "Ut nectar mentem corripuere meam."
            },
            {
                "verse": "Felix, qui niveas potis es concernere formas,"
            },
            {
                "verse": "Dum calcat mustum lubrica planta novum."
            }
        ]
    ]
}