            {
                "verse": "Syderis instar,"
            },
            {
                "verse": "Solvimus grates Superum parenti,"
            },
            {
                "verse": "Ad magistratum modo qui vocavit"
            },
            {
                "verse": "Urbis in nostrae laribus paternis"
