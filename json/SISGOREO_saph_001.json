            {
                "verse": "Te, bone praetor."
            },
            {
                "verse": "Pindari linguae fluvios disertae"
            },
            {
                "verse": "Posco praeturam cupiens sonare,"
            },
            {
                "verse": "Quam geris solers, animo modesto"
