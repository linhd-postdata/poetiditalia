            {
                "verse": "Cultor, illustris procerumque amator,"
            },
            {
                "verse": "Tu decus doctae specimenque turbae,"
            },
            {
                "verse": "Inclyte praetor."
            },
            {
                "verse": "Tu colis vatum graciles Camoenas,"
            },
            {
                "verse": "Tu sapis Phoebum redolesque laurum,"
