{
    "author": "Bovetino Bovetini",
    "title": "carmen",
    "url": "http://www.poetiditalia.it/texts/BOVETINO|carm|001",
    "text": [
        [
            {
                "verse": "Musula Bovetide, genuit quem patria vatis,"
            },
            {
                "verse": "Muxatum poscas, venerande Palladis alme"
            },
            {
                "verse": "Munere fulgentem, tunc visso funde querelas,"
            },
            {
                "verse": "Tunc tortura manus conodet, in ethera clamor"
            },
            {
                "verse": "Tunc eat et pallor tua tunc exterminet ora"
            },
            {
                "verse": "Morte Leonardi, patrie qui iura fovere"
            },
            {
                "verse": "Posse suo nititur libertatemque tueri"
            },
            {
                "verse": "Milicie populique simul de morte Maionis,"
            },
            {
                "verse": "Dicque suo mittat Bonicontro carmina, sodes,"
            },
            {
                "verse": "Dicta Leonardo Guizemani dicta poete"
            },
            {
                "verse": "Dardanidum. \"Versus, nunquam memorande, sodales"
            },
            {
                "verse": "Post ullos, vultu Bonicontri, dicito, talli"
            },
            {
                "verse": "Suscipe quallis erat, secum cum sacra colebas"
            },
            {
                "verse": "Palladis, est ulli que non similanda dearum\"."
            }
        ]
    ]
}