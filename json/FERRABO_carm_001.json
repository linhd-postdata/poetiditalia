{
    "author": "Giovanni Andrea Ferrab\u00f2",
    "title": "carmina",
    "url": "http://www.poetiditalia.it/texts/FERRABO|carm|001",
    "text": [
        [
            {
                "verse": "Felices multos audivimus, inclite Princeps,"
            },
            {
                "verse": "Atque id non una nec ratione pari."
            },
            {
                "verse": "Est alius generis celso qui ducat Olympo"
            },
            {
                "verse": "Principium, summus nobilitate sua;"
            },
            {
                "verse": "Est alius patria primus, qui gaudeat urbe"
            },
            {
                "verse": "Roma, vel si qua est maior in Hesperia."
            },
            {
                "verse": "Possidet hic pingues agros, hic aurea tecta,"
            },
            {
                "verse": "Sidonias vestes, ditia regna Midae."
            },
            {
                "verse": "Est coniunx alii, qua non formosior ulla,"
            },
            {
                "verse": "Et cui non laesa labe pudicitiae."
            },
            {
                "verse": "Et placidi mores et sancta in pectore virtus"
            },
            {
                "verse": "Est aliis et cum religione fides."
            },
            {
                "verse": "Hos tulit ad caelum gestarum gloria rerum"
            },
            {
                "verse": "Gadibus ad Gangem bellaque nota viris."
            },
            {
                "verse": "Est qui doctrinam sectetur, et omnibus horis"
            },
            {
                "verse": "Exornet magnis ingenium studiis."
            },
            {
                "verse": "Diversis igitur causis felix homo; quisque"
            },
            {
                "verse": "Laetatur rebus tempus in omne suis,"
            },
            {
                "verse": "Sed non una tibi causa est: sunt mille: quis ergo"
            },
            {
                "verse": "Felix esse magis te, Federice, potest?"
            },
            {
                "verse": "Et genus et patria et uxor castissima pulchra,"
            },
            {
                "verse": "Divitiae mores religio atque decus."
            },
            {
                "verse": "Cana fides, magna nulli virtute secundus,"
            },
            {
                "verse": "Bellica nec cuiquam gloria tanta fuit."
            },
            {
                "verse": "Doctior es doctis praestans invictus ubique:"
            },
            {
                "verse": "Felix ergo magis non, Federice, alius."
            }
        ],
        [
            {
                "verse": "Non tam laeta suum spectavit Roma Camillum"
            },
            {
                "verse": "Quum victis Gallis venerat in patriam;"
            },
            {
                "verse": "Non sic ipsa ducem, versa Carthagine, magnum,"
            },
            {
                "verse": "Non alium qui vis auxerit imperium,"
            },
            {
                "verse": "Parthenope Calabrum nostro non tempore regem,"
            },
            {
                "verse": "Quum Gallos iterum iecit ab Italia,"
            },
            {
                "verse": "Quam tuus, o princeps, populus te conspicit omnis:"
            },
            {
                "verse": "Laetitiam monstrant urbs domus et paries."
            }
        ],
        [
            {
                "verse": "Pallens Picena turdis donatur oliva"
            },
            {
                "verse": "Et pingues alias rusticus addit aves;"
            },
            {
                "verse": "Tubera vimineo mittit velata canistro"
            },
            {
                "verse": "Et poma Herculeo digna labore peti;"
            },
            {
                "verse": "Castaneas et quales olim Amaryllis amavit"
            },
            {
                "verse": "Alter et a Tusco stipite missa pira"
            },
            {
                "verse": "Et saturas Umbro iam dudum lacte mariscas"
            },
            {
                "verse": "Malaque quis nomen Punica terra dedit:"
            },
            {
                "verse": "Quum sibi non sit avis, teneat nec rustica dona,"
            },
            {
                "verse": "Cor Ferabos animum ingeniumque tradit."
            },
            {
                "verse": "Phillyrides curat Phoenicis lumina Chiron,"
            },
            {
                "verse": "Alter quod telo vulnus ab Herculeo:"
            },
            {
                "verse": "Urbinas Princeps, quo non humanior ullus,"
            },
            {
                "verse": "Sanavit vatis munere pauperiem,"
            },
            {
                "verse": "Quod rapit hora sequens; ast tu, Federice, dedisti:"
            },
            {
                "verse": "Aeternum Ferabos perpetuumque dabit,"
            },
            {
                "verse": "Quum tua victuris committet nomina chartis:"
            },
            {
                "verse": "Non minuent laudes saecula longa tuas."
            }
        ],
        [
            {
                "verse": "O Urbini decus, o splendida gloria"
            },
            {
                "verse": "Regni, Castaliis dux pater et viris,"
            },
            {
                "verse": "Non credas animum vivere, supplico,"
            },
            {
                "verse": "Torpentem mihi: iam nulla diu legis"
            },
            {
                "verse": "Nostris edita quod carmina fontibus,"
            },
            {
                "verse": "Non vatem leviter sic tua numina"
            },
            {
                "verse": "Tangunt quae Clarii sunt tibi numinis"
            },
            {
                "verse": "Instar, non adeo vel ducis immemor"
            },
            {
                "verse": "Ducit nostra suos Calliope dies,"
            },
            {
                "verse": "Feltrani generis delitiae sui."
            },
            {
                "verse": "Nam nec nos elegis ludimus amplius,"
            },
            {
                "verse": "Nec versu tenero mascula scribere"
            },
            {
                "verse": "Regum gesta decet: maius opus quoque"
            },
            {
                "verse": "Aetas nostra petit; et magis ubere"
            },
            {
                "verse": "Vena iam historiae firmata spargimus"
            },
            {
                "verse": "Longa et per titulos vela damus tuos."
            },
            {
                "verse": "Hic curas animum magna vel ocia"
            },
            {
                "verse": "Ponam quae facies, Dux placidissime."
            }
        ],
        [
            {
                "verse": "Iam ducis splendor Ligurum, potentis,"
            },
            {
                "verse": "Praesulis summi decus atque virtus,"
            },
            {
                "verse": "Appuli regis Federicus alti"
            },
            {
                "verse": "Induperator;"
            },
            {
                "verse": "Quem putant aula supera remissum,"
            },
            {
                "verse": "Quo pater caelo nihilum deorum"
            },
            {
                "verse": "Cernat ex illo nisi iam subactas"
            },
            {
                "verse": "Principe terras;"
            },
            {
                "verse": "Cuius exoptant Itali favorem"
            },
            {
                "verse": "Hostis et cuius trepidat furorem"
            },
            {
                "verse": "Victor ad primam remeat triumphans"
            },
            {
                "verse": "Imperii urbem."
            },
            {
                "verse": "Ergo festantes iuvenum catervae"
            },
            {
                "verse": "Myrtheas iactant plateis corollas,"
            },
            {
                "verse": "Unidique et tecti superat cacumen"
            },
            {
                "verse": "Delphica laurus;"
            },
            {
                "verse": "Cernimus (passim vigeant corymbi),"
            },
            {
                "verse": "Cernimus florum varios colores;"
            },
            {
                "verse": "Feltri ad magnos comitis paratus"
            },
            {
                "verse": "Undique sparsos;"
            },
            {
                "verse": "Tendit et palmas puer et puella,"
            },
            {
                "verse": "Aureum quaerit pluvium per auras"
            },
            {
                "verse": "Terra iam magnis resonatque caelum"
            },
            {
                "verse": "Vocibus omne."
            },
            {
                "verse": "Signa nosco purpureis equorum"
            },
            {
                "verse": "Insita in loris capiti, lapillos"
            },
            {
                "verse": "Coccinum nobis rutilante ripa"
            },
            {
                "verse": "Quos mare donat."
            },
            {
                "verse": "Fulget et lauro religatus ipse:"
            },
            {
                "verse": "<>"
            },
            {
                "verse": "Civitas gaudens, io Federice,"
            },
            {
                "verse": "Caesar et alter."
            },
            {
                "verse": "Hic dies felix io, Federice,"
            },
            {
                "verse": "Detrahet curas, io Federice,"
            },
            {
                "verse": "Vivimus tuti, timor omnis esto,"
            },
            {
                "verse": "Te duce, longe"
            },
            {
                "verse": "Ante iam cedit niveis lacernis"
            },
            {
                "verse": "Ordoque et sacros humeros sequetur"
            },
            {
                "verse": "Ut frequens lato populus prematur"
            },
            {
                "verse": "Tramite laetus."
            },
            {
                "verse": "Non Acarnanus magica vel arte"
            },
            {
                "verse": "Herculis vitans furibundus ictus,"
            },
            {
                "verse": "Gloriam tanti cecinit triumphi"
            },
            {
                "verse": "Tempore ab illo;"
            },
            {
                "verse": "Telemus numquam truce puniendum"
            },
            {
                "verse": "Ante Cyclopem digitis Ulyssis;"
            },
            {
                "verse": "Sicut et frontem feriendus hasta,"
            },
            {
                "verse": "Dixeris, acris"
            },
            {
                "verse": "Filias non helleboro Melampus"
            },
            {
                "verse": "Mente qui Proeti medicat benigna"
            },
            {
                "verse": "Spiritu Phoebi magis et repletus"
            },
            {
                "verse": "Non adides."
            },
            {
                "verse": "Augur haud dixit magicusve quisquam"
            },
            {
                "verse": "Ast ego, dum regem animo feroci"
            },
            {
                "verse": "Feltrium forti videam, dedissem"
            },
            {
                "verse": "Sub pede monstra."
            },
            {
                "verse": "Barbaros statim placuit per hostes"
            },
            {
                "verse": "Castra ducturum et facile in catenas"
            },
            {
                "verse": "Perfidos omnes rabidumque secum"
            },
            {
                "verse": "Vertere Turcum."
            },
            {
                "verse": "Turba nunc ardens igitur sequatur"
            },
            {
                "verse": "Imperatorem celebri superbum"
            },
            {
                "verse": "Classe quo Martis modo cumque mavult"
            },
            {
                "verse": "Flectere signa;"
            },
            {
                "verse": "Seu plagas quaerat gelidis pruinis"
            },
            {
                "verse": "Horridas seu per mediam feratur"
            },
            {
                "verse": "Solis exustam nimio calore,"
            },
            {
                "verse": "Puppe vel alta"
            },
            {
                "verse": "Cicladum littus veniat secando"
            },
            {
                "verse": "Seu minas ponti metuens furentis"
            },
            {
                "verse": "Bosphori cernat fremitum sonantis"
            },
            {
                "verse": "Milite multo;"
            },
            {
                "verse": "Longius flectat Libyes ad oras"
            },
            {
                "verse": "Torridae currens oculos vel ipse"
            },
            {
                "verse": "Caucasi frigus videat nivalis"
            },
            {
                "verse": "Acer et audens."
            },
            {
                "verse": "Nunc Iovi centum dabimus iuvencos"
            },
            {
                "verse": "Firmet ut recte cecinit poeta."
            },
            {
                "verse": "Tollet a nobis meritos honores"
            },
            {
                "verse": "Pulcher Apollo."
            },
            {
                "verse": "Candido lucem celebret capillo"
            },
            {
                "verse": "Omnis ac aris pia tura sacris"
            },
            {
                "verse": "Reddat Urbinas populus per urbem"
            },
            {
                "verse": "Dignaque vota."
            },
            {
                "verse": "Hic dies festus redeat quotannis"
            },
            {
                "verse": "Semper et cuncti decorent minores,"
            },
            {
                "verse": "Quo datus nostris Federicus oris"
            },
            {
                "verse": "Axe secundo."
            },
            {
                "verse": "Tum manu quassa celeri sonora"
            },
            {
                "verse": "Tympana haud ullae taceant tubaeque"
            },
            {
                "verse": "Principis laudes pariter canantur"
            },
            {
                "verse": "Vocibus altis."
            },
            {
                "verse": "Concutat letis pedibus solumque"
            },
            {
                "verse": "Turba maiorum, iuvenes, puellae"
            },
            {
                "verse": "Gaudeant cives celebri triumpho"
            },
            {
                "verse": "Omne per aevum;"
            },
            {
                "verse": "Videat longos Federicus annos;"
            },
            {
                "verse": "Ille Tithonis videat senectam,"
            },
            {
                "verse": "Vivat et rerum dominus paterque"
            },
            {
                "verse": "Salus in orbem."
            }
        ]
    ]
}