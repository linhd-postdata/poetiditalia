{
    "author": "Lovato Lovati",
    "title": "carmina Mussati vel Lovati",
    "url": "http://www.poetiditalia.it/texts/LOVATI|dubi|001",
    "text": [
        [
            {
                "verse": "Transtulit Hectoreas studium, bone Iambone, proles,"
            },
            {
                "verse": "Nos alienigenae progenuere patres."
            },
            {
                "verse": "Serviles animos dominae fecere monetae,"
            },
            {
                "verse": "Quas patriae praefert immoderatus amor"
            },
            {
                "verse": "De sale quid dicam? sal nec sapientia nobis"
            },
            {
                "verse": "Adveniunt: fabricat iam sua claustra Sinon."
            }
        ],
        [
            {
                "verse": "Gracula si mater non occurrisset eunti,"
            },
            {
                "verse": "Graculus inceptum continuasset iter."
            },
            {
                "verse": "Ibat ut exueret nigras teterrimus alas;"
            },
            {
                "verse": "Obstitit et rauco gutture verba dedit:"
            },
            {
                "verse": "Quo properas, mi nate? redi: tibi fata minantur:"
            },
            {
                "verse": "Ne pereas, cursus ora reflecte tui."
            },
            {
                "verse": "Territus ad rapidi revolasse cubilia nidi"
            },
            {
                "verse": "Fertur et in pluma delituisse sua."
            }
        ]
    ]
}