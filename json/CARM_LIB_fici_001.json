{
    "author": "carmina libraria",
    "title": "Marsilius Ficinus, opera",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|fici|001",
    "text": [
        [
            {
                "verse": "Michael Petreius Hammelburgensis lectori salutem"
            }
        ],
        [
            {
                "verse": "Quisquis ades doctique vides monumenta laboris"
            },
            {
                "verse": "Et pia sed prisci temporis acta legis."
            },
            {
                "verse": "Ingenium authoris merito digneris honore"
            },
            {
                "verse": "Et doctum grata suscipe mente librum."
            },
            {
                "verse": "Insignis iuvenis clarus nullisque secundus"
            },
            {
                "verse": "Arte ubi tantum prodidit Author honor."
            },
            {
                "verse": "Quod cum virtutis laudes almae pietatis"
            },
            {
                "verse": "Tum solidae flores utilitatis habet."
            },
            {
                "verse": "Quae fuerant etenim summo quaerenda labore,"
            },
            {
                "verse": "Quae clemens virtus te liber iste docet."
            },
            {
                "verse": "Quicquid divinis scriptum fuit undique libris"
            },
            {
                "verse": "Quod sacra iura docent, quod medicina potest;"
            },
            {
                "verse": "Quicquid Marsilius scripsit sapientiae amator"
            },
            {
                "verse": "Qui Phoebum coluit Pegasidesque Deas,"
            },
            {
                "verse": "Cuncta sub hoc libro cernis nunc aere sonante"
            },
            {
                "verse": "Atque studes tenera si legis arte. Vale."
            }
        ]
    ]
}