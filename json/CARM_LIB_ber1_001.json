{
    "author": "carmina libraria",
    "title": "Philippus Beroaldus, annotationes centum",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|ber1|001",
    "text": [
        [
            {
                "verse": "Habes adolescens ornatissime has tamquam primicias frugum mearum. Deinceps maturiores fructus accipies. Vale et nostrum hunc libellum boni consule et hoc hendecasyllabon si non sordescit edisce."
            }
        ],
        [
            {
                "verse": "Uldrici nitidos lares adite"
            },
            {
                "verse": "Centenae simul annotationes"
            },
            {
                "verse": "Atque a limine protinus salutem"
            },
            {
                "verse": "Centenam date regulo rosensi"
            },
            {
                "verse": "Quo nil candidius dedere terrae"
            },
            {
                "verse": "Arctoae populis diu latinis,"
            },
            {
                "verse": "Qui comis facilis libens benignus"
            },
            {
                "verse": "Vos vultu excipiet sereniore,"
            },
            {
                "verse": "Vos larga premet osculatione,"
            },
            {
                "verse": "Vos et perleget et manu tenebit."
            },
            {
                "verse": "Illi nomine moc meo agminatim"
            },
            {
                "verse": "Hec mandata referte diligenter."
            },
            {
                "verse": "O gentis decus inclytum rosensis"
            },
            {
                "verse": "O rara probitate nominande"
            },
            {
                "verse": "Auceps Phasiacas dabit volucres,"
            },
            {
                "verse": "Ventator capreas lepusculosque"
            },
            {
                "verse": "Piscator maris amniunque praedam."
            },
            {
                "verse": "Merces institor hortulanus erbas."
            },
            {
                "verse": "Sic mittet sua quisque sic Philippus"
            },
            {
                "verse": "Noster conditor et parens patrono"
            },
            {
                "verse": "Nos donat tibi. Tu novas clientes"
            },
            {
                "verse": "Sub patrocinio tuo tuere."
            },
            {
                "verse": "Uni nos tibi mittimur dicamur,"
            },
            {
                "verse": "Uni scribimur, omnibus legemur."
            },
            {
                "verse": "Uldrici nitidos lares adite"
            },
            {
                "verse": "Centenae simul annotationes."
            }
        ]
    ]
}