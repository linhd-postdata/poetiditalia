{
    "author": "carmina libraria",
    "title": "Statius, Thebais",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|sta3|001",
    "text": [
        [
            {
                "verse": "nobili adolescenti Ioanni Barclaio"
            },
            {
                "verse": "Quales autumno fructus tibi proferat aetas"
            },
            {
                "verse": "Tales hoc flores quae tibi vere tulit."
            }
        ],
        [],
        [
            {
                "verse": "Philippus D'Aremberghe"
            }
        ],
        [
            {
                "verse": "Stabat in ignavo Statius paedore tabernae"
            },
            {
                "verse": "Nec poterat nido liber abire suo;"
            },
            {
                "verse": "Stabat sollicitae mersus caligine noctis"
            },
            {
                "verse": "Nec poterat docto laetus ab ore legi."
            },
            {
                "verse": "Barclaius postquam celeres attexuit alas"
            },
            {
                "verse": "Iam non stat Statius, sed super astra volat."
            }
        ],
        [],
        [
            {
                "verse": "ad Statium Ioanni Barclai commentario exornatum"
            }
        ],
        [
            {
                "verse": "Quid te, poeta summe, subtili tegis"
            },
            {
                "verse": "Sub nocte radiis nubila obtendens tui;"
            },
            {
                "verse": "I, quaere acutas, ut voles tenebras cape"
            },
            {
                "verse": "Inusitate vestis insolentiam;"
            },
            {
                "verse": "Barclaius imo pectore arcana eruens"
            },
            {
                "verse": "Te veste spoliat turbida ac nudum omnibus"
            },
            {
                "verse": "Praebet videndum: puncta notaque imprimit"
            },
            {
                "verse": "Deinde nudo pectori; noli tamen"
            },
            {
                "verse": "Pudore vultum tegere cum nudum aspicis."
            },
            {
                "verse": "Nam docta cum te nudat et notat manus"
            },
            {
                "verse": "Micantiori vestesine labe induit."
            }
        ],
        [],
        [
            {
                "verse": "N. Romano IVD"
            }
        ],
        [
            {
                "verse": "Aliud eiusdem ad ioannem Barclaium"
            },
            {
                "verse": "Nuper sollicite vetus sodali"
            },
            {
                "verse": "Fixis in te oculis rogabat, ecquid"
            },
            {
                "verse": "Barclaius faciet gravi senecta"
            },
            {
                "verse": "Si tanta haec tenera facit iuventa"
            },
            {
                "verse": "Nomen perpetuum sibi ac honores"
            },
            {
                "verse": "Barclaius parat et decus supremum"
            },
            {
                "verse": "Hoc ipsum faciet gravi senecta;"
            },
            {
                "verse": "Sic unum iuvenis facit senexque."
            }
        ],
        [],
        [
            {
                "verse": "de eodem"
            }
        ],
        [
            {
                "verse": "Desinet Herculeam mirari forte iuventam;"
            },
            {
                "verse": "Barclai iuvenis qui monumenta leget;"
            },
            {
                "verse": "Ille quidem magnos, sed adulta aetate labores"
            },
            {
                "verse": "Pertulit, afflictos dum iuvat arte manu."
            },
            {
                "verse": "At puer hic lustrat graios Latiosque penates"
            },
            {
                "verse": "Lumen ut obscuris praeferat ingeniis;"
            },
            {
                "verse": "Plenaque pubertas nondum lanugine malas"
            },
            {
                "verse": "Vestit et exequitur munia digna viro."
            },
            {
                "verse": "Iac. de Flavigni"
            }
        ],
        [],
        [
            {
                "verse": "ad eundem"
            }
        ],
        [
            {
                "verse": "Quod Statium sibi stare facis, barclaie, poetam"
            },
            {
                "verse": "Et Statii doctis das tua dicta libris;"
            },
            {
                "verse": "Crede mihi: per te Statius stat tuque per ipsum"
            },
            {
                "verse": "Tamque diu stabis, stabit ut ipse diu."
            }
        ]
    ]
}