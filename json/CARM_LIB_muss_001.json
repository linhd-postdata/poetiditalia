{
    "author": "carmina libraria",
    "title": "Petri Mussonii Virdunensis tragoediae",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|muss|001",
    "text": [
        [
            {
                "verse": "ad Clarissimum et admodum Reverendum D. Rodulphum Gazilium Doctorem Sorbonicum Collegii Aurelianensis Societatis Iesu Fundatorem explicatio Superioris Tabulae"
            }
        ],
        [
            {
                "verse": "Dum claustra Gallo Romula milite"
            },
            {
                "verse": "Prima tuetur militia puer"
            },
            {
                "verse": "Martinus et Persis tremenda"
            },
            {
                "verse": "Pannonius movet arma tyro;"
            },
            {
                "verse": "Campoque anhelum cornipedem fodit"
            },
            {
                "verse": "Spectante turma Bucephalum velut"
            },
            {
                "verse": "Ludens apricantem domabat"
            },
            {
                "verse": "Aemathius melior parente."
            },
            {
                "verse": "Repente pleno gramine pauperis"
            },
            {
                "verse": "Adstat cadaver, cui cutis horrida"
            },
            {
                "verse": "Contracta ruga, semihiantes"
            },
            {
                "verse": "Orbe rotant oculi reducto."
            },
            {
                "verse": "Livore totos perpetuo lues"
            },
            {
                "verse": "Incrustat artus machina tibiam"
            },
            {
                "verse": "Infida fulcit, nec minus qua"
            },
            {
                "verse": "Integer est, metuit ruinam."
            },
            {
                "verse": "Ergo severa lege necessitas"
            },
            {
                "verse": "Precatur assem, quo media endromis,"
            },
            {
                "verse": "Quo vile crustum iam minantis"
            },
            {
                "verse": "Tardet adhuc mala tela fati."
            },
            {
                "verse": "At ense tyro non gravior levi,"
            },
            {
                "verse": "Nudaque onustus non mage parmula,"
            },
            {
                "verse": "Qua parte ieiuni latratum"
            },
            {
                "verse": "Contineat rabiemque ventris"
            },
            {
                "verse": "Mendica caelo vota suadeat"
            },
            {
                "verse": "Narrare? Vultu exterminet aspero?"
            },
            {
                "verse": "Aut dum querelas larva cantat"
            },
            {
                "verse": "Terque quaterque nigros bacillo"
            },
            {
                "verse": "Cruentet artus? An vacuo aequore"
            },
            {
                "verse": "Laetus citato quadrupede emicet:"
            },
            {
                "verse": "Ruatque sustentetque et aret"
            },
            {
                "verse": "Gyret agens habilis per orbes?"
            },
            {
                "verse": "At non iuventus, esuriem fugat"
            },
            {
                "verse": "Spectata campo. Reiicere, heu pudet;"
            },
            {
                "verse": "Frustra laborat posse; frustra"
            },
            {
                "verse": "Pauperiem dolor est coactum."
            },
            {
                "verse": "Consulta fundo zona ter intimo"
            },
            {
                "verse": "Votum fefellit; lumine ter polum"
            },
            {
                "verse": "Suspexit irato ter acri"
            },
            {
                "verse": "Bile tumens iecur incitavit."
            },
            {
                "verse": "Dum cura pectus nobile dividit"
            },
            {
                "verse": "Sagum susurrans lene Favonius"
            },
            {
                "verse": "Subit repente, et purpurati"
            },
            {
                "verse": "Orbiculat pretiosa limbi."
            },
            {
                "verse": "Hic tum recepto numine fervidus,"
            },
            {
                "verse": "Vagina iberam ducit yapigem,"
            },
            {
                "verse": "Levaque ruptam parte vestem"
            },
            {
                "verse": "Ingerit increpitans timenti."
            },
            {
                "verse": "Grates parabat. Diffugit ocius"
            },
            {
                "verse": "Volante penna, seque manipulo"
            },
            {
                "verse": "Reddit gregali, mox futurus"
            },
            {
                "verse": "Opprobrium ioculare turmae."
            },
            {
                "verse": "O si qua coelo te pietas videt,"
            },
            {
                "verse": "Illustris heros, eveniat dies"
            },
            {
                "verse": "Qua te paludatum triumpho"
            },
            {
                "verse": "Aethereae vehat aula Romae!"
            },
            {
                "verse": "Tali fugacem prosequitur prece"
            },
            {
                "verse": "Piisque malas fletibus irrigat"
            },
            {
                "verse": "Addixit astris Christus; inde"
            },
            {
                "verse": "Bis Tyrio saturata succo,"
            },
            {
                "verse": "Noctemque vincens irradiat chlamys,"
            },
            {
                "verse": "Caeli aeviterno condita scrinio"
            },
            {
                "verse": "Tellure vox omni repostas"
            },
            {
                "verse": "Dum excitet imperiosa gentes."
            },
            {
                "verse": "Rodulphe, quis te hac pinxit imagine"
            },
            {
                "verse": "Sancte latentem? Qualiter aegida"
            },
            {
                "verse": "Bona occupavit fraude ductam"
            },
            {
                "verse": "Phydiaci nova forma caeli."
            },
            {
                "verse": "Te clara virtus extulerat sacras"
            },
            {
                "verse": "Martini ad aras; praepositus Patres"
            },
            {
                "verse": "Ad iura cogebas verendos,"
            },
            {
                "verse": "Idem hominum medius Deique."
            },
            {
                "verse": "At pulchra rerum spernere prodigus"
            },
            {
                "verse": "Praebenda muta, quo superindui"
            },
            {
                "verse": "Possit secundis abdicato"
            },
            {
                "verse": "Pauperies animosa mundo."
            },
            {
                "verse": "Plus mirer eius munera; plus tua?"
            },
            {
                "verse": "Martinus una se chlamyde exuit"
            },
            {
                "verse": "Tu te ipse Martino exuisti"
            },
            {
                "verse": "Et dubitas placuisse Christo?"
            }
        ],
        [
            {
                "verse": "ad eundem"
            }
        ],
        [
            {
                "verse": "Ergone putri fornice quos senex"
            },
            {
                "verse": "Dudum sepultos urnula commovet?"
            },
            {
                "verse": "Suprema quos adiudicavit"
            },
            {
                "verse": "Funereo Lybithina busto."
            },
            {
                "verse": "Exosa coelo machina si caput"
            },
            {
                "verse": "Coelo propinquum sublevet, ultimo"
            },
            {
                "verse": "Donabit aevo, numinumque"
            },
            {
                "verse": "In numerum moritura ponet?"
            },
            {
                "verse": "Aut forte Musis sacta sororibus"
            },
            {
                "verse": "Iovisque nato limina Fulvius"
            },
            {
                "verse": "Si ponat, aeternabit omnis"
            },
            {
                "verse": "Pone sequens series nepotum?"
            },
            {
                "verse": "Aut te Deorum coetibus inserat"
            },
            {
                "verse": "Marcelle virtus? Atque vetat mori"
            },
            {
                "verse": "Si templa virtuti superbo"
            },
            {
                "verse": "Aedifices ruitura saxo?"
            },
            {
                "verse": "Speres et aevi tempora si fides,"
            },
            {
                "verse": "Donata sacris aedibus Attili?"
            },
            {
                "verse": "Pioque non ingrata tecto"
            },
            {
                "verse": "Te pietas Acili beabit?"
            },
            {
                "verse": "Non sic superbi marmoris aequore"
            },
            {
                "verse": "Vel celte quicquid scripseris, hoc edax"
            },
            {
                "verse": "Absumet aetas, sic Deorum"
            },
            {
                "verse": "Sic pereunt monumenta Regum"
            },
            {
                "verse": "Inscripta vivis nomina mentibus"
            },
            {
                "verse": "Aeterna restant unica sic tuum"
            },
            {
                "verse": "Rudolphe nomen per sequentes"
            },
            {
                "verse": "Conspicuum volitabit annos."
            },
            {
                "verse": "Devota Divis atria quae facis"
            },
            {
                "verse": "Musisque coelorum aemula, nesciunt"
            },
            {
                "verse": "Subesse fatorum voraci"
            },
            {
                "verse": "Imperio cariemque vincunt."
            },
            {
                "verse": "Vivax Rodulphum concinet aevitas"
            },
            {
                "verse": "Spretoque saxi munere te sibi"
            },
            {
                "verse": "Trans stagna lethes evehendum"
            },
            {
                "verse": "Posteritas numerosa tradet."
            }
        ],
        [],
        [
            {
                "verse": "ad eundem"
            }
        ],
        [
            {
                "verse": "Rudolphe doctae gloria laureae"
            },
            {
                "verse": "O Parisini dulce decus soli,"
            },
            {
                "verse": "Quis charitatis te sacrato"
            },
            {
                "verse": "Igne senem stupeat pereso?"
            },
            {
                "verse": "Dum te in remoto littore Sequaenae"
            },
            {
                "verse": "Lassus palestrae Palladicae puer"
            },
            {
                "verse": "Artus fatigatos sopore"
            },
            {
                "verse": "Sub veridi recreas salicto."
            },
            {
                "verse": "Extemplo ab astris ignea Charitas"
            },
            {
                "verse": "Labens, iacentem sic temere in thoro"
            },
            {
                "verse": "Molli reclinavit rosarum,"
            },
            {
                "verse": "Et violis pia membra texit."
            },
            {
                "verse": "Ardensque alumni fata recludere"
            },
            {
                "verse": "Sic ora solvit, pars animae altera"
            },
            {
                "verse": "Rudolphe Christi, quid pusilla"
            },
            {
                "verse": "In tenero iacularis aevo."
            },
            {
                "verse": "I, perge felix, te Sapientia"
            },
            {
                "verse": "Duddum secundis ominibus vocat,"
            },
            {
                "verse": "Pindique deduductum per antra, et"
            },
            {
                "verse": "Molle supercilium, beatas"
            },
            {
                "verse": "Sorbonae in arces tollit, ubi nitens"
            },
            {
                "verse": "Sudore, laurum praemia frontium"
            },
            {
                "verse": "Aeterna doctarum mereris,"
            },
            {
                "verse": "Ostro humeros solito decorus."
            },
            {
                "verse": "Mox docta puris ora tumens aquis"
            },
            {
                "verse": "Ruis profundo flumine Regius"
            },
            {
                "verse": "Ecclesiastes, et cupitis"
            },
            {
                "verse": "Arva lavas sitibunda lymphis."
            },
            {
                "verse": "Proh! Quantus atrae sudor adest stygi?"
            },
            {
                "verse": "Quae criminosis dira phalangibus"
            },
            {
                "verse": "Dum pulpito crebrum tonante"
            },
            {
                "verse": "Proteris Eumenidum cerastas."
            },
            {
                "verse": "Frustra Sabaudis orta paludibus"
            },
            {
                "verse": "Caliginosas impietas faces"
            },
            {
                "verse": "Quassavit, infidamque frustra"
            },
            {
                "verse": "Tartareo vomit ore turmam,"
            },
            {
                "verse": "Laevique vultu lubrica respici"
            },
            {
                "verse": "Cohors Dionis, non sine libero"
            },
            {
                "verse": "Impura castae fata genti"
            },
            {
                "verse": "Idalio meditatur arcu."
            },
            {
                "verse": "Frustra; nam Iesu fida phalanx tui,"
            },
            {
                "verse": "Refringit omnes haresis impetus,"
            },
            {
                "verse": "Castoque deducit triumpho"
            },
            {
                "verse": "Lucifugos Veneris sodales."
            },
            {
                "verse": "Te copias teque auspicia, et tuas"
            },
            {
                "verse": "Praebente sedes, hinc tibi adorea"
            },
            {
                "verse": "Ribebit aetas, hinc parentem"
            },
            {
                "verse": "Usque feret domus haec Iesu."
            },
            {
                "verse": "Tum larga deplens pocula Delii"
            },
            {
                "verse": "Vates Rudolpho dimidio sui"
            },
            {
                "verse": "Sacrat Sophocleum cothurnum,"
            },
            {
                "verse": "Melpomene tibi gestiente."
            },
            {
                "verse": "Haec sunto amoris pignora; dein simul"
            },
            {
                "verse": "Vos reddet astris tarda necessitas"
            },
            {
                "verse": "Longum bibetis caelo amorem"
            },
            {
                "verse": "Ambrosia saturi beata."
            },
            {
                "verse": "Sic elocuta virgine, et arduas"
            },
            {
                "verse": "Scindente nubes ocius excutis"
            },
            {
                "verse": "Faustum soporem, Charitatis"
            },
            {
                "verse": "Indocilis cohibere flammas."
            }
        ],
        [],
        [
            {
                "verse": "ad eundem"
            }
        ],
        [
            {
                "verse": "O docte Patris caeliferi nepos"
            },
            {
                "verse": "Maiae propago, tu superis Deum"
            },
            {
                "verse": "Communis interpres et imis"
            },
            {
                "verse": "Gratus utrasque domus pererras."
            },
            {
                "verse": "Condenda moles: est animus supra"
            },
            {
                "verse": "Caeli coruscos tollere fornices"
            },
            {
                "verse": "Aeternitati post futuram"
            },
            {
                "verse": "Pyramidem, mihi dexter adsis."
            },
            {
                "verse": "Opaca furvae regna proserpinae"
            },
            {
                "verse": "Linquant Myrones, Praxiteles Mii."
            },
            {
                "verse": "Praetende virgam viperinam"
            },
            {
                "verse": "Redde animas cineri sepulto."
            },
            {
                "verse": "Quiid lentus audis vota? Quid obsidet"
            },
            {
                "verse": "Aerata nigri limimina Taenari"
            },
            {
                "verse": "Trifauce bacchatus latratu"
            },
            {
                "verse": "Cerberus, et prohibet regressum."
            },
            {
                "verse": "Ergo sepultis manibus est nefas"
            },
            {
                "verse": "Mortalis auras visere luminis,"
            },
            {
                "verse": "Immitis aeterno rigentes"
            },
            {
                "verse": "Sunt admante fores Averni."
            },
            {
                "verse": "Esto, moretur, vertere non licet"
            },
            {
                "verse": "Iurata divis, fata, novus mihi"
            },
            {
                "verse": "Favebit Orpheus, et vocata"
            },
            {
                "verse": "Illice saxa cheli ciebit."
            },
            {
                "verse": "Vides ut actae carminis Entheo"
            },
            {
                "verse": "Furore rupes , marmoream in struem"
            },
            {
                "verse": "Acuminentur, ut minanti"
            },
            {
                "verse": "Vertice mox super astra surgant,"
            },
            {
                "verse": "Ut illa Reges quattuor efferant"
            },
            {
                "verse": "Cervice flexa, quam placido advolet"
            },
            {
                "verse": "Superne descensu rosaque"
            },
            {
                "verse": "Ambrosiae Dea pulchra fundit."
            },
            {
                "verse": "Videre  dextro Numine vota Di"
            },
            {
                "verse": "Caput superbum Pyramis exerit,"
            },
            {
                "verse": "Quam non vetustas, non corusci"
            },
            {
                "verse": "Tela Iovis iaculata sternant."
            },
            {
                "verse": "Post saecla stabit, nempe sacer labor"
            },
            {
                "verse": "Vatum perennat, musaque deteri"
            },
            {
                "verse": "Igara, tellurem relinquit,"
            },
            {
                "verse": "Et supero caput abdit orbe."
            },
            {
                "verse": "Rudolphe Phoebi care sororibus"
            },
            {
                "verse": "Tibi camoenae caesariem implicant,"
            },
            {
                "verse": "Plauduntque viventi triumphos:"
            },
            {
                "verse": "Gloria sera venit sepulchro,"
            },
            {
                "verse": "Ne sint pudori munera carminum"
            },
            {
                "verse": "Laudanda praestas, quid memorem sacras"
            },
            {
                "verse": "Aedesque collatasque sedes"
            },
            {
                "verse": "Et patrii data iura fundi."
            },
            {
                "verse": "Quid te minorem vellere praedicem"
            },
            {
                "verse": "Martiniano, quo pietas tua"
            },
            {
                "verse": "Aevo beato munerentur"
            },
            {
                "verse": "Ipsa suis opulenta damnis."
            },
            {
                "verse": "Cui non stupendum iudicium fuit?"
            },
            {
                "verse": "Et melle sermo dulcior Attico?"
            },
            {
                "verse": "Cum lene stridentis susurro"
            },
            {
                "verse": "Pulpita subsiluere linguae."
            },
            {
                "verse": "Tibi efficacis cedit Ulyssei"
            },
            {
                "verse": "Mens cata laurum, quo properas levis"
            },
            {
                "verse": "Talia, laudes parce, parce,"
            },
            {
                "verse": "Ingenii tenuare culpa."
            }
        ]
    ]
}