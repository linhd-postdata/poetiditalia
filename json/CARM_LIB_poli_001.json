{
    "author": "carmina libraria",
    "title": "Angelus Politianus, opera",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|poli|001",
    "text": [
        [
            {
                "verse": "Petri Criniti"
            }
        ],
        [
            {
                "verse": "Hic hic viator paululum gradum siste."
            },
            {
                "verse": "Vatem potentis spiritum vides clarum"
            },
            {
                "verse": "Qui mente promptus acri et arduum spirans"
            },
            {
                "verse": "Ac summa quaeque et alta consequi suetus."
            },
            {
                "verse": "Is ille ego Angelus Politianus sum"
            },
            {
                "verse": "Fovit benigno me sinu Flora et illic"
            },
            {
                "verse": "In fata cessi Parthenopeos reges"
            },
            {
                "verse": "Cum Gallica arma irruerunt minabunda,"
            },
            {
                "verse": "Tu vale et hoc sis meriti memor nostri."
            }
        ],
        [
            {
                "verse": "eiusdem Criniti Monodia de laude Ang. Politiani"
            }
        ],
        [
            {
                "verse": "Siren relicto Daunio"
            },
            {
                "verse": "Quaerebat ubi consisteret"
            },
            {
                "verse": "Diuque vagula devolans"
            },
            {
                "verse": "Accessit amnem lassula"
            },
            {
                "verse": "Arni fluentis leniter"
            },
            {
                "verse": "Et commorata satis iter"
            },
            {
                "verse": "Parabat inceptum sequi."
            },
            {
                "verse": "Sede ecce dum vaga subvolat"
            },
            {
                "verse": "Audit repente rem novam"
            },
            {
                "verse": "Novosque numeros et nova"
            },
            {
                "verse": "Carmina canentis Angeli."
            },
            {
                "verse": "Itaque statim in labra convolat"
            },
            {
                "verse": "Dulci lepore perlita"
            },
            {
                "verse": "Et -inquit- hic libet agere,"
            },
            {
                "verse": "Hic hic morabor sedulo,"
            },
            {
                "verse": "Ubi gratia decor et Venus"
            },
            {
                "verse": "Tam blanditer simul canunt"
            },
            {
                "verse": "Carmenque fundunt aureum."
            },
            {
                "verse": "Tu nunc valebis Dauniae."
            }
        ],
        [
            {
                "verse": "Ex Strotii filii epogrammatibus"
            }
        ],
        [
            {
                "verse": "Quid Lycias fortes? Quis Phtiae carmina vatis?"
            },
            {
                "verse": "Transmissos Deli quis neget huc tripodas?"
            },
            {
                "verse": "Quippe huc assidue Phoebus Musaeque parentant"
            },
            {
                "verse": "Et caros lacrymis saepe lavant cineres."
            },
            {
                "verse": "Quin si fata velint tegerentur numina, sub qua"
            },
            {
                "verse": "Angelus obdormit Politianus humo."
            }
        ],
        [
            {
                "verse": "Incerti cuiusdam"
            }
        ],
        [
            {
                "verse": "Politianus in hoc tumulo iacet Angelus, unum"
            },
            {
                "verse": "Qui caput et linguas res nova tres habuit."
            }
        ]
    ]
}