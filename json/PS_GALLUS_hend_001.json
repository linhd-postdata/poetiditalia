{
    "author": "Pseudo Gallus",
    "title": "hendecasyllabi",
    "url": "http://www.poetiditalia.it/texts/PS_GALLUS|hend|001",
    "text": [
        [
            {
                "verse": "O mei procul ite nunc amores,"
            },
            {
                "verse": "Ite nunc, Veneres Cupidinesque"
            },
            {
                "verse": "Quicquid et fuit antea iocundum!"
            },
            {
                "verse": "Galla me, o superi, impia fefellit,"
            },
            {
                "verse": "Galla, proh superi, impudica moechos"
            },
            {
                "verse": "Praetulit mihi clanculum trecentos."
            },
            {
                "verse": "O scelesta, potes pati haec nefanda"
            },
            {
                "verse": "Nec videre pudet diem atque lucem"
            },
            {
                "verse": "Nec vero metuis truces iambos?"
            },
            {
                "verse": "Surgite, hendecasyllabi, repente!"
            },
            {
                "verse": "Impudentia percitentur ora!"
            },
            {
                "verse": "Clamate! audiat ista moecha turpis."
            },
            {
                "verse": "\"Scortum \u00e2\u0080\u00a0vulgare\u00e2\u0080\u00a0, putidum lupanar,"
            },
            {
                "verse": "Publicae stabulum et cloaca gentis,"
            },
            {
                "verse": "Vas urinae olidum, vorago penum,"
            },
            {
                "verse": "Sordido meritoriae tabernae"
            },
            {
                "verse": "Scorto putidior lutosiorque,"
            },
            {
                "verse": "Ten ego \u00e2\u0080\u00a0aspiciam ten \u00e2\u0080\u00a0 videre possim?"
            },
            {
                "verse": "Nec pares oculos habes nec ora"
            },
            {
                "verse": "Munda nec faciem, ut prius, venustam,"
            },
            {
                "verse": "Mala, quin etiam abstinente lingua"
            },
            {
                "verse": "Nec mundo pede nec manu pudenti."
            },
            {
                "verse": "Novit Niliacus tuum pudorem"
            },
            {
                "verse": "Cum tu, concubia vagata nocte,"
            },
            {
                "verse": "Custos impavida trementis arcis,"
            },
            {
                "verse": "Amicum tuum adusque ventitabas"
            },
            {
                "verse": "Atque intus, socio pudore misso,"
            },
            {
                "verse": "Ardens et nimium impudica moecha,"
            },
            {
                "verse": "Huius inguina turpe palpitasti."
            },
            {
                "verse": "Salve, Penelope et pudica Dido"
            },
            {
                "verse": "Ac Lucretia castitatis instar!"
            },
            {
                "verse": "Te diana suis cupit magistram"
            },
            {
                "verse": "Dare virginibus tuumque Vesta"
            },
            {
                "verse": "Nomen predicat omnibus sacellis."
            },
            {
                "verse": "O Niliaci amica sordidiosi,"
            },
            {
                "verse": "Sola qui tibi mentula probatur,"
            },
            {
                "verse": "Recte nos alios nil esse ducis,"
            },
            {
                "verse": "Quos tanta inguina non habere nosti."
            },
            {
                "verse": "Desine aspicere ac videre lucem!\""
            },
            {
                "verse": "Sed nil verba facit neque his movetur;"
            },
            {
                "verse": "Haeret mentula namque fixa cordi,"
            },
            {
                "verse": "Aures obstruit. Ergo colloquamur"
            },
            {
                "verse": "Suis, qui rabiem domare possunt:"
            },
            {
                "verse": "\"Agnati, quibus imminet propinaque"
            },
            {
                "verse": "Cura, quid facitis? Furit puella"
            },
            {
                "verse": "Effrenis neque iam movetur ullo"
            },
            {
                "verse": "Pudore in patria provinciaque.\""
            }
        ]
    ]
}