{
    "author": "carmina libraria",
    "title": "Georgius Purbachius, spaerae mundi",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|purb|001",
    "text": [
        [
            {
                "verse": "carmina in impressoris huius opuscoli laudem"
            }
        ],
        [
            {
                "verse": "Uraniae quantum quantum debere fatentur"
            },
            {
                "verse": "Cuncta canopeo cognitaque astra viro"
            },
            {
                "verse": "Santritter helbronna lucili ex urbe Iohannes"
            },
            {
                "verse": "Schemata sic debent ipsa reperta tibi."
            },
            {
                "verse": "Nec minus haec tibi de Sanctis Hieronyme debent"
            },
            {
                "verse": "Quam socio nanque hic invenit; ipse secas."
            }
        ]
    ]
}