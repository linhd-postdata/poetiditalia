{
    "author": "Alessandro Geraldini",
    "title": "carmina",
    "url": "http://www.poetiditalia.it/texts/GERAL_AL|carm|001",
    "text": [
        [
            {
                "verse": "Insula nomen habet generosum matris amatae,"
            },
            {
                "verse": "Chara nimis merito quae mihi semper erit."
            },
            {
                "verse": "Marmoreum Latia tenet in regione sepulchrum"
            },
            {
                "verse": "Atque etiam patrio membra sepulta solo."
            },
            {
                "verse": "Nomen ad Antipodes retinet tamen illa perenne,"
            },
            {
                "verse": "Nobile quaque sibi cum foret omne decus."
            },
            {
                "verse": "Dotibus ingenii, sancto virtutis honore"
            },
            {
                "verse": "Aequabat veterum saecula prisca patrum."
            },
            {
                "verse": "Excoluit doctas animum celebranda per artes,"
            },
            {
                "verse": "Cum caperet primos alma puella dies."
            },
            {
                "verse": "Filius hoc posuit nunc terra marmor in ista,"
            },
            {
                "verse": "Commemorat matris cum bona magna suae,"
            },
            {
                "verse": "Praesul Alexander submoti in sydere mundi"
            },
            {
                "verse": "Per refluum poscens templa beata mare."
            }
        ],
        [
            {
                "verse": "Per mare velivolum nostrae ultra sydera terrae"
            },
            {
                "verse": "Venimus incolumes."
            },
            {
                "verse": "Sydera per coelum mutato currere vultu"
            },
            {
                "verse": "Undique conspicimus:"
            },
            {
                "verse": "Ursa, per arctoos longe distincta triones,"
            },
            {
                "verse": "Hic prope iuncta mari."
            },
            {
                "verse": "Hic quoque nunc refluunt Neptuni littora magni"
            },
            {
                "verse": "Tenta per Oceanum."
            },
            {
                "verse": "Hic quoque iam pelagi se pandunt mo[n]stra profundi"
            },
            {
                "verse": "Turpia et ore truci."
            },
            {
                "verse": "Deterius nihil et nihil est immanius usquam,"
            },
            {
                "verse": "Nil adeoque ferum."
            },
            {
                "verse": "Humanae passim comedunt qui corpora gentis"
            },
            {
                "verse": "Voceque cum resona,"
            },
            {
                "verse": "Nullos esse Deos credunt et numina nulla"
            },
            {
                "verse": "Aethera per superum."
            },
            {
                "verse": "At decus aeternum multo qui robore praestant"
            },
            {
                "verse": "Per populos capiunt."
            },
            {
                "verse": "Regna per immensos sunt haec diffusa rotatus"
            },
            {
                "verse": "Qua volitant oculi."
            },
            {
                "verse": "Innumerae passim se pandunt undique gentes"
            },
            {
                "verse": "Oppidaque innumera,"
            },
            {
                "verse": "Atque venenatas iaciunt per bella sagittas"
            },
            {
                "verse": "Et nece cum subita."
            },
            {
                "verse": "Fronte truci, nimium truculento denique vultu,"
            },
            {
                "verse": "Proelia cruda movent."
            },
            {
                "verse": "Has ego per terras ignoti in cardine mundi,"
            },
            {
                "verse": "Has ego perque plagas,"
            },
            {
                "verse": "Post multos menses longo post tempore veni,"
            },
            {
                "verse": "Classe secante mare,"
            },
            {
                "verse": "Gentibus et populis sanctissima signa secutis,"
            },
            {
                "verse": "Quae veneranda micant."
            },
            {
                "verse": "Et me pastorem vitae voluere tenendae"
            },
            {
                "verse": "Esse suum penitus;"
            },
            {
                "verse": "Pontificem populi longas legere per oras"
            },
            {
                "verse": "Innumero spatio."
            },
            {
                "verse": "Me miserum tota mundi regione removi"
            },
            {
                "verse": "Qua decus ingenii,"
            },
            {
                "verse": "Linquere sorte datum populos et regna potentum"
            },
            {
                "verse": "Quae ratione vigent."
            },
            {
                "verse": "Et numquam in terras equidem remeabo Latinas,"
            },
            {
                "verse": "Qua genus, et proavi,"
            },
            {
                "verse": "Ossa meae matris recubant ubi marmore tecta"
            },
            {
                "verse": "Chara mihi nimium,"
            },
            {
                "verse": "Et pater et proavi nostrique ex ordine fratres"
            },
            {
                "verse": "Membra sepulta tenent."
            },
            {
                "verse": "Hic me barbaricas inter subsistere gentes"
            },
            {
                "verse": "Sors inimica iubet."
            },
            {
                "verse": "Intrabunt quondam sacra haec per templa futuri"
            },
            {
                "verse": "Noster ubi tumulus,"
            },
            {
                "verse": "Atque inspectantes nostros e marmore vultus,"
            },
            {
                "verse": "Stemmata celsa domus,"
            },
            {
                "verse": "'Terrarum,' dicent, 'tali subsistere parte"
            },
            {
                "verse": "Hic homo natus erat;"
            },
            {
                "verse": "Urbe diu praesul quondam permansit in ista"
            },
            {
                "verse": "Nomine cum celebri,"
            },
            {
                "verse": "Et docuit vulgo Christi documenta beati"
            },
            {
                "verse": "Iure sequenda fore."
            },
            {
                "verse": "Maxima per nostras induxit commoda gentes,"
            },
            {
                "verse": "Pastor ubique pius."
            },
            {
                "verse": "Hic reseravit iter lati per sydera coeli"
            },
            {
                "verse": "Gentibus et populis."
            },
            {
                "verse": "Et merito vivet magno cum nomine semper"
            },
            {
                "verse": "Qui bona tanta tulit.'"
            },
            {
                "verse": "Sed nostras penitus laudes numerare relinquam,"
            },
            {
                "verse": "Quae mihi sunt odio."
            },
            {
                "verse": "Ad me cum magna certe ratione redibo,"
            },
            {
                "verse": "Quod mihi iure licet,"
            },
            {
                "verse": "Qui varios casus per totas undique terras"
            },
            {
                "verse": "Per pelagusque vagum,"
            },
            {
                "verse": "Ac animo intrepido discrimina cuncta subegi"
            },
            {
                "verse": "Quae renuunt reliqui."
            },
            {
                "verse": "Me superi nimium radiantia numina Phoebi"
            },
            {
                "verse": "Heu penitus lacerant,"
            },
            {
                "verse": "Humida terra nocet, prosunt alimenta nec ulla,"
            },
            {
                "verse": "Pomaque nulla placent."
            },
            {
                "verse": "Hic mihi nempe seni funus iam fata maligna,"
            },
            {
                "verse": "Sors et amara parant."
            },
            {
                "verse": "Debebam nostro populos a cardine motos"
            },
            {
                "verse": "Quaerere per pelagus,"
            },
            {
                "verse": "Ac alia aversi multum per sydera coeli"
            },
            {
                "verse": "Cum rate vela dare."
            },
            {
                "verse": "Fata valent nimium, fatis agitamur iniquis,"
            },
            {
                "verse": "Sic tolerante Deo,"
            },
            {
                "verse": "Et saepe in toto pariter nos cernimus orbe"
            },
            {
                "verse": "Quid mala fata ferant."
            }
        ],
        [
            {
                "verse": "Nunc ego templum studeo beatum"
            },
            {
                "verse": "Magna Reginae superum potenti"
            },
            {
                "verse": "Atque cum multo per inane latum"
            },
            {
                "verse": "Tollere honore."
            },
            {
                "verse": "Et meas vires animumque totum"
            },
            {
                "verse": "Spondeo et magnos pariter labores,"
            },
            {
                "verse": "Undique ut digno niteant decore"
            },
            {
                "verse": "Omnia plena."
            },
            {
                "verse": "Numini magno genitricis almae,"
            },
            {
                "verse": "Ventre quae summum potuit Tonantem"
            },
            {
                "verse": "Ferre, sunt certe tribuenda quoque"
            },
            {
                "verse": "Grandia multum,"
            },
            {
                "verse": "Quae supra gentes volitent caducas,"
            },
            {
                "verse": "Sintque res pulchrae similes Olympo,"
            },
            {
                "verse": "Nilque terrenae subeant ruinae"
            },
            {
                "verse": "Omnia mira."
            },
            {
                "verse": "Sed tamen nostrum capiet laborem"
            },
            {
                "verse": "Illa cum vultu pietate pleno:"
            },
            {
                "verse": "Nam solet totas refovere terras"
            },
            {
                "verse": "Fronte serena."
            },
            {
                "verse": "Et solet gentes recreare moestas,"
            },
            {
                "verse": "Pallio subter retinere sancto,"
            },
            {
                "verse": "Et solet turbae misere vocanti"
            },
            {
                "verse": "Ferre levamen."
            },
            {
                "verse": "Haec supra celsas retinebit aras,"
            },
            {
                "verse": "Picta praeclari manibus magistri,"
            },
            {
                "verse": "Atque coelesti facie beata"
            },
            {
                "verse": "Oreque miti."
            },
            {
                "verse": "Et leget psalmos veterum probatos,"
            },
            {
                "verse": "Cum quibus coelum penetrabat altum,"
            },
            {
                "verse": "Cum David sedes superum subibat"
            },
            {
                "verse": "Pectore toto."
            },
            {
                "verse": "Nuncius stabit veniens ab astris,"
            },
            {
                "verse": "Undique insigni radiante vultu;"
            },
            {
                "verse": "Lilium dextra retinebit alta"
            },
            {
                "verse": "Ore decorus."
            },
            {
                "verse": "Desuper magnis radiis columba"
            },
            {
                "verse": "Cincta consistet niveo colore;"
            },
            {
                "verse": "Alites certe superabit omnes"
            },
            {
                "verse": "Ore notando."
            },
            {
                "verse": "Ipse tum nobis memoranda semper"
            },
            {
                "verse": "Verba divini referet Parentis:"
            },
            {
                "verse": "'Nunc ave terras merito per imas,"
            },
            {
                "verse": "Gratia plena!"
            },
            {
                "verse": "'Tu quidem laeto sacra virgo Olympo"
            },
            {
                "verse": "Aetheris Regem paries potentem,"
            },
            {
                "verse": "Unaque illaesos uteri recessus"
            },
            {
                "verse": "Sola tenebis.'"
            },
            {
                "verse": "Parte sub templi media per amplum"
            },
            {
                "verse": "Aeris tractum cruce rite Christus"
            },
            {
                "verse": "Corpore et totis manibus retentus"
            },
            {
                "verse": "Ipse manebit."
            },
            {
                "verse": "Sive se dextra paries retendet,"
            },
            {
                "verse": "Sive per partes retrahet sinistras,"
            },
            {
                "verse": "Sive qua templum renitebit alta"
            },
            {
                "verse": "Sede vel ima,"
            },
            {
                "verse": "Arte censebunt veterum magistra"
            },
            {
                "verse": "Cuncta praeclaro fabricata cultu,"
            },
            {
                "verse": "Undique et laeta pia templa ubique"
            },
            {
                "verse": "Luce nitebunt."
            },
            {
                "verse": "Aedis in prima facie sacratae"
            },
            {
                "verse": "Quaque consurgunt lapides quadrati,"
            },
            {
                "verse": "Quaque se tendunt hominum labores"
            },
            {
                "verse": "Arte notanda."
            },
            {
                "verse": "Fronte sub templi media superbi"
            },
            {
                "verse": "Signa Romanae venerandae sedis"
            },
            {
                "verse": "Et sacrum Papae diadema fulget"
            },
            {
                "verse": "Undique celsum,"
            },
            {
                "verse": "Qui caput trina redimens corona,"
            },
            {
                "verse": "Rite sub coelo residens Latino,"
            },
            {
                "verse": "Iura dat toti sacra nempe mundo"
            },
            {
                "verse": "Rector ubique."
            },
            {
                "verse": "Inde cum pulchris aquilae thiaris"
            },
            {
                "verse": "Caesaris partem retegunt sinistram:"
            },
            {
                "verse": "Illa magnorum fuerant Quiritum"
            },
            {
                "verse": "Stemmata quondam,"
            },
            {
                "verse": "Marte qui totas tenuere terras,"
            },
            {
                "verse": "Subditum forti pelagusque dextra,"
            },
            {
                "verse": "Qua vagi Phoebi radiant quadrigae"
            },
            {
                "verse": "Littore utroque."
            },
            {
                "verse": "At Geraldinae sacra signa gentis"
            },
            {
                "verse": "Inde cum multo veniunt honore,"
            },
            {
                "verse": "Sydere in terno peramanda multum"
            },
            {
                "verse": "Palladis arbor,"
            },
            {
                "verse": "Quae bonam praebens populis quietem"
            },
            {
                "verse": "Bella per gentes removet tremenda,"
            },
            {
                "verse": "Regibus sanctum tribuit liquorem"
            },
            {
                "verse": "Chrismaque notum,"
            },
            {
                "verse": "Sculpta, cum praesul foret urbis huius"
            },
            {
                "verse": "Nempe Alexander pietate notus,"
            },
            {
                "verse": "Qui dedit multis documenta magna"
            },
            {
                "verse": "Regibus olim,"
            },
            {
                "verse": "Semper et castas coluit Camenas,"
            },
            {
                "verse": "Atque Parnassi iuga celsa montis"
            },
            {
                "verse": "Saepe scandebat, sociatus illo"
            },
            {
                "verse": "Ordine sancto."
            }
        ]
    ]
}