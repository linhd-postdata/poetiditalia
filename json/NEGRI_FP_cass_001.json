{
    "author": "Francesco Pescennio Negri",
    "title": "ad Cassandram Fidelem",
    "url": "http://www.poetiditalia.it/texts/NEGRI_FP|cass|001",
    "text": [
        [
            {
                "verse": "Francisci Nigri Veneti doctoris in Cassandram Pistaeam paeonicon sapphicon"
            }
        ],
        [
            {
                "verse": "Virgo, cui tinnit falerata lingua"
            },
            {
                "verse": "Ore facundo gravibusque dictis,"
            },
            {
                "verse": "Cui tonat dignum Cicerone pectus"
            },
            {
                "verse": "Eloquiumque,"
            },
            {
                "verse": "Sive sermonem sequeris pedestrem,"
            },
            {
                "verse": "Sive bullatum potius cothurnum"
            },
            {
                "verse": "Vel quod intonsi sapuere patres"
            },
            {
                "verse": "Quodque novelli,"
            },
            {
                "verse": "Nescio quo te celebrare plectro"
            },
            {
                "verse": "Debeam; laudes subeunt Camoenis"
            },
            {
                "verse": "Namque tot nostris, quibus alma fulges"
            },
            {
                "verse": "Virgo per orbem,"
            },
            {
                "verse": "Cedat ut plectrum facilis Properti,"
            },
            {
                "verse": "Musa Nasonis simul et Catulli"
            },
            {
                "verse": "Atque onus tantum fugiat Maronis"
            },
            {
                "verse": "Calliopea."
            },
            {
                "verse": "Flebilis illi statuere versus:"
            },
            {
                "verse": "Cynthiae dulcis modulatus ignes"
            },
            {
                "verse": "Ille, dilectae lachrymans Corynnae"
            },
            {
                "verse": "Alter amores;"
            },
            {
                "verse": "Lesbiae flammas cecinit Catullus,"
            },
            {
                "verse": "Filium laetae Veneris poeta"
            },
            {
                "verse": "Omnium princeps simul et deorum"
            },
            {
                "verse": "Arma virumque."
            },
            {
                "verse": "Quid novi tandem cecinere tales?"
            },
            {
                "verse": "Hic virum fortem, solitos hic ignes"
            },
            {
                "verse": "Dixit et quae sunt recitata quondam"
            },
            {
                "verse": "Carmine prisco."
            },
            {
                "verse": "Tu tamen cunctos superas, puella,"
            },
            {
                "verse": "Quos vel ornatis fidibus Latini"
            },
            {
                "verse": "Sive nugaces voluere Graii"
            },
            {
                "verse": "Erre sub astra."
            },
            {
                "verse": "Hinc nites pulchra facie, venusto"
            },
            {
                "verse": "Crine, paestanis labiis, serena"
            },
            {
                "verse": "Fronte, ridenti penitus ocello"
            },
            {
                "verse": "Numinis instar."
            },
            {
                "verse": "Hinc velut sidus nitidum refulget"
            },
            {
                "verse": "Casta mens, sanctum simul et pudicum"
            },
            {
                "verse": "Pectus et summa probitate totum"
            },
            {
                "verse": "Corpus odorum."
            },
            {
                "verse": "Inde facundo vigilata corde"
            },
            {
                "verse": "Dicta mirantur iuvenes senesque;"
            },
            {
                "verse": "Credimus scriptis utriusque linguae"
            },
            {
                "verse": "Numen adesse."
            },
            {
                "verse": "Ecquis ornatus poterit pedester"
            },
            {
                "verse": "Prosequi tantas totidemque laudes?"
            },
            {
                "verse": "Ecquis in dignos poterit cothurnus"
            },
            {
                "verse": "Surgere cantus?"
            },
            {
                "verse": "Virgines multi cecinere doctas,"
            },
            {
                "verse": "Tulliam, Gracchi pariter sororem,"
            },
            {
                "verse": "Quaeve facundi meruit parentis"
            },
            {
                "verse": "Dicier haeres;"
            },
            {
                "verse": "Secla quas vano genuere mundo,"
            },
            {
                "verse": "Nam decus linguae licet ipse Phoebus"
            },
            {
                "verse": "Cesserit tantis merito puellis"
            },
            {
                "verse": "More vetusto,"
            },
            {
                "verse": "Non tamen pectus tribuit pudicum,"
            },
            {
                "verse": "Quod tibi Pallas dedit inter omnes,"
            },
            {
                "verse": "Quod tibi virtus, probitas fidesque"
            },
            {
                "verse": "Cana reliquit."
            },
            {
                "verse": "Ergo divino decorata vultu"
            },
            {
                "verse": "Virgo, divinam sapiensque vocem,"
            },
            {
                "verse": "Quod nisi sacrum tibi numen esse"
            },
            {
                "verse": "Confiteamur?"
            },
            {
                "verse": "Sacra es et caelo veniens profecto,"
            },
            {
                "verse": "Ut leves Martis populum ferocem,"
            },
            {
                "verse": "Secla Saturni tribuasque nobis"
            },
            {
                "verse": "Sidere fausto,"
            },
            {
                "verse": "Diva, sed quo te populus fatiget"
            },
            {
                "verse": "Thure? Vel quovis titulo vocari?"
            },
            {
                "verse": "Iure pistaeam precibus pudicis"
            },
            {
                "verse": "Flectite vates."
            },
            {
                "verse": "Hanc sibi Phoebus cupiat puellam,"
            },
            {
                "verse": "Haec novenarum numeros sororum"
            },
            {
                "verse": "Augeat; denam vocitemus omnes"
            },
            {
                "verse": "Hanc quoque Musam."
            },
            {
                "verse": "Haec mihi cantus tribuat fidesque"
            },
            {
                "verse": "Haec modos, plectrum, cytharam, cothurnum,"
            },
            {
                "verse": "Haec mihi demum faveat deinceps"
            },
            {
                "verse": "Calliopea."
            }
        ]
    ]
}