{
    "author": "Pietro Paolo Vergerio",
    "title": "carmen",
    "url": "http://www.poetiditalia.it/texts/VERGERIO|carm|001",
    "text": [
        [
            {
                "verse": "Petrus Paulus Vergerius Doctissimo viro Friderico Nausa"
            }
        ],
        [
            {
                "verse": "Ut isti pereant viri maligni,"
            },
            {
                "verse": "Qui me tam male, tam ferunt inique"
            },
            {
                "verse": "Scripsisse in te, hominem bene eruditum."
            },
            {
                "verse": "Ego ne id facerem? Tibi ve possem,"
            },
            {
                "verse": "Tibi, Nausea, sic molestus esse?"
            },
            {
                "verse": "Quam solent mihi nauseam movere"
            },
            {
                "verse": "Hi viri illepidi atque pestilentes"
            },
            {
                "verse": "Qui ferunt ea quae fuere nunquam,"
            },
            {
                "verse": "Ut me Nausea non amaret ille,"
            },
            {
                "verse": "Ille Nausea, quem fovet Marinus,"
            },
            {
                "verse": "Ille ille optimus omnium Marinus,"
            },
            {
                "verse": "Praeceptor meus et pater Marinus."
            },
            {
                "verse": "Si, qui isti fuerint, rescire possum,"
            },
            {
                "verse": "Sat est: tu modo Nausea erudite,"
            },
            {
                "verse": "Crede non fatuis et infacetis,"
            },
            {
                "verse": "Sed mihi magis, ac magis Marino."
            },
            {
                "verse": "Quippete ex animo tam amamus unum,"
            },
            {
                "verse": "Quam tu et ingenio vales, et arte."
            }
        ]
    ]
}