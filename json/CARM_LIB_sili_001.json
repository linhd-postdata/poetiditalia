{
    "author": "carmina libraria",
    "title": "Silius Italicus, Punica",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|sili|001",
    "text": [
        [
            {
                "verse": "Aspicis illustris lector quicumque libellos"
            },
            {
                "verse": "Si cupis artificum nomina nosce, lege"
            },
            {
                "verse": "Aspera ridebis cognomina Teutona, forsan"
            },
            {
                "verse": "Mitiget ars musis inscia verba virum."
            },
            {
                "verse": "Conradus Sweynheym Arnoldus, Pannartzque magistri"
            },
            {
                "verse": "Romae impresserunt talia multa simul"
            },
            {
                "verse": "Petrus cum fratre Francisco Maximus ambo"
            },
            {
                "verse": "Huic operi aptatam contribuere domum."
            }
        ]
    ]
}