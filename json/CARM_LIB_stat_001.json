{
    "author": "carmina libraria",
    "title": "Statuta synodalia epixscopatus Basileensis, ediderunt Joannes Amerbach etc.",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|stat|001",
    "text": [
        [
            {
                "verse": "carmina commendaticia"
            }
        ],
        [
            {
                "verse": "Quod mores renovas priscos meritisssime praesul"
            },
            {
                "verse": "Cum synodo antiqua religione sacra,"
            },
            {
                "verse": "Raurica spumanti plaudit contermina Rheno"
            },
            {
                "verse": "Ecclesia et laudes spargit ubique tuas."
            },
            {
                "verse": "Obstupui ut vidi victricia signa ferentes"
            },
            {
                "verse": "Dum caneret celso candida turba Deo."
            },
            {
                "verse": "Ipse habitu augusto vittata fronde verendus"
            },
            {
                "verse": "Exhilarans populos ora diserta moves."
            },
            {
                "verse": "Non sic Oenotrii Capitolia vana subibunt"
            },
            {
                "verse": "Virginea lauro concelebrare Iovem."
            },
            {
                "verse": "Propterea aeternum linguis animisque favendo"
            },
            {
                "verse": "Te tuus o clerus candide pastor amet."
            }
        ],
        [
            {
                "verse": "carmen excitatorium"
            },
            {
                "verse": "Praesulis exemplo vitam sine sorde sacerdos"
            },
            {
                "verse": "Ducere quisque velis;"
            },
            {
                "verse": "Legitimos tibi cura precor sit sedula mores"
            },
            {
                "verse": "Illius usque sequi."
            },
            {
                "verse": "Virtuti praemissa salus sedesque beatae"
            },
            {
                "verse": "Praemia te moveant."
            },
            {
                "verse": "Muneris altus apex moveat te, vel crucifixi"
            },
            {
                "verse": "Vulnera quinque Dei;"
            },
            {
                "verse": "Aut Phlegetonteo saltem moveare barathro"
            },
            {
                "verse": "Tergeminoque cane."
            }
        ]
    ]
}