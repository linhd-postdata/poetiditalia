{
    "author": "Francesco Filelfo",
    "title": "carmen ad Othonem",
    "url": "http://www.poetiditalia.it/texts/FILELFO|carm|001",
    "text": [
        [
            {
                "verse": "Astra Pio minitantur, Otho, caelestia mortem,"
            },
            {
                "verse": "Augustum ad mensem qui prope carpit iter."
            },
            {
                "verse": "Si quae doctori statuit rata praemia solvet"
            },
            {
                "verse": "Iste suo, mendax opto sit ipse polus;"
            },
            {
                "verse": "Sin servare fidem nolit, sint sydera certo"
            },
            {
                "verse": "Enumerata gradu quo Pius intereat."
            }
        ]
    ]
}