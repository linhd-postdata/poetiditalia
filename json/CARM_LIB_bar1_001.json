{
    "author": "carmina libraria",
    "title": "Andreas Barbatia, commentaria",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|bar1|001",
    "text": [
        [
            {
                "verse": "Ugeri Pontremulensis ad lectorem"
            }
        ],
        [
            {
                "verse": "Caesareas quisquis leges cognoscere tentat"
            },
            {
                "verse": "Iuraque quae sanxit pontificalis apex,"
            },
            {
                "verse": "Hoc legat, hoc relegat studiosa mente volumen:"
            },
            {
                "verse": "Hic quae delectent discere multa potest"
            },
            {
                "verse": "Plurima quae latuere prius nam sensa videbit:"
            },
            {
                "verse": "Miratus dicet: \"quis, rogo, fecit opus?\""
            },
            {
                "verse": "Edidit haec toto Barbatia notus in orbe;"
            },
            {
                "verse": "Cetera iam sileo sit monuisse satis."
            }
        ],
        [],
        [
            {
                "verse": "Henrici Laiadi lusitani, ad eundem"
            }
        ],
        [
            {
                "verse": "Qui cupit ambages legum atque aenigmata iuris"
            },
            {
                "verse": "Solvere et implicitis obvius ire dolis"
            },
            {
                "verse": "Ut fit cuique suum, vitae ut servetur honestas"
            },
            {
                "verse": "Inque virum ne quis saeviat innocuum;"
            },
            {
                "verse": "Perlegat egregium noctesque diesque volumen:"
            },
            {
                "verse": "Aeacus aut Minos vel Radamanthus erit."
            },
            {
                "verse": "Composuit vigili Barbatia nempe lucerna"
            },
            {
                "verse": "Posteritas cuius nomen ad astra feret."
            }
        ]
    ]
}