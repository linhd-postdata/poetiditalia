{
    "author": "carmina libraria",
    "title": "Iohannes Reuchlin, phorcensis scenica",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|reuc|001",
    "text": [
        [
            {
                "verse": "Sebastianus Brant"
            }
        ],
        [
            {
                "verse": "Accipe Vangionum praesul venerande Ioannis"
            },
            {
                "verse": "Capnionis nostri comica dulciloqui"
            },
            {
                "verse": "Quo duce Germanos comoedia prisca revisit"
            },
            {
                "verse": "Et meruit soccis Rhenus inire novis."
            },
            {
                "verse": "Barbarico ex fumo flammas meus ille decoras"
            },
            {
                "verse": "Capnion elicuit. Gratia multa viro."
            },
            {
                "verse": "Multum docta cohors, multum cirrata iuventus"
            },
            {
                "verse": "Debet Capnioni, multa Thalia meo"
            }
        ]
    ]
}