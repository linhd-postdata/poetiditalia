{
    "author": "Galeotto Marzio",
    "title": "carmen dubium",
    "url": "http://www.poetiditalia.it/texts/MARZIO|dubi|001",
    "text": [
        [
            {
                "verse": "Eloquio valui, curavi commoda pacis,"
            },
            {
                "verse": "Praeclarus sophia Martius ipse fui."
            },
            {
                "verse": "Nar natale solum, celebrem me reddidit Hunnus,"
            },
            {
                "verse": "Pontifici carus, regis amicus eram."
            },
            {
                "verse": "Nunc placeo superis, sunt haec monimenta laboris,"
            },
            {
                "verse": "Exemplo ut patriae sim, locus iste monet."
            }
        ]
    ]
}