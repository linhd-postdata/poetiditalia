{
    "author": "Cassandra Fedele",
    "title": "epigramma ad Cassandram",
    "url": "http://www.poetiditalia.it/texts/FEDELE|epig|001",
    "text": [
        [
            {
                "verse": "ad Cassandram Fidelem poetissam epigramma"
            }
        ],
        [
            {
                "verse": "Quanta tua est probitas, quanta et praestantia linguae"
            },
            {
                "verse": "Quamque tuum prisco diferat ingenium"
            },
            {
                "verse": "Vidimus et nobis monstravit epistola quam tu"
            },
            {
                "verse": "Misisti nuper nostro achademico."
            },
            {
                "verse": "O quam foelices nimium, Cassandra, parentes"
            },
            {
                "verse": "Natalique fuit grata Lucina tuo."
            },
            {
                "verse": "Illo nempe die non recordatus amorem"
            },
            {
                "verse": "Phetontis cinxit purpura clara deum."
            },
            {
                "verse": "Riserunt faciles undis Nereides et quae"
            },
            {
                "verse": "Siqua colunt sacrum numina Castalium"
            },
            {
                "verse": "Nam Lybiae tu sola vales mulcere pericla"
            },
            {
                "verse": "Sola canum Scyllae vincere duritiem"
            },
            {
                "verse": "Tollere sola tui poenas haec, Sysiphe, saxi"
            },
            {
                "verse": "Posset, tantalea summere poma manu,"
            },
            {
                "verse": "Sola puellarum venetum tu gloria prolis,"
            },
            {
                "verse": "Sola potes duris mentibus esse quies,"
            },
            {
                "verse": "Peligni doctam sileat modo carta Corinnam"
            },
            {
                "verse": "Cedat et Aurelii Cynthia culta modis."
            }
        ]
    ]
}