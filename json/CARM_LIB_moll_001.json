{
    "author": "carmina libraria",
    "title": "Iohannes Moller, sequentes iuris controversi positiones",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|moll|001",
    "text": [
        [
            {
                "verse": "Viro Nobili et Doctissimo Iohanni Mollero, pro summma  in utroque laurea disputanti"
            }
        ],
        [
            {
                "verse": "Ordire forti proelia dextera"
            },
            {
                "verse": "Mollere magni progenies Patris"
            },
            {
                "verse": "Quem consili et legum potentem"
            },
            {
                "verse": "Hammonius coluit senatus."
            },
            {
                "verse": "Numquam clientes deseruit suos"
            },
            {
                "verse": "Astraea nec te numine deseret;"
            },
            {
                "verse": "Illam tenellis namque ab annis"
            },
            {
                "verse": "Cura fuit tibi sola amare."
            },
            {
                "verse": "Mox mox quadrigis provehet aureis"
            },
            {
                "verse": "Phoebus diem qua Rauraca te Themis"
            },
            {
                "verse": "Rubra decorum fronte mitra"
            },
            {
                "verse": "Eunomiae sociabit aris."
            }
        ],
        [
            {
                "verse": "Boni omnis ergo p."
            },
            {
                "verse": "Iohannes Secreta Schotnovius"
            },
            {
                "verse": "A Zavorzitz,  Praga Boh. M.D."
            }
        ]
    ]
}