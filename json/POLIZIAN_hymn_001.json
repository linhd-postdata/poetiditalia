{
    "author": "Angelo Poliziano",
    "title": "hymni",
    "url": "http://www.poetiditalia.it/texts/POLIZIAN|hymn|001",
    "text": [
        [],
        [
            {
                "verse": "O virgo prudentissima,"
            },
            {
                "verse": "Quam coelo missus Gabriel"
            },
            {
                "verse": "Supremi regis nuntius"
            },
            {
                "verse": "Plenam testatur gratia;"
            },
            {
                "verse": "Cuius devota humilitas,"
            },
            {
                "verse": "Gemmis ornata fulgidis"
            },
            {
                "verse": "Fidentis conscientiae,"
            },
            {
                "verse": "Amore Deum rapit;"
            },
            {
                "verse": "Te sponsam Factor omnium,"
            },
            {
                "verse": "Te matrem Dei Filius,"
            },
            {
                "verse": "Te vocat habitaculum"
            },
            {
                "verse": "Suum beatus Spiritus."
            },
            {
                "verse": "Per te de tetro carcere"
            },
            {
                "verse": "Antiqui patres exeunt,"
            },
            {
                "verse": "Per te nobis astriferae"
            },
            {
                "verse": "Panduntur aulae limina."
            },
            {
                "verse": "Tu stellis comam cingeris,"
            },
            {
                "verse": "Tu lunam premis pedibus,"
            },
            {
                "verse": "Te sole amictam candido"
            },
            {
                "verse": "Chori stupent angelici."
            },
            {
                "verse": "Tu stella maris diceris,"
            },
            {
                "verse": "Quae nobis inter scopulos"
            },
            {
                "verse": "Inter obscuros turbines"
            },
            {
                "verse": "Portum salutis indicat."
            },
            {
                "verse": "Audi, virgo puerpera"
            },
            {
                "verse": "Et sola mater integra,"
            },
            {
                "verse": "Audi precantes, quaesumus,"
            },
            {
                "verse": "Tuos, Maria, servulos."
            },
            {
                "verse": "Repelle mentis tenebras,"
            },
            {
                "verse": "Disrumpe cordis glaciem;"
            },
            {
                "verse": "Nos sub tuum praesidium"
            },
            {
                "verse": "Confugientes protege;"
            },
            {
                "verse": "Da nobis in proposito"
            },
            {
                "verse": "Sancto perseverantiam,"
            },
            {
                "verse": "Ne noster adversarius"
            },
            {
                "verse": "In te sperantes superet."
            },
            {
                "verse": "Sed et cunctis fidelibus,"
            },
            {
                "verse": "Qui tuum templum visitant,"
            },
            {
                "verse": "Benigna mater, dexteram"
            },
            {
                "verse": "Da coelestis auxilii."
            }
        ]
    ]
}