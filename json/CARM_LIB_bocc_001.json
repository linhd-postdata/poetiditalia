{
    "author": "carmina libraria",
    "title": "Iohannes Boccatius, de claris mulieribus",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|bocc|001",
    "text": [
        [
            {
                "verse": "Hieronymi Frick carmen ad lectorem"
            }
        ],
        [
            {
                "verse": "Heroum si facta virum praeclara merentur"
            },
            {
                "verse": "Laudibus aeternis concelebranda dari,"
            },
            {
                "verse": "Plenaque sunt illis veterum monumenta sophorum,"
            },
            {
                "verse": "Quid veterum? Passim, si bene gesta placent."
            },
            {
                "verse": "Ut quibus exemplis ceu calcare quisque movetur"
            },
            {
                "verse": "Ingenuis factis conciliare decus."
            },
            {
                "verse": "Exemplis aliis sicuti sapientia vera putatur"
            },
            {
                "verse": "Discere quae ex usu quaeque nociva tibi."
            },
            {
                "verse": "Adde quod hoc speculo tua quae bene facta sequantur"
            },
            {
                "verse": "Praemia multiplici laude videre potes."
            },
            {
                "verse": "Cur non perpetuae mittentur nomina famae"
            },
            {
                "verse": "Illaque scriptorum docta litura notet."
            },
            {
                "verse": "Gesta matronarum cunctis miranda per orbem"
            },
            {
                "verse": "Quaedam adamanda tibi, quaedam et habenda odio"
            },
            {
                "verse": "Qualia dat noster Boccatius optime lector"
            },
            {
                "verse": "Plurima queis emptis ipse potire. Vale."
            }
        ]
    ]
}