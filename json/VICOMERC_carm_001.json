{
    "author": "Giovanni Antonio Vicomercati",
    "title": "carmen",
    "url": "http://www.poetiditalia.it/texts/VICOMERC|carm|001",
    "text": [
        [
            {
                "verse": "Una fui matrum prestanti prole beata"
            },
            {
                "verse": "Que genui summos Lucia magna duces."
            },
            {
                "verse": "Est mea progenies patrie regnator opime"
            },
            {
                "verse": "Anguiger Insubris Cesar in orbe novus."
            },
            {
                "verse": "Felicem terris gnatorum Gloria fecit"
            },
            {
                "verse": "Sed dedit hetereis me pia vita dies."
            }
        ]
    ]
}