{
    "author": "Andrea Angulo",
    "title": "carmen ad Varchium",
    "url": "http://www.poetiditalia.it/texts/ANGULO|carm|001",
    "text": [
        [
            {
                "verse": "ad Benedictum Varchium"
            },
            {
                "verse": "Te patera numerisque licet Mendotius ornet,"
            },
            {
                "verse": "Sunt Varchi meritis dona minora tui;"
            },
            {
                "verse": "Iccirco mea Musa tuos si sculpere honores"
            },
            {
                "verse": "Digne foret, tumulo splendidiora darem."
            },
            {
                "verse": "Sculpet enim varia doctrina pectus onustum"
            },
            {
                "verse": "Esse tuum et Thuscae te iubar hystoriae."
            }
        ]
    ]
}