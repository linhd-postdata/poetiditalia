{
    "author": "Filippo Beroaldo",
    "title": "endecasyllabon ad Asinum Aureum",
    "url": "http://www.poetiditalia.it/texts/BEROALDO|ende|001",
    "text": [
        [
            {
                "verse": "I dextro pede, Sidere et secundo"
            },
            {
                "verse": "Felix pannoniam petas Aselle,"
            },
            {
                "verse": "Te non aspera saxa, non uiarum"
            },
            {
                "verse": "Scruppi, flumina, cespites, lacunae"
            },
            {
                "verse": "Cliuorum salebrosa, senticeta"
            },
            {
                "verse": "Delassent: iter hoc laboriosum"
            },
            {
                "verse": "Transcurras sine claudicatione."
            },
            {
                "verse": "Commentaria nostra sarcinasque"
            },
            {
                "verse": "Perfer nauiter, offerasque Munus"
            },
            {
                "verse": "Petro Pontifici pio eloquenti,"
            },
            {
                "verse": "Quo nil doctius elegantiusque,"
            },
            {
                "verse": "Quo nil sanctius est benigniusque."
            },
            {
                "verse": "Is te comiter obuioque gressu"
            },
            {
                "verse": "Gratanter capiet, statim refectum"
            },
            {
                "verse": "Vnguento, Ambrosia, Toro fouebit."
            },
            {
                "verse": "Tunc molli satur educatione"
            },
            {
                "verse": "Fies delicium tui patroni,"
            },
            {
                "verse": "Gustabisque rosas, eris Torosus"
            },
            {
                "verse": "Splendens Floridus aureusque Asellus."
            },
            {
                "verse": "I dextro pede, Sidere et secundo"
            },
            {
                "verse": "Felix pannoniam petas Aselle."
            }
        ]
    ]
}