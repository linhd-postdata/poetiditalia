{
    "author": "carmina libraria",
    "title": "Policarpus Smyrnaeus, opera Dionysii, convertit etiam Marsilius Ficinus",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|posm|001",
    "text": [
        [
            {
                "verse": "thetrasticon in profundissima opera maximi Dionysii"
            }
        ],
        [
            {
                "verse": "Auditor Pauli Dionysius optimus ille"
            },
            {
                "verse": "Philosophus verum doctus amare Deum."
            },
            {
                "verse": "Scripsit quae multis sunt abdita quaeque moderni"
            },
            {
                "verse": "Theologi libris inseruere suis."
            },
            {
                "verse": "Seraphicus sanctusque Thomas Dionysii"
            },
            {
                "verse": "Te passim testem gaudet habere Deum."
            }
        ],
        [
            {
                "verse": "hinc liba sacro Graecorum nectare farta Advena Iohannes spondeo meo Carolo"
            }
        ],
        [
            {
                "verse": "Maxime Francigenum cui regia fortia fulgent"
            },
            {
                "verse": "Munera votiferi sint tibi grata tui."
            },
            {
                "verse": "Vos qui Romuleas nescitis temnere thermas"
            },
            {
                "verse": "Attica ne pigeat sumere gymnasia."
            },
            {
                "verse": "Quorum si quaedam per me scintilla relucet,"
            },
            {
                "verse": "Versibus Ausoniis, si libet, aspicite."
            },
            {
                "verse": "Molestum si non nostri munire laborem"
            },
            {
                "verse": "Firmetur vestri pondere iudicii."
            },
            {
                "verse": "Si quid nodosum durumve notatur in ipso"
            },
            {
                "verse": "Parcite Cecropidis Attica tela sequor."
            },
            {
                "verse": "At si mendosus declinat tramite recto"
            },
            {
                "verse": "Mellifluo vestro famine corrigite."
            },
            {
                "verse": "Quod si quorundam mordetur dente feroci"
            },
            {
                "verse": "(Hoc leve namque meo contigit Hieronymo)"
            },
            {
                "verse": "Ut vero stabulis mameat fundamine firmo"
            },
            {
                "verse": "Regali stathmo figere sufficiet."
            },
            {
                "verse": "Crediderim multos tangentum summa sophiae"
            },
            {
                "verse": "Non despecturos donula nostra fore."
            },
            {
                "verse": "Sudorisque gravis tentabunt carpere fructum"
            },
            {
                "verse": "Forsan virtutem vilia verba tenent."
            },
            {
                "verse": "Saepe solent spinis redolentes crescere flores,"
            },
            {
                "verse": "Nodosae vitis sumitur uva ferax."
            },
            {
                "verse": "Lumine sidereo Dionysius noster Athenas"
            },
            {
                "verse": "Areopagites magnificusque sophos."
            },
            {
                "verse": "Primo commotus Phoebo subeunte selena"
            },
            {
                "verse": "Tempore quo stauro fixus erat Dominus."
            },
            {
                "verse": "Mox ut conversus mira supefactus eclypsi"
            },
            {
                "verse": "Consequitur gaudens ierothea ducem."
            },
            {
                "verse": "Qui mox edoctus praeclarus namque magister"
            },
            {
                "verse": "Pneumatis excelsi fonte renatus erat."
            },
            {
                "verse": "Nec mora perfulgens caelestis duce sophiae"
            },
            {
                "verse": "Atridas edocuit de quibus ortus adest."
            },
            {
                "verse": "Namque ferunt Paulum qui Christum sparsit in orbem"
            },
            {
                "verse": "Ipsi felices imposuisse manus."
            },
            {
                "verse": "Ut mox perfectus doctoris summatus instans"
            },
            {
                "verse": "Rexit Cecropidas pugil archiereus."
            },
            {
                "verse": "Alta dehinc volitans paulum supra alta secutus"
            },
            {
                "verse": "Empyrei caeli tertia regna videt."
            },
            {
                "verse": "Supicit ad Seraphin primos sanctosque Cherubin"
            },
            {
                "verse": "Aetherosque thronos quo sedet ipse Deus."
            },
            {
                "verse": "Post hos virtutes dominatus atque potentes"
            },
            {
                "verse": "Agminibus sacris enitet ordo sequens"
            },
            {
                "verse": "Mentibus axiramis tertia taxis inest."
            },
            {
                "verse": "Hos igitur numeros tibi trinos numine septos"
            },
            {
                "verse": "Praedicti patris mystica dicta docent."
            }
        ]
    ]
}