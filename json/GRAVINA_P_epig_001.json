{
    "author": "Pietro Gravina",
    "title": "epigramma",
    "url": "http://www.poetiditalia.it/texts/GRAVINA_P|epig|001",
    "text": [
        [
            {
                "verse": "Qui legis in tenero praedulces carmine lusus"
            },
            {
                "verse": "Et laudas Latiae tam noua plectra lyrae,"
            },
            {
                "verse": "Hoc mirere magis quod uir Lacedaemone natus"
            },
            {
                "verse": "Romano potuit cultius ore loqui."
            },
            {
                "verse": "Hic est qui Ausonias mulcet Manilius aures"
            },
            {
                "verse": "Quique habuit faciles in sua uota deas."
            },
            {
                "verse": "Lesbia, iam tecum, iam tecum, Delia, fas est"
            },
            {
                "verse": "Proferat ornatum pulchra Licinna caput."
            }
        ]
    ]
}