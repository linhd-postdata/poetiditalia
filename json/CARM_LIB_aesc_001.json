{
    "author": "carmina libraria",
    "title": "Aeschylus, tragoediae conversae per Ioannem Sanravium Montempessulanensem",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|aesc|001",
    "text": [
        [
            {
                "verse": "Bernardi Bertrandi ad lectorem"
            }
        ],
        [
            {
                "verse": "Attica Cecropio regno decorata vetusto"
            },
            {
                "verse": "\u00ce\u0091\u00ce\u0099\u00ce\u00a3\u00ce\u00a7\u00ce\u0099\u00ce\u009b\u00ce\u0095\u00ce\u00a9 genuit laudibus eximium."
            },
            {
                "verse": "Melpomene tragicum vatem dedit horrida, regum"
            },
            {
                "verse": "Persarum cladis carmine maesta canit."
            },
            {
                "verse": "Hactenus hunc Latiis celavit Graecia semper"
            },
            {
                "verse": "Quem tibi Sanravius transtulit in Latium;"
            },
            {
                "verse": "Cui merito  debes magnas persolvere grates"
            },
            {
                "verse": "Muneris ut tanti praemia digna ferat."
            }
        ]
    ]
}