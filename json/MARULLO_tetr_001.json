{
    "author": "Michele Marullo",
    "title": "carmen tetrastichon",
    "url": "http://www.poetiditalia.it/texts/MARULLO|tetr|001",
    "text": [
        [
            {
                "verse": "tetrastichon in laudem Innocentii pontificis"
            }
        ],
        [
            {
                "verse": "Aurea Saturni redierunt saecula prisci:"
            },
            {
                "verse": "Omnia sedato murmure tuta iacent,"
            },
            {
                "verse": "Pontifice adventante novo, clementia cuius"
            },
            {
                "verse": "Proelia delevit, pax et ubique iacet."
            }
        ]
    ]
}