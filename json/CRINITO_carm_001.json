{
    "author": "Pietro Crinito",
    "title": "carmen ad Ugolinum Verinum",
    "url": "http://www.poetiditalia.it/texts/CRINITO|carm|001",
    "text": [
        [
            {
                "verse": "Te nascente ferunt ex ortu flumina mella"
            },
            {
                "verse": "Dulcatis cunctata vadis oleique liquoris"
            },
            {
                "verse": "Isse per attonitas bacca pendente trapetas."
            },
            {
                "verse": "Protulit undantem segetem sine semine campus"
            },
            {
                "verse": "Et sine se natis invidit pampinus uvis."
            },
            {
                "verse": "Hibernae rubuere rosae spretoque rigore"
            },
            {
                "verse": "Lilia permistis insultavere pruinis."
            },
            {
                "verse": "Tale puerperium quotiens Lucina resolvit,"
            },
            {
                "verse": "Mos elementorum caedit, Musaeque futurae"
            },
            {
                "verse": "Fit rerum novitas decus. Venisse beatos"
            },
            {
                "verse": "Sic loquitur natura Deos. Constantis Iuli"
            },
            {
                "verse": "Lambebant teneros incendia blanda capillos;"
            },
            {
                "verse": "Astyages Cyro pellendus forte nepoti"
            },
            {
                "verse": "Inguinis expavit diffuso vite racemum."
            }
        ]
    ]
}