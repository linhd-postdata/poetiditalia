{
    "author": "carmina libraria",
    "title": "Ludoicus Enocus Uxeludonensis, partitiones grammaticae",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|enoc|001",
    "text": [
        [
            {
                "verse": "Enocus ad adulescentulos Genevienses"
            }
        ],
        [
            {
                "verse": "Grammaticen edit quisque. At cur edimus et nos?"
            },
            {
                "verse": "Ut vos hic ordo cum brevitate iuvet."
            },
            {
                "verse": "Multa sciens prudens praecepta intacta reliqui"
            },
            {
                "verse": "Pondera ne parvis magna ferenda darem."
            },
            {
                "verse": "Omnia qui miscet, nil ut nescire putetur,"
            },
            {
                "verse": "Is cui ne noceat sit sibi grammaticus."
            }
        ]
    ]
}