{
    "author": "Cristofano Serarrighi",
    "title": "carmina",
    "url": "http://www.poetiditalia.it/texts/SERARR|carm|001",
    "text": [
        [],
        [
            {
                "verse": "Armati foris, arma, fossa, murus"
            },
            {
                "verse": "Cingunt hoc mihi terreum involucrum,"
            },
            {
                "verse": "Quod spe qua fere cunque spoliatum,"
            },
            {
                "verse": "Dum est carum sibi, iam induit timorem."
            },
            {
                "verse": "Audax spiritus intus invalescit,"
            },
            {
                "verse": "Indignans grave fine contineri"
            },
            {
                "verse": "Terrestri; repetens ut assolet mors"
            },
            {
                "verse": "Tetri carceris esse finis aptus."
            },
            {
                "verse": "Illud, heu nimis, et timet, doletque,"
            },
            {
                "verse": "Hoc secum reputans bonum esse maius,"
            },
            {
                "verse": "Si quos suggerit, explet appetitus."
            },
            {
                "verse": "Hic cuius Deus est vigor, fidesque,"
            },
            {
                "verse": "Liber usque polum, volat, reditque;"
            },
            {
                "verse": "Solamenque mali advehit prementis."
            }
        ],
        []
    ]
}