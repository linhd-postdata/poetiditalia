{
    "author": "Quinto Emiliano Cimbriaco",
    "title": "uersus uarii",
    "url": "http://www.poetiditalia.it/texts/CIMBRIAC|uers|001",
    "text": [
        [
            {
                "verse": "Si tibi fata dabunt, Laurenti Roscide uates,"
            },
            {
                "verse": "Vt gelidi redeas ad Bachilonis aquas,"
            },
            {
                "verse": "Hic ubi mellifluae me lactauere Camoenae"
            },
            {
                "verse": "Pantagathi puerum, cuius alumnus eram."
            },
            {
                "verse": "I, precor, ad cineres sacras, monumenta parentis,"
            },
            {
                "verse": "Et dicas lapidi: Mollius ossa premas;"
            },
            {
                "verse": "Ne laedas Manes; nam quem tu credis obisse;"
            },
            {
                "verse": "Viuit adhuc Patriae gloria magna suae."
            }
        ],
        [
            {
                "verse": "Patris delicium et nouem sororum"
            },
            {
                "verse": "Hac hospes iacet Helius sub urna"
            },
            {
                "verse": "Bassus Cimbriaci puer poetae."
            },
            {
                "verse": "Quoi si stamina longiora Parcae"
            },
            {
                "verse": "Neuissent potuit chelim paternam"
            },
            {
                "verse": "Et Musas clarius sequi sub antro."
            },
            {
                "verse": "Verum praecipiti Atropo redemptus"
            },
            {
                "verse": "Viuit coelitibus choris beatus."
            },
            {
                "verse": "Ergo uos etiam omnibus Sibyllis"
            },
            {
                "verse": "Custodes cinerum perenniores."
            }
        ],
        [
            {
                "verse": "Sunt qui me credant uita non esse pudica,"
            },
            {
                "verse": "Quod scribam uersus liberiore pede,"
            },
            {
                "verse": "Quod placeat nobis, si qua est lasciua puella,"
            },
            {
                "verse": "Et laeta semper uiuere fronte uelim."
            },
            {
                "verse": "Ingenue fatear, mecum Galatea iocatur;"
            },
            {
                "verse": "Sed peccant isti deteriora iocis."
            }
        ],
        [
            {
                "verse": "Andreas Zeuxis nostraeque aetatis Apelles"
            },
            {
                "verse": "Hoc Bellunellus nobile pinxit opus."
            }
        ],
        [
            {
                "verse": "Conditur hic Salome, Raunachi e stirpe uetusta"
            },
            {
                "verse": "Orta olim, quae nec degener a proauis."
            },
            {
                "verse": "Durrensis consors Federici, ast saepe Naonis"
            },
            {
                "verse": "In praefectura gessit honore uices."
            },
            {
                "verse": "In qua, ab magnifico, dum uita, absente marito, est"
            },
            {
                "verse": "Functa, uale extremum sumere non potuit."
            }
        ],
        [
            {
                "verse": "Heus tu, qui spectris oculos depascis inanis"
            },
            {
                "verse": "Picturae, huc Ciuis, seu peregrinus ades:"
            },
            {
                "verse": "In qua spectatur qualis Iammeia Virgo,"
            },
            {
                "verse": "Virgo Dei Genitrix fertur ad astra Poli."
            },
            {
                "verse": "Scito etiam Salomes Raunachae gentis alumnae"
            },
            {
                "verse": "Non procul hinc pariis ossa subesse locis."
            },
            {
                "verse": "Haec ait emoriens: Coniux Federice ualeto:"
            },
            {
                "verse": "Dum tibi cara fui, Tu mihi carus eras."
            },
            {
                "verse": "Nunc alio me fata uocant: Tu Caesaris urbem"
            },
            {
                "verse": "Hanc regito, et cari pignora coniugii."
            },
            {
                "verse": "Postquam decedes, Lethaeos neglige fontes."
            },
            {
                "verse": "Si nostrum amplexum quaeris, in Astra ueni."
            },
            {
                "verse": "Hoc igitur cara Durer pro coniuge fecit"
            },
            {
                "verse": "Argumentum ingens, et pietatis opus."
            }
        ],
        [
            {
                "verse": "Roche, decus patriae, Christi Confessor, et ingens"
            },
            {
                "verse": "Morborum expulsor, tabificique mali:"
            },
            {
                "verse": "Protege deuotas hominum tibi quaeso cateruas,"
            },
            {
                "verse": "Et pestem aduersus, quam potes affer opem."
            },
            {
                "verse": "Si tua per magnum pateant miracula mundum;"
            },
            {
                "verse": "Si pro Cimbriaci fama loquatur anus:"
            },
            {
                "verse": "Terrarum ignotus quondam peregrinus, Olympi"
            },
            {
                "verse": "Nunc Ciuis Rochus nobilis astra tenet."
            }
        ],
        [
            {
                "verse": "Captiua hoc trahitur Celina cornu"
            },
            {
                "verse": "Et fessa in fluuium cadit Naonem"
            },
            {
                "verse": "Fomenta Venetis datura flammis."
            }
        ],
        [
            {
                "verse": "Hoc matris Domini sed integellae"
            },
            {
                "verse": "Spectrum Virginis omnium aduocatae"
            },
            {
                "verse": "Ne ledas caue, nam facit stupenda"
            },
            {
                "verse": "Sicut fixa tholo explicat tabella."
            }
        ],
        [
            {
                "verse": "Tu, qui Cimbriaci rudis Poetae"
            },
            {
                "verse": "Versus endecasyllabos notabis,"
            },
            {
                "verse": "Rideto licet, et iocator usque;"
            },
            {
                "verse": "Hanc scibis iuuenum esse Manticarum"
            },
            {
                "verse": "Sic uernante domum hospitalitate"
            },
            {
                "verse": "Hirsutum ut Thraseam mouere possit."
            },
            {
                "verse": "Non sunt indigenae senis Naonis"
            },
            {
                "verse": "Sed Comi ueteres Noui coloni;"
            },
            {
                "verse": "Et Mantus tripodas genus locutae,"
            },
            {
                "verse": "Nomen nobilium unde Manticarum."
            },
            {
                "verse": "Si quis postulat hanc fidem uidere"
            },
            {
                "verse": "Annales repetat uetustiores."
            }
        ],
        [
            {
                "verse": "Haec sunt ora uirum, quae cernis picta, uiator,"
            },
            {
                "verse": "Ast intus ueri sunt, mihi crede, uiri;"
            },
            {
                "verse": "Illis pulcra dedit uarius tentoria pictor"
            },
            {
                "verse": "Iuppiter his mentem pulcrius imposuit."
            }
        ],
        [
            {
                "verse": "Qui primus latias effinxit in aere lituras,"
            },
            {
                "verse": "Et docuit sacros aere notare libros,"
            },
            {
                "verse": "Nonne putas dicti coelum superasse Myronis?"
            },
            {
                "verse": "Phidiacas Veneres, Parrhasiosue ioues?"
            },
            {
                "verse": "Hunc ego Daedaleos etiam uicisse labores."
            },
            {
                "verse": "Hunc ego Palladias, credo, habuisse manus."
            }
        ],
        [
            {
                "verse": "Hic decus est uenetum Donatae et gloria gentis"
            },
            {
                "verse": "Nicoleos praesul, clara Aquileia, tuus,"
            },
            {
                "verse": "Iure sacro, pietate nitens uirtuteque summa,"
            },
            {
                "verse": "Moribus, eloquio, religione, fide."
            },
            {
                "verse": "Lustra decemque duoque et quartum uixerat annum"
            },
            {
                "verse": "Quarta messe cadit - proh dolor! - imperio."
            }
        ],
        [
            {
                "verse": "Praesulis hoc tumulo uenerabilis ossa quiescunt"
            },
            {
                "verse": "Nicolai prisca simplicitate senis."
            },
            {
                "verse": "Certior ut fias: Donatae gloria gentis"
            },
            {
                "verse": "Et decus antiquum religionis erat."
            },
            {
                "verse": "Iustitiae cultor, uitae miserator egenae:"
            },
            {
                "verse": "De quorum meritis nunc tenet astra. Vale."
            }
        ],
        [
            {
                "verse": "Pontem sublicium diu uetustum,"
            },
            {
                "verse": "Nunc stratum silice, arcubus duobus"
            },
            {
                "verse": "Quem tu sic pedibus premis sedentem,"
            },
            {
                "verse": "Quanti, te precor, aestimas locatum?"
            },
            {
                "verse": "Aut quot millia curruum tulisse,"
            },
            {
                "verse": "Qua nunc materia est is fabricatus?"
            },
            {
                "verse": "Quae tu, si bene cogitaris, utrum"
            },
            {
                "verse": "Actores superet suos uideto"
            },
            {
                "verse": "Scire hoc te uolui, hospes, et ualere."
            }
        ],
        [
            {
                "verse": "Veronae decus et domus Secundae,"
            },
            {
                "verse": "Plini, gloria: cognitumque lumen"
            },
            {
                "verse": "Ingens historiae perennioris."
            },
            {
                "verse": "Quis te sic nitidum manu expoliuit?"
            },
            {
                "verse": "Docta (Iuppiter) et scientiori,"
            },
            {
                "verse": "Vel per se Aonidas sequi sorores?"
            },
            {
                "verse": "Vt qui tam fueras lacer retonsus"
            },
            {
                "verse": "Nunc sis integer: elegans subinde"
            },
            {
                "verse": "Musarum sacer agnitus sacerdos"
            },
            {
                "verse": "Sic possis merito omnibus placere"
            },
            {
                "verse": "Exutus scabie librariorum?"
            },
            {
                "verse": "Paeanti ingenio eruditiori:"
            },
            {
                "verse": "Et digno cythara mei Catulli"
            },
            {
                "verse": "Tandem sum mihi redditus: meaeque"
            },
            {
                "verse": "Assertus patriae uetustiori:"
            },
            {
                "verse": "Qua herciscens Athesis remugit undis"
            },
            {
                "verse": "Me Comi licet arbitrantur ortum"
            },
            {
                "verse": "Quod si plus fidei infidens requiris"
            },
            {
                "verse": "Veronam petito: et uideto saxum:"
            },
            {
                "verse": "Sacris quod posui ossibus parentum"
            },
            {
                "verse": "Tunc dices tripodas tibi locutos."
            },
            {
                "verse": "Sic phoenix Arabes apud Sabaeos:"
            },
            {
                "verse": "Cum longae exuerit situm senectae:"
            },
            {
                "verse": "Eoas uolitat nouus per oras:"
            },
            {
                "verse": "Sic olim coluber grauatus annis."
            },
            {
                "verse": "Squammas exuuiis nouat repostis."
            },
            {
                "verse": "Ergo me legito aure uellicata:"
            },
            {
                "verse": "Sic te Castalios iuuat liquores."
            },
            {
                "verse": "Vesaeos bibere: et nouare carmen"
            },
            {
                "verse": "Doctum flectere barbiton: uel ipsam"
            },
            {
                "verse": "Naturae historiam uidere ad unguem."
            }
        ],
        [
            {
                "verse": "Qui non uult stigiis fata sub umbris"
            },
            {
                "verse": "Saeua pestilitate deperire,"
            },
            {
                "verse": "Nec ferrugineam uidere pupim,"
            },
            {
                "verse": "Nec uitae precium seni trientem"
            },
            {
                "verse": "Olli porrigere ore subtrementi,"
            },
            {
                "verse": "Sed uiuo esse potens sui uigore,"
            },
            {
                "verse": "Et seruare animam diu ualentem,"
            },
            {
                "verse": "Haec, hortor, legat aure non supina,"
            },
            {
                "verse": "Quae Paeantius exarauit, artis"
            },
            {
                "verse": "Ille assertor Apollinaris, ille"
            },
            {
                "verse": "Ingens Castalidum decus Sororum."
            }
        ],
        [
            {
                "verse": "En haec Caesarei moles spectanda sepulchri,"
            },
            {
                "verse": "Quae teget inuicti Caesaris ossa mei."
            },
            {
                "verse": "Aequat pyramides, aequat sibi laude colossos,"
            },
            {
                "verse": "Aequat Mausoli nobile regis opus."
            },
            {
                "verse": "Non is cippus erat, quo magnus decubat Hector,"
            },
            {
                "verse": "Talis in incerto non fuit urna rogo."
            },
            {
                "verse": "Vix ego crediderim Dictaea rupe sepulchrum"
            },
            {
                "verse": "Gnosiaci quondam tale fuisse Iouis."
            },
            {
                "verse": "Sed ne Cimbriacum credas maiora locutum,"
            },
            {
                "verse": "Tu coelum inspicias, artificumque manus."
            },
            {
                "verse": "Quo magni uiuunt Friderici Caesaris ora,"
            },
            {
                "verse": "Sustinet excisum Sinnade marmor opus."
            },
            {
                "verse": "Et circumpositi spirant ex ordine patres,"
            },
            {
                "verse": "Quos coluit Caeesar cum pietate uiros."
            },
            {
                "verse": "Et nunc Caesareos mirentur ab hoste triumphos,"
            },
            {
                "verse": "Post uero in superos accomitentur iter."
            },
            {
                "verse": "Caesaris effigiem uideas ex marmore ductam,"
            },
            {
                "verse": "Quidue ferant sacri singula membra ducis."
            },
            {
                "verse": "Candida quos hilarat risu concordia uultus,"
            },
            {
                "verse": "Horrescunt Daci Pannnoniique truces."
            },
            {
                "verse": "Et quicumque uagi mirantur cornua Rheni,"
            },
            {
                "verse": "Et quos occiduis abluit aequor aquis."
            },
            {
                "verse": "Hoc caput immensi cognoscunt sidera mundi,"
            },
            {
                "verse": "Quale sub arctois non fuit ante polis."
            },
            {
                "verse": "Quam bene conueniunt humeris uictoribus arma,"
            },
            {
                "verse": "Abiecta hoc pectus aegide Pallas habet."
            },
            {
                "verse": "His et fulminei manibus data gloria Martis,"
            },
            {
                "verse": "Inuictum hoc nunquam terruit hasta latus."
            },
            {
                "verse": "Hae plantae Domini sanctum tetigere sepulchrum,"
            },
            {
                "verse": "Et bis Tarpeii templa superba Iouis."
            },
            {
                "verse": "Bella iuuent alios, et saeuae Gorgonis arma,"
            },
            {
                "verse": "Caesaris ast proprius sit pietatis amor:"
            },
            {
                "verse": "Quando Pelaeos aequauit marte triumphos,"
            },
            {
                "verse": "Et prisca insignem relligione Numam."
            },
            {
                "verse": "Elysiis alius uiuat nouus incola campis."
            },
            {
                "verse": "Ignea si noster sidera Caesar habet."
            }
        ],
        [
            {
                "verse": "Solus in amplexu tenere resolutus amice"
            },
            {
                "verse": "Securus Venetum quo ferat arma Leo"
            },
            {
                "verse": "Exige concessum spaciis fatalibus aeuum"
            },
            {
                "verse": "Et tua tecum agitet candida Flora dies."
            },
            {
                "verse": "Plura tibi loquerer, sed me tuus improbus urget"
            },
            {
                "verse": "Nuntius, et calamo nulla papyrus adest."
            }
        ],
        [
            {
                "verse": "Ecquid sic properas foras abire,"
            },
            {
                "verse": "Leuis Callimachi mei libelle?"
            },
            {
                "verse": "Anne sic totiens feras lituras,"
            },
            {
                "verse": "Duram Cimbriaci manum perosus?"
            },
            {
                "verse": "Vt uis! Te moneo canes cauere,"
            },
            {
                "verse": "Non qualem Erigone puella cernit"
            },
            {
                "verse": "Inter sidereos globos nitere,"
            },
            {
                "verse": "Spurcos, dico, libelle, paedagogos"
            },
            {
                "verse": "Et qui conueniunt senes cinaedi."
            },
            {
                "verse": "Tu coetus iuuenum bonos frequenta,"
            },
            {
                "verse": "Quales Omnibonus scholas habebat,"
            },
            {
                "verse": "Praeceptor meus, Isthmiae Diones."
            },
            {
                "verse": "Ac uerum Latiae decus Camenae"
            },
            {
                "verse": "Et Crispi aemulus et Titi Sabellus"
            },
            {
                "verse": "Nunc orsus Venetum ducem referre"
            },
            {
                "verse": "Quos et nobilitat sacer poeta"
            },
            {
                "verse": "Horum gloria temporum, Hermolaus."
            },
            {
                "verse": "Quodam sic licet audiente dicam"
            },
            {
                "verse": "De nugis plagiario superbo"
            },
            {
                "verse": "Et Cynthi male nomine arrogato:"
            },
            {
                "verse": "Haec si decipis aure non supina,"
            },
            {
                "verse": "Tum nasum et pueros salariorum"
            },
            {
                "verse": "Et ridere ciconiam licebit"
            },
            {
                "verse": "Subscriptore meo tibi Hermolao."
            }
        ],
        [
            {
                "verse": "Attila Callimachi regem uisure Quiritum,"
            },
            {
                "verse": "Carpe iter et fausto limina tange pede,"
            },
            {
                "verse": "Limina, quae audito uenerantur Marte Sicambri,"
            },
            {
                "verse": "Limina, quae assidue Belgica turba colit."
            },
            {
                "verse": "Huc ueniens statim dominum miratus et aulam,"
            },
            {
                "verse": "Hanc caelum, ast illum dixeris esse Iouem."
            },
            {
                "verse": "Talis utrique manet species, micat altera gemmis,"
            },
            {
                "verse": "Alterius decorant aegis et hasta manus."
            },
            {
                "verse": "Hic est diuini Federici filius ille,"
            },
            {
                "verse": "Ingens terrai Maximilianus honos,"
            },
            {
                "verse": "Cui modo Sigaei cessit diadema Quirini,"
            },
            {
                "verse": "Vt consors esset Caesaris atque Iouis,"
            },
            {
                "verse": "Et iam terdecies uictor decessit ab hoste,"
            },
            {
                "verse": "Exuuias secum captaque signa ferens."
            },
            {
                "verse": "Hoc duce Cumaei remeabit carminis aeuum,"
            },
            {
                "verse": "Nec minor aegiocho tum Ioue Caesar erit."
            },
            {
                "verse": "Si de te quaereret: Cuia haec sunt carmina? quare"
            },
            {
                "verse": "Huc uenias? quo sis de genitore satus?"
            },
            {
                "verse": "Sum, - dices, tantum ne longa exordia sumas, -"
            },
            {
                "verse": "Carmen Cimbriaci et Attila Callimachi;"
            },
            {
                "verse": "Ipse tuis de me cognosces laudibus olim"
            },
            {
                "verse": "Esse quidem Hunnorum facta minora ducis."
            },
            {
                "verse": "Me legito, quotiens Geticus dabit otia Mauors,"
            },
            {
                "verse": "Inter adorata pocula gratus ero."
            }
        ],
        [
            {
                "verse": "Quantum Callimacho debet rex Attila, per quem"
            },
            {
                "verse": "Est potis a tota posteritate legi,"
            },
            {
                "verse": "Tantum Malatestae non ultima fama Ruberti,"
            },
            {
                "verse": "Clare nepos, debet, o Lazarine, tibi."
            },
            {
                "verse": "Qui, ne blatta esset, desti impressoribus illum,"
            },
            {
                "verse": "Vnde etiam possit cognitus esse magis."
            },
            {
                "verse": "Sic Geticum uates illustrat Apolline Martem,"
            },
            {
                "verse": "Sic Clarii miles explicat ora dei."
            },
            {
                "verse": "Ergo pro meritis Daphnaeis praemia rami"
            },
            {
                "verse": "Debet utrisque uiris magnus uterque decus."
            }
        ],
        [
            {
                "verse": "O nec Callimacho minor uetusto,"
            },
            {
                "verse": "Nec qui Tartareas secutus umbras"
            },
            {
                "verse": "Placauit cithara deos seueros"
            },
            {
                "verse": "Ac taetro Eurydicem euocauit Orco;"
            },
            {
                "verse": "Sed nec Maeonio minor canente"
            },
            {
                "verse": "Captam uix Phrygiam decem per annos"
            },
            {
                "verse": "Et Laertiaden diu uagantem"
            },
            {
                "verse": "Ad Laestrygonas et malam Charybdim;"
            },
            {
                "verse": "Nec plectro inferior sacro Maronis,"
            },
            {
                "verse": "Inter qui merito potes uocari"
            },
            {
                "verse": "Alter Battiades sacros poetas"
            },
            {
                "verse": "Nostris doctior omnibus Catullis"
            },
            {
                "verse": "Et citra inuidiam nouus Philetas."
            },
            {
                "verse": "Quando Cimbriacus tuae fidelis"
            },
            {
                "verse": "Spectator citharae uenustioris"
            },
            {
                "verse": "Decantabitur in tuis Camenis:"
            },
            {
                "verse": "Quis totam Atropos imputat Sibyllam"
            },
            {
                "verse": "Tithoniue senis dies et aeuum,"
            },
            {
                "verse": "Vt me posteritas loquatur olim,"
            },
            {
                "verse": "Qui uates aliis erunt in annis,"
            },
            {
                "verse": "Docti Callimachi fuisse amicum."
            }
        ]
    ]
}