{
    "author": "carmina libraria",
    "title": "Iacobus Locher, oratio de studio humanarum disciplinarum et laude poetarum extemporalis",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|loc1|001",
    "text": [
        [
            {
                "verse": "epigramma Iacobi Locher philomusi ad lectores"
            },
            {
                "verse": "Quisquis ad exiguos nostri dictaminis orsus"
            },
            {
                "verse": "Divertis, rigidum tolle supercilium:"
            },
            {
                "verse": "Non mea grandiloquo sunt carmina facta cothurno,"
            },
            {
                "verse": "Sed resonant tenui consociata pede."
            },
            {
                "verse": "Hinc procul aufugiat qui Rhinocerontis adunca"
            },
            {
                "verse": "Nare: meos censet dilaceratque iocos."
            },
            {
                "verse": "Scribimus ingenuis quos pagina nostra iuvabit"
            },
            {
                "verse": "In me nil iuris lividus osor habet."
            }
        ],
        [],
        [
            {
                "verse": "ad Udalricum Zasium edecasyllabon Iacobi philomusi"
            },
            {
                "verse": "Cultor Pyeridum sacrique vatis"
            },
            {
                "verse": "Thymbrei fidicen quidem canorus"
            },
            {
                "verse": "Mecum concipe basiationes"
            },
            {
                "verse": "Quas dant Aonii lacus puellae;"
            },
            {
                "verse": "Mecum pollice tibiam sonoram"
            },
            {
                "verse": "Molli tangito, barbito vel acri"
            },
            {
                "verse": "Mecum iam Aeolios modos tonato."
            },
            {
                "verse": "Ad ludos celebres chorosque dulces"
            },
            {
                "verse": "Te vates revocant disertiores"
            },
            {
                "verse": "Ut nectar lepidum bibas et alme"
            },
            {
                "verse": "Degustes Veneris cibos amoenos."
            },
            {
                "verse": "Sic altus sapido deum liquore"
            },
            {
                "verse": "Mecum Romuleam coles sophiam"
            },
            {
                "verse": "Permissisque sacri virecta grata"
            },
            {
                "verse": "Mecum sedurus arbitre fovebis"
            },
            {
                "verse": "Cultor Pyeridum vale benigne."
            }
        ],
        [],
        [
            {
                "verse": "tetrastichon eiusdem ad Gabrielem Iorch"
            },
            {
                "verse": "Tu quoque Cyrrhei cultor facunde liquoris"
            },
            {
                "verse": "Adsis deliciis deprecor usque meis."
            },
            {
                "verse": "Te revocat pariter socium divina poesis"
            },
            {
                "verse": "Ut mecum lauro carmina digna canas."
            }
        ],
        [],
        []
    ]
}