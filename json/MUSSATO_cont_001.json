{
    "author": "Albertino Mussato",
    "title": "controversia",
    "url": "http://www.poetiditalia.it/texts/MUSSATO|cont|001",
    "text": [
        [
            {
                "verse": "Si potes et locus est, nostrae reminiscere mulae"
            },
            {
                "verse": "Aptaque ne pereant tempora, Mule, cave."
            },
            {
                "verse": "Haec per amicitiam quae nos coniunxit ab ortu,"
            },
            {
                "verse": "Qua non est avido gratior esca Lupo."
            }
        ],
        [
            {
                "verse": "Tentantem nostrae passim vestigia mulae"
            },
            {
                "verse": "Impediunt catuli dens strepitusque tui."
            },
            {
                "verse": "Quo prohibente tamen per flumina Mulus et ignes"
            },
            {
                "verse": "Cadmus ero, mihi tu semper Agenor eris."
            }
        ],
        [
            {
                "verse": "Hic catulus noster, qui te contristat, Aselle,"
            },
            {
                "verse": "Tentantem mulae fata levare meae,"
            },
            {
                "verse": "Cum sopor exciderit, Parnasi tractus ab antro:"
            },
            {
                "verse": "Irrita, clamabit, somnia misit ebur."
            }
        ]
    ]
}