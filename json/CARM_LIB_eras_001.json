{
    "author": "carmina libraria",
    "title": "Desiderius Erasmus, Conradus Lycosthenes, apophtegmata",
    "url": "http://www.poetiditalia.it/texts/CARM_LIB|eras|001",
    "text": [
        [
            {
                "verse": "Thomas Naogeorgus lectoribus"
            }
        ],
        [
            {
                "verse": "Dicta olim verum mixtim collegit Erasmus"
            },
            {
                "verse": "Unde ingens studiis prodiit utilitas."
            },
            {
                "verse": "Olim ea Conradus nidis communibus apte"
            },
            {
                "verse": "Disposuit curis auxit et illa suis."
            },
            {
                "verse": "Unde lucrum maius facies studiosa iuventus"
            },
            {
                "verse": "Quod certo invenies condita quoque loco."
            },
            {
                "verse": "Scilicet ut prodest apte digesta supellex"
            },
            {
                "verse": "Cum citius quicquid flagitat usus adest."
            },
            {
                "verse": "Sic habet in studiis cum facta argutaque dicta"
            },
            {
                "verse": "In classes retines quaeque redacta suas."
            },
            {
                "verse": "Ergo Lycosthenei dignas persolvito grates"
            },
            {
                "verse": "Cuius ita assidua rite iuvaris ope."
            }
        ]
    ]
}