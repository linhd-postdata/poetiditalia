    <text>
        <body>
            <div1 xml:id="d001" type="work" decls="#md" met="E">
                <p xml:id="d001l1" n="0|1">Pontifex</p>
                <l xml:id="d001l2" n="1">Quid mors saeva petis?</l>
                <p xml:id="d001l3" n="1|1">Mors</p>
                <l xml:id="d001l4" n="1" part="F">Te.</l>
                <p xml:id="d001l5" n="1|1">Pontifex</p>
                <l xml:id="d001l6" n="1" part="F">Me! Quo iure?</l>
                <p xml:id="d001l7" n="1|1">Mors</p>
                <l xml:id="d001l8" n="1" part="F">Quod hora</l>
                <l xml:id="d001l9" n="2">En properat...</l>
                <p xml:id="d001l10" n="2|1">Pontifex</p>
                <l xml:id="d001l11" n="2" part="F">Quid ais?</l>
                <p xml:id="d001l12" n="2|1">Mors</p>
                <l xml:id="d001l13" n="2" part="F">...Parcaque fila secat.</l>
                <p xml:id="d001l14" n="2|1">Pontifex</p>
                <l xml:id="d001l15" n="3">Hei mihi!</l>
                <p xml:id="d001l16" n="3|1">Mors</p>
                <l xml:id="d001l17" n="3" part="F">Quid luges?</l>
                <p xml:id="d001l18" n="3|1">Pontifex</p>
                <l xml:id="d001l19" n="3" part="F">Parum vixisse.</l>
                <p xml:id="d001l20" n="3|1">Mors</p>
                <l xml:id="d001l21" n="3" part="F">Videtur</l>
                <l xml:id="d001l22" n="4">Omnibus at nimium.</l>
                <p xml:id="d001l23" n="4|1">Pontifex</p>
                <l xml:id="d001l24" n="4" part="F">Cur, rogo?</l>
                <p xml:id="d001l25" n="4|1">Mors</p>
                <l xml:id="d001l26" n="4" part="F">Quod malus es.</l>
                <p xml:id="d001l27" n="4|1">Pontifex</p>
                <l xml:id="d001l28" n="5">Dic quid, quaeso, mali commisi?</l>
                <p xml:id="d001l29" n="5|1">Mors</p>
                <l xml:id="d001l30" n="5" part="F">Causa fuisti,</l>
                <l xml:id="d001l31" n="6">Quod praedae Gallis Itala terra fiat.</l>
                <l xml:id="d001l32" n="7">Non hoc parum.</l>
                <p xml:id="d001l33" n="7|1">Pontifex</p>
                <l xml:id="d001l34" n="7" part="F">Invitus feci, non sponte; necesse</l>
                <l xml:id="d001l35" n="8">Sed fecisse fuit.</l>
                <p xml:id="d001l36" n="8|1">Mors</p>
                <l xml:id="d001l37" n="8" part="F">Iam satis est: morere.</l>
                <p xml:id="d001l38" n="8|1">Pontifex</p>
                <l xml:id="d001l39" n="9">Hoc numquid solum cogit me tartara adire?</l>
                <p xml:id="d001l40" n="9|1">Mors</p>
                <l xml:id="d001l41" n="10">Non; fas esse tibi quod scelus omne putas.</l>
                <p xml:id="d001l42" n="10|1">Pontifex</p>
                <l xml:id="d001l43" n="11">Quod scelus? Heu miserum!</l>
                <p xml:id="d001l44" n="11|1">Mors</p>
                <l xml:id="d001l45" n="11" part="F">Solitus quod vendere cuncta</l>
                <l xml:id="d001l46" n="12">Per fas atque nefas.</l>
                <p xml:id="d001l47" n="12|1">Pontifex</p>
                <l xml:id="d001l48" n="12" part="F">Poenitet.</l>
                <p xml:id="d001l49" n="12|1">Mors</p>
                <l xml:id="d001l50" n="12" part="F">Hoc nihil est.</l>
                <p xml:id="d001l51" n="12|1">Pontifex</p>
                <l xml:id="d001l52" n="13">Saeva nimis, cur hoc nihil est?</l>
                <p xml:id="d001l53" n="13|1">Mors</p>
                <l xml:id="d001l54" n="13" part="F">In funere quando</l>
                <l xml:id="d001l55" n="14">Haud prodest aliquem poenituisse mali.</l>
                <p xml:id="d001l56" n="14|1">Pontifex</p>
                <l xml:id="d001l57" n="15">Iulia, me miserum, cur non defendis, amavi</l>
                <l xml:id="d001l58" n="16">Si te corde magis?</l>
                <p xml:id="d001l59" n="16|1">Mors</p>
                <l xml:id="d001l60" n="16" part="F">Digna lenone, satis.</l>
                <l xml:id="d001l61" n="17">Nunc morere; et te non defendet Iulia, nec quae</l>
                <l xml:id="d001l62" n="18">Enixa est utero terque quaterque tibi.</l>
                <p xml:id="d001l63" n="18|1">Pontifex</p>
                <l xml:id="d001l64" n="19">Da saltem ante obitum...</l>
                <p xml:id="d001l65" n="19|1">Mors</p>
                <l xml:id="d001l66" n="19" part="F">Garris.</l>
                <p xml:id="d001l67" n="19|1">Pontifex</p>
                <l xml:id="d001l68" n="19" part="F">Concede roganti</l>
                <l xml:id="d001l69" n="20">Hoc unum.</l>
                <p xml:id="d001l70" n="20|1">Mors</p>
                <l xml:id="d001l71" n="20" part="F">Insanis.</l>
                <p xml:id="d001l72" n="20|1">Pontifex</p>
                <l xml:id="d001l73" n="20" part="F">Hoc.</l>
                <p xml:id="d001l74" n="20|1">Mors</p>
                <l xml:id="d001l75" n="20" part="F">Citius morere.</l>
                <p xml:id="d001l76" n="20|1">Pontifex</p>
                <l xml:id="d001l77" n="21">Hoc.</l>
                <p xml:id="d001l78" n="21|1">Mors</p>
                <l xml:id="d001l79" n="21" part="F">Cedo.</l>
                <p xml:id="d001l80" n="21|1">Pontifex</p>
                <l xml:id="d001l81" n="21" part="F">Ut peream illius susceptus in ulnis,</l>
                <l xml:id="d001l82" n="22">Quae modo ab Hispania vecta puella mihi est.</l>
                <p xml:id="d001l83" n="22|1">Mors</p>
                <l xml:id="d001l84" n="23">Haec est illa senem quae te sine fine coegit</l>
                <l xml:id="d001l85" n="24">Insanire; furor non amor! Hem, morere!</l>
                <p xml:id="d001l86" n="24|1">Pontifex</p>
                <l xml:id="d001l87" n="25">Ergo mihi moriendum est?</l>
                <p xml:id="d001l88" n="25|1">Mors</p>
                <l xml:id="d001l89" n="25" part="F">Est.</l>
                <p xml:id="d001l90" n="25|1">Pontifex</p>
                <l xml:id="d001l91" n="25" part="F">Qua morte?</l>
                <p xml:id="d001l92" n="25|1">Mors</p>
                <l xml:id="d001l93" n="25" part="F">Peribis</l>
                <l xml:id="d001l94" n="26">Febre gravi qua nunc languida membra iacent.</l>
                <p xml:id="d001l95" n="26|1">Pontifex</p>
                <l xml:id="d001l96" n="27">Febre cadam?</l>
                <p xml:id="d001l97" n="27|1">Mors</p>
                <l xml:id="d001l98" n="27" part="F">Sic est.</l>
                <p xml:id="d001l99" n="27|1">Pontifex</p>
                <l xml:id="d001l100" n="27" part="F">Fugias!</l>
                <p xml:id="d001l101" n="27|1">Mors</p>
                <l xml:id="d001l102" n="27" part="F">Cur?</l>
                <p xml:id="d001l103" n="27|1">Pontifex</p>
                <l xml:id="d001l104" n="27" part="F">Stulta, putasne</l>
                <l xml:id="d001l105" n="28">Ut qui non perii fulmine febre cadam?</l>
                <lb/>
            </div1>
        </body>
    </text>
