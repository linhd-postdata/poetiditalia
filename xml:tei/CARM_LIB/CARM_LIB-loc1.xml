    <text>
        <body>
            <div1 xml:id="d001" type="work" decls="#md" met="various">
                <div2 xml:id="001c001" type="carmen" met="E">
                    <head>
                        <abbr>1</abbr>
                    </head>
                    <p xml:id="d001l1" n="tit.">epigramma Iacobi Locher philomusi ad lectores</p>
                    <l xml:id="d001l2" n="1">Quisquis ad exiguos nostri dictaminis orsus</l>
                    <l xml:id="d001l3" n="2">Divertis, rigidum tolle supercilium:</l>
                    <l xml:id="d001l4" n="3">Non mea grandiloquo sunt carmina facta cothurno,</l>
                    <l xml:id="d001l5" n="4">Sed resonant tenui consociata pede.</l>
                    <l xml:id="d001l6" n="5">Hinc procul aufugiat qui Rhinocerontis adunca</l>
                    <l xml:id="d001l7" n="6">Nare: meos censet dilaceratque iocos.</l>
                    <l xml:id="d001l8" n="7">Scribimus ingenuis quos pagina nostra iuvabit</l>
                    <l xml:id="d001l9" n="8">In me nil iuris lividus osor habet.</l>
                    <lb/>
                    <lb/>
                </div2>
                <div2 xml:id="001c002" type="carmen" met="F">
                    <head>
                        <abbr>2</abbr>
                    </head>
                    <p xml:id="d001l10" n="tit.">ad Udalricum Zasium edecasyllabon Iacobi philomusi</p>
                    <l xml:id="d001l11" n="1">Cultor Pyeridum sacrique vatis</l>
                    <l xml:id="d001l12" n="2">Thymbrei fidicen quidem canorus</l>
                    <l xml:id="d001l13" n="3">Mecum concipe basiationes</l>
                    <l xml:id="d001l14" n="4">Quas dant Aonii lacus puellae;</l>
                    <l xml:id="d001l15" n="5">Mecum pollice tibiam sonoram</l>
                    <l xml:id="d001l16" n="6">Molli tangito, barbito vel acri</l>
                    <l xml:id="d001l17" n="7">Mecum iam Aeolios modos tonato.</l>
                    <l xml:id="d001l18" n="8">Ad ludos celebres chorosque dulces</l>
                    <l xml:id="d001l19" n="9">Te vates revocant disertiores</l>
                    <l xml:id="d001l20" n="10">Ut nectar lepidum bibas et alme</l>
                    <l xml:id="d001l21" n="11">Degustes Veneris cibos amoenos.</l>
                    <l xml:id="d001l22" n="12">Sic altus sapido deum liquore</l>
                    <l xml:id="d001l23" n="13">Mecum Romuleam coles sophiam</l>
                    <l xml:id="d001l24" n="14">Permissisque sacri virecta grata</l>
                    <l xml:id="d001l25" n="15">Mecum sedurus arbitre fovebis</l>
                    <l xml:id="d001l26" n="16">Cultor Pyeridum vale benigne.</l>
                    <lb/>
                    <lb/>
                </div2>
                <div2 xml:id="001c003" type="carmen" met="E">
                    <head>
                        <abbr>3</abbr>
                    </head>
                    <p xml:id="d001l27" n="tit.">tetrastichon eiusdem ad Gabrielem Iorch</p>
                    <l xml:id="d001l28" n="1">Tu quoque Cyrrhei cultor facunde liquoris</l>
                    <l xml:id="d001l29" n="2">Adsis deliciis deprecor usque meis.</l>
                    <l xml:id="d001l30" n="3">Te revocat pariter socium divina poesis</l>
                    <l xml:id="d001l31" n="4">Ut mecum lauro carmina digna canas.</l>
                    <lb/>
                    <lb/>
                </div2>
            </div1>
        </body>
