    <text>
        <body>
            <div1 xml:id="d001" type="work" decls="#md" met="S1">
                <p xml:id="d001l1" n="0|1">carmen sapphicum Iacobi Wimphelingi Sletstarensis ad Robertum Gauginum Francorum regis oratorem</p>
                <lb/>
                <l xml:id="d001l2" n="1">Regis ex raptu tetrico procacis</l>
                <l xml:id="d001l3" n="2">Qui dolo sedat thalamos pudicos</l>
                <l xml:id="d001l4" n="3">Quae prius gratos dederant odores</l>
                <l xml:id="d001l5" n="4" met="A">Lilia marcent.</l>
                <l xml:id="d001l6" n="5">Filiam nuper aquilae potentis</l>
                <l xml:id="d001l7" n="6">Gallus elegit, rapiens amavit</l>
                <l xml:id="d001l8" n="7">Compotem regni cupiens futuram</l>
                <l xml:id="d001l9" n="8" met="A">Lilia marcent.</l>
                <l xml:id="d001l10" n="9">Corde nutanti posuit pudorem</l>
                <l xml:id="d001l11" n="10">Immemor iusti, fugiens honestum</l>
                <l xml:id="d001l12" n="11">Negligit famae titulos vetustae,</l>
                <l xml:id="d001l13" n="12" met="A">Lilia marcent.</l>
                <l xml:id="d001l14" n="13">Integro lustro placuit puella</l>
                <l xml:id="d001l15" n="14">Et simul vita fruitur suavi</l>
                <l xml:id="d001l16" n="15">Sperat et tedas sibi praeferendas,</l>
                <l xml:id="d001l17" n="16" met="A">Lilia marcent.</l>
                <l xml:id="d001l18" n="17">Ut decet sponsam fovet, osculatur</l>
                <l xml:id="d001l19" n="18">Assidet stringit niveis lacertis;</l>
                <l xml:id="d001l20" n="19">Dat manus, ridet recreatque moestum;</l>
                <l xml:id="d001l21" n="20" met="A">Lilia marcent.</l>
                <l xml:id="d001l22" n="21">Coniugis totum studet exhibere</l>
                <l xml:id="d001l23" n="22">Munus, at frustra silicem quis optet</l>
                <l xml:id="d001l24" n="23">Ut nivem aut ceram fluidam resolvi;</l>
                <l xml:id="d001l25" n="24" met="A">Lilia marcent.</l>
                <l xml:id="d001l26" n="25">Mox amor Galli bonus et pudicus</l>
                <l xml:id="d001l27" n="26">Carneo victa vitioque virtus</l>
                <l xml:id="d001l28" n="27">Victa languescunt, cecidere lugent,</l>
                <l xml:id="d001l29" n="28" met="A">Lilia marcent.</l>
                <l xml:id="d001l30" n="29">Sponsa quae cordi fuit ac placebat</l>
                <l xml:id="d001l31" n="30">Discplicet coniunx, soceri placet nunc.</l>
                <l xml:id="d001l32" n="31">Magne cur princeps adeo vacillas?</l>
                <l xml:id="d001l33" n="32" met="A">Lilia marcent.</l>
                <l xml:id="d001l34" n="33">Sic mihi causam subitae repulsae</l>
                <l xml:id="d001l35" n="34">Cur tuam spernis aliena prefers?</l>
                <l xml:id="d001l36" n="35">Sic volo dices satis est potesse</l>
                <l xml:id="d001l37" n="36" met="A">Lilia marcent.</l>
                <l xml:id="d001l38" n="37">Sic ubi regis decor aut honestas?</l>
                <l xml:id="d001l39" n="38">Sic ubi verum pudor et cor altum?</l>
                <l xml:id="d001l40" n="39">Sic ubi virtus animusque costans?</l>
                <l xml:id="d001l41" n="40" met="A">Lilia marcent.</l>
            </div1>
        </body>
    </text>
