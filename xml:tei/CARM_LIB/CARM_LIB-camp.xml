    <text>
        <body>
            <div1 xml:id="d001" type="work" decls="#md" met="various">
                <div2 xml:id="001c001" type="carmen" met="A">
                    <head>
                        <abbr>1</abbr>
                    </head>
                    <p xml:id="d001l1" n="tit.">carmen</p>
                    <lb/>
                    <l xml:id="d001l2" n="1">Nubila pellit</l>
                    <l xml:id="d001l3" n="2">Grandine foeta</l>
                    <l xml:id="d001l4" n="3">Aere sonoro</l>
                    <l xml:id="d001l5" n="4">Turribus altis</l>
                    <l xml:id="d001l6" n="5">Edita tandem</l>
                    <l xml:id="d001l7" n="6">Machina tanto</l>
                    <l xml:id="d001l8" n="7">Fulsa decore</l>
                    <l xml:id="d001l9" n="8">Gloria numquam</l>
                    <l xml:id="d001l10" n="9">Desinet axis</l>
                    <l xml:id="d001l11" n="10">Dum vehet orbem</l>
                    <l xml:id="d001l12" n="11">Libera tristi</l>
                    <l xml:id="d001l13" n="12">Facta timore</l>
                    <l xml:id="d001l14" n="13">Saecula vivens</l>
                    <l xml:id="d001l15" n="14">Cuncta loquetur.</l>
                    <lb/>
                    <l xml:id="d001l16" n="15" met="h">Pulsabat solida et toti notissima mundi</l>
                    <l xml:id="d001l17" n="16" met="p">Per coelum et terras fama vagata mea est.</l>
                    <l xml:id="d001l18" n="17" met="h">Turbida tempestas rupit me sola Tyferni</l>
                    <l xml:id="d001l19" n="18" met="p">Nubila dum sonitu rumpere posse puto.</l>
                    <l xml:id="d001l20" n="19" met="h">Me miserum fulvi rapiebant frustra metalli</l>
                    <l xml:id="d001l21" n="20" met="p">Et qui curaret reddere nemo fuit.</l>
                    <l xml:id="d001l22" n="21" met="h">Sed me iterum Fernus vigili sudore Michael</l>
                    <l xml:id="d001l23" n="22" met="p">Iam tandem fudit prae pietate meus.</l>
                    <p xml:id="d001l24" n="22|1">Sint gratiae Domino</p>
                    <lb/>
                    <lb/>
                </div2>
                <div2 xml:id="001c002" type="carmen" met="H">
                    <head>
                        <abbr>2</abbr>
                    </head>
                    <p xml:id="d001l25" n="tit.">Petri Sabini in nova Campani editionem decastichon</p>
                    <lb/>
                    <l xml:id="d001l26" n="1">Perlege Campani numerosa volumina lector</l>
                    <l xml:id="d001l27" n="2" met="G">Non otiosas paginas;</l>
                    <l xml:id="d001l28" n="3">Nam proprii et pressi cultor sermonis ubique est</l>
                    <l xml:id="d001l29" n="4" met="G">Priscis cedere nescius.</l>
                    <l xml:id="d001l30" n="5">Gratia magna tibi referenda est FERNE MICHAEL</l>
                    <l xml:id="d001l31" n="6" met="G">Quod ductu tuo prodeunt.</l>
                    <l xml:id="d001l32" n="7">Sicut Maeonios versus Pisistratus olim</l>
                    <l xml:id="d001l33" n="8" met="G">Haec undecumque colligis.</l>
                    <l xml:id="d001l34" n="9">Ergo ut durat adhuc tanti pia fama Tyranni</l>
                    <l xml:id="d001l35" n="10" met="G">Sic te posteritas colet.</l>
                    <lb/>
                    <lb/>
                </div2>
                <div2 xml:id="001c003" type="carmen" met="E">
                    <head>
                        <abbr>3</abbr>
                    </head>
                    <p xml:id="d001l36" n="tit.">ad Sabinum</p>
                    <lb/>
                    <l xml:id="d001l37" n="1">Hei mihi quam vana est spes haec et gloria famae</l>
                    <l xml:id="d001l38" n="2">Pro fama morimur docte Sabine fame.</l>
                    <l xml:id="d001l39" n="3">Stet sua per totum virtutis gloria mundum</l>
                    <l xml:id="d001l40" n="4">Recte vive deo cetera fumus erunt.</l>
                    <lb/>
                    <lb/>
                </div2>
                <div2 xml:id="001c004" type="carmen" met="E">
                    <head>
                        <abbr>4</abbr>
                    </head>
                    <p xml:id="d001l41" n="tit.">Michael</p>
                    <lb/>
                    <l xml:id="d001l42" n="1">Rumpite si libeat iam nunc mea stamina parcae,</l>
                    <l xml:id="d001l43" n="2">Imperium vitae non habet ulla meae.</l>
                    <l xml:id="d001l44" n="3">Vivere quod posthac properantia fata negarint</l>
                    <l xml:id="d001l45" n="4">Id mihi Campani iam monimenta dabunt.</l>
                </div2>
            </div1>
        </body>
